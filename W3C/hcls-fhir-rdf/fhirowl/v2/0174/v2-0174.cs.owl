v2-0174:S  a            owl:Class ;
        rdfs:label      "Superset-a set of batteries or procedures ordered under a single code unit but processed as separate batteries (e.g. routines = CBC, UA, electrolytes)<p>This set indicates that the code being described is used to order multiple service/test/observation b" ;
        skos:prefLabel  "Superset-a set of batteries or procedures ordered under a single code unit but processed as separate batteries (e.g. routines = CBC, UA, electrolytes)<p>This set indicates that the code being described is used to order multiple service/test/observation b" .

v2-0174:C  a            owl:Class ;
        rdfs:label      "Single observation calculated via a rule or formula from other independent observations (e.g. Alveolar-arterial ratio, cardiac output)" ;
        skos:prefLabel  "Single observation calculated via a rule or formula from other independent observations (e.g. Alveolar-arterial ratio, cardiac output)" .

v2-0174:A  a            owl:Class ;
        rdfs:label      "Atomic service/test/observation (test code or treatment code)" ;
        skos:prefLabel  "Atomic service/test/observation (test code or treatment code)" .

v2-0174:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 0174 ( Nature of Service/Test/Observation)" ;
        rdfs:label       "v2 Nature of Service/Test/Observation" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/0174/2.8.2" ;
        owl:versionInfo  "v2 Nature of Service/Test/Observation(2.8.2)" .

v2-0174:F  a            owl:Class ;
        rdfs:label      "Functional procedure that may consist of one or more interrelated measures (e.g. glucose tolerance test, creatinine clearance), usually done at different times and/or on different specimens" ;
        skos:prefLabel  "Functional procedure that may consist of one or more interrelated measures (e.g. glucose tolerance test, creatinine clearance), usually done at different times and/or on different specimens" .

v2-0174:P  a            owl:Class ;
        rdfs:label      "Profile or battery consisting of many independent atomic observations (e.g. SMA12, electrolytes), usually done at one instrument on one specimen" ;
        skos:prefLabel  "Profile or battery consisting of many independent atomic observations (e.g. SMA12, electrolytes), usually done at one instrument on one specimen" .
