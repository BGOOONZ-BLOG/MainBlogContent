@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix quantity-comparator: <http://hl7.org/fhir/quantity-comparator/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

<http://hl7.org/fhir/quantity-comparator/<=>
        a                owl:Class ;
        rdfs:label       "Less or Equal to" ;
        skos:definition  "The actual value is less than or equal to the given value." ;
        skos:prefLabel   "Less or Equal to" .

<http://hl7.org/fhir/quantity-comparator/>>
        a                owl:Class ;
        rdfs:label       "Greater than" ;
        skos:definition  "The actual value is greater than the given value." ;
        skos:prefLabel   "Greater than" .

<http://hl7.org/fhir/quantity-comparator/>=>
        a                owl:Class ;
        rdfs:label       "Greater or Equal to" ;
        skos:definition  "The actual value is greater than or equal to the given value." ;
        skos:prefLabel   "Greater or Equal to" .

<http://hl7.org/fhir/quantity-comparator/<>
        a                owl:Class ;
        rdfs:label       "Less than" ;
        skos:definition  "The actual value is less than the given value." ;
        skos:prefLabel   "Less than" .

quantity-comparator:  a  owl:Ontology ;
        rdfs:comment     "How the Quantity should be understood and represented." ;
        rdfs:label       "QuantityComparator" ;
        owl:versionIRI   "http://hl7.org/fhir/quantity-comparator/3.1.0" ;
        owl:versionInfo  "QuantityComparator(3.1.0)" .
