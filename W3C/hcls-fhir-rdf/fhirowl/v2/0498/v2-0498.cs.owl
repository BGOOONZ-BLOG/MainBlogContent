@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix v2-0498: <http://hl7.org/fhir/v2/0498/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v2-0498:R  a            owl:Class ;
        rdfs:label      "Refused - Consent has been refused" ;
        skos:prefLabel  "Refused - Consent has been refused" .

v2-0498:A  a            owl:Class ;
        rdfs:label      "Active - Consent has been granted" ;
        skos:prefLabel  "Active - Consent has been granted" .

v2-0498:P  a            owl:Class ;
        rdfs:label      "Pending - Consent has not yet been sought" ;
        skos:prefLabel  "Pending - Consent has not yet been sought" .

v2-0498:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 0498 ( Consent Status)" ;
        rdfs:label       "v2 Consent Status" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/0498/2.8.2" ;
        owl:versionInfo  "v2 Consent Status(2.8.2)" .

v2-0498:X  a            owl:Class ;
        rdfs:label      "Rescinded - Consent was initially granted, but was subsequently revoked or ended." ;
        skos:prefLabel  "Rescinded - Consent was initially granted, but was subsequently revoked or ended." .

v2-0498:B  a            owl:Class ;
        rdfs:label      "Bypassed (Consent not sought)" ;
        skos:prefLabel  "Bypassed (Consent not sought)" .

v2-0498:L  a            owl:Class ;
        rdfs:label      "Limited - Consent has been granted with limitations" ;
        skos:prefLabel  "Limited - Consent has been granted with limitations" .
