@prefix v3-RelationshipConjunction: <http://hl7.org/fhir/v3/RelationshipConjunction/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-RelationshipConjunction:XOR
        a                owl:Class ;
        rdfs:label       "exclusive or" ;
        skos:definition  "One and only one of the XOR conditions must be true." ;
        skos:prefLabel   "exclusive or" .

v3-RelationshipConjunction:AND
        a                owl:Class ;
        rdfs:label       "and" ;
        skos:definition  "This condition must be true." ;
        skos:prefLabel   "and" .

v3-RelationshipConjunction:OR
        a                owl:Class ;
        rdfs:label       "or" ;
        skos:definition  "At least one of the condition among all OR conditions must be true." ;
        skos:prefLabel   "or" .

v3-RelationshipConjunction:
        a                owl:Ontology ;
        rdfs:comment     "**** MISSING DEFINITIONS ****" ;
        rdfs:label       "v3 Code System RelationshipConjunction" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/RelationshipConjunction/2016-11-11" ;
        owl:versionInfo  "v3 Code System RelationshipConjunction(2016-11-11)" .
