@prefix fhir:  <http://hl7.org/fhir/> .
@prefix contact-point-use: <http://hl7.org/fhir/contact-point-use/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

contact-point-use:mobile
        a                owl:Class ;
        rdfs:label       "Mobile" ;
        skos:definition  "A telecommunication device that moves and stays with its owner. May have characteristics of all other use codes, suitable for urgent matters, not the first choice for routine business." ;
        skos:prefLabel   "Mobile" .

contact-point-use:old
        a                owl:Class ;
        rdfs:label       "Old" ;
        skos:definition  "This contact point is no longer in use (or was never correct, but retained for records)." ;
        skos:prefLabel   "Old" .

contact-point-use:home
        a                owl:Class ;
        rdfs:label       "Home" ;
        skos:definition  "A communication contact point at a home; attempted contacts for business purposes might intrude privacy and chances are one will contact family or other household members instead of the person one wishes to call. Typically used with urgent cases, or if no other contacts are available." ;
        skos:prefLabel   "Home" .

contact-point-use:temp
        a                owl:Class ;
        rdfs:label       "Temp" ;
        skos:definition  "A temporary contact point. The period can provide more detailed information." ;
        skos:prefLabel   "Temp" .

contact-point-use:work
        a                owl:Class ;
        rdfs:label       "Work" ;
        skos:definition  "An office contact point. First choice for business related contacts during business hours." ;
        skos:prefLabel   "Work" .

contact-point-use:  a    owl:Ontology ;
        rdfs:comment     "Use of contact point" ;
        rdfs:label       "ContactPointUse" ;
        owl:versionIRI   "http://hl7.org/fhir/contact-point-use/3.1.0" ;
        owl:versionInfo  "ContactPointUse(3.1.0)" .
