@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .
@prefix flag-priority: <http://hl7.org/fhir/flag-priority-code/> .

flag-priority:PN  a      owl:Class ;
        rdfs:label       "No alarm" ;
        skos:definition  "No alarm." ;
        skos:prefLabel   "No alarm" .

flag-priority:PL  a      owl:Class ;
        rdfs:label       "Low priority" ;
        skos:definition  "Low priority." ;
        skos:prefLabel   "Low priority" .

flag-priority:  a        owl:Ontology ;
        rdfs:comment     "This value set is provided as an exemplar. The value set is driven by IHE Table B.8-4: Abnormal Flags, Alert Priority." ;
        rdfs:label       "Flag Priority Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/flag-priority-code/3.1.0" ;
        owl:versionInfo  "Flag Priority Codes(3.1.0)" .

flag-priority:PH  a      owl:Class ;
        rdfs:label       "High priority" ;
        skos:definition  "High priority." ;
        skos:prefLabel   "High priority" .

flag-priority:PM  a      owl:Class ;
        rdfs:label       "Medium priority" ;
        skos:definition  "Medium priority." ;
        skos:prefLabel   "Medium priority" .
