PREFIX schema: <http://schema.org/> 

SELECT  * WHERE {
  ?x a <http://schema.org/MusicEvent> .
  ?x <http://schema.org/location> ?loc .
  ?x <http://schema.org/name> ?name .
  ?x <http://schema.org/offers> ?off .
  ?x <http://schema.org/startDate> ?sd .

  ?loc a <http://schema.org/Place> .
  ?loc <http://schema.org/address> ?str_adr .
  ?loc <http://schema.org/name> ?loc_name .

  ?off a  <http://schema.org/Offer> .
  ?off  <http://schema.org/url> ?u .

}
