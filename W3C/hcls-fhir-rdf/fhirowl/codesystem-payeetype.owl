@prefix payeetype: <http://hl7.org/fhir/payeetype/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

payeetype:  a            owl:Ontology ;
        rdfs:comment     "This value set includes sample Payee Type codes." ;
        rdfs:label       "Claim Payee Type Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/payeetype/3.1.0" ;
        owl:versionInfo  "Claim Payee Type Codes(3.1.0)" .

payeetype:provider  a    owl:Class ;
        rdfs:label       "Provider" ;
        skos:definition  "Any benefit payable will be paid to the provider (Assignment of Benefit)." ;
        skos:prefLabel   "Provider" .

payeetype:other  a       owl:Class ;
        rdfs:label       "Provider" ;
        skos:definition  "Any benefit payable will be paid to a third party such as a guarrantor." ;
        skos:prefLabel   "Provider" .

payeetype:subscriber  a  owl:Class ;
        rdfs:label       "Subscriber" ;
        skos:definition  "The subscriber (policy holder) will be reimbursed." ;
        skos:prefLabel   "Subscriber" .
