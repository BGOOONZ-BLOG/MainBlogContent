@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix trigger-type: <http://hl7.org/fhir/trigger-type/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

trigger-type:data-access-ended
        a                owl:Class ;
        rdfs:label       "Data Access Ended" ;
        skos:definition  "The trigger occurs whenever access to data of a particular type is completed" ;
        skos:prefLabel   "Data Access Ended" .

trigger-type:periodic
        a                owl:Class ;
        rdfs:label       "Periodic" ;
        skos:definition  "The trigger occurs at a specific time or periodically as described by a timing or schedule" ;
        skos:prefLabel   "Periodic" .

trigger-type:data-accessed
        a                owl:Class ;
        rdfs:label       "Data Accessed" ;
        skos:definition  "The trigger occurs whenever data of a particular type is accessed" ;
        skos:prefLabel   "Data Accessed" .

trigger-type:data-removed
        a                owl:Class ;
        rdfs:label       "Data Removed" ;
        skos:definition  "The trigger occurs whenever data of a particular type is removed" ;
        skos:prefLabel   "Data Removed" .

trigger-type:  a         owl:Ontology ;
        rdfs:comment     "The type of trigger" ;
        rdfs:label       "TriggerType" ;
        owl:versionIRI   "http://hl7.org/fhir/trigger-type/3.1.0" ;
        owl:versionInfo  "TriggerType(3.1.0)" .

trigger-type:data-modified
        a                owl:Class ;
        rdfs:label       "Data Modified" ;
        skos:definition  "The trigger occurs whenever data of a particular type is modified" ;
        skos:prefLabel   "Data Modified" .

trigger-type:named-event
        a                owl:Class ;
        rdfs:label       "Named Event" ;
        skos:definition  "The trigger occurs in response to a specific named event" ;
        skos:prefLabel   "Named Event" .

trigger-type:data-added
        a                owl:Class ;
        rdfs:label       "Data Added" ;
        skos:definition  "The trigger occurs whenever data of a particular type is added" ;
        skos:prefLabel   "Data Added" .
