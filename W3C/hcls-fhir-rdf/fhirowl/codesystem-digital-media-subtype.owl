@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix digital-media-subtype: <http://hl7.org/fhir/media-subtype/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

digital-media-subtype:fingerprint
        a                owl:Class ;
        rdfs:label       "Fingerprint" ;
        skos:definition  "A finger print scan used for identification purposes" ;
        skos:prefLabel   "Fingerprint" .

digital-media-subtype:iris
        a                owl:Class ;
        rdfs:label       "Iris" ;
        skos:definition  "An iris scan used for identification purposes" ;
        skos:prefLabel   "Iris" .

digital-media-subtype:scan
        a                owl:Class ;
        rdfs:label       "Scanned Document" ;
        skos:definition  "A digital scan of a document. This is reserved for when there is not enough metadata to create a document reference" ;
        skos:prefLabel   "Scanned Document" .

digital-media-subtype:fax
        a                owl:Class ;
        rdfs:label       "Fax" ;
        skos:definition  "A digital record of a fax document" ;
        skos:prefLabel   "Fax" .

digital-media-subtype:diagram
        a                owl:Class ;
        rdfs:label       "Diagram" ;
        skos:definition  "A diagram. Often used in diagnostic reports" ;
        skos:prefLabel   "Diagram" .

digital-media-subtype:
        a                owl:Ontology ;
        rdfs:comment     "Detailed information about the type of the image - its kind, purpose, or the kind of equipment used to generate it." ;
        rdfs:label       "Media SubType" ;
        owl:versionIRI   "http://hl7.org/fhir/media-subtype/3.1.0" ;
        owl:versionInfo  "Media SubType(3.1.0)" .

digital-media-subtype:face
        a                owl:Class ;
        rdfs:label       "Face" ;
        skos:definition  "A face scan used for identification purposes" ;
        skos:prefLabel   "Face" .

digital-media-subtype:retina
        a                owl:Class ;
        rdfs:label       "Retina scan" ;
        skos:definition  "A retinal image used for identification purposes" ;
        skos:prefLabel   "Retina scan" .

digital-media-subtype:palm
        a                owl:Class ;
        rdfs:label       "Palm" ;
        skos:definition  "A palm scan used for identification purposes" ;
        skos:prefLabel   "Palm" .
