@prefix benefit-network: <http://hl7.org/fhir/benefit-network/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

benefit-network:out  a   owl:Class ;
        rdfs:label       "Out of Network" ;
        skos:definition  "Services rendered by a provider who is not in the Network" ;
        skos:prefLabel   "Out of Network" .

benefit-network:in  a    owl:Class ;
        rdfs:label       "In Network" ;
        skos:definition  "Services rendered by a Network provider" ;
        skos:prefLabel   "In Network" .

benefit-network:  a      owl:Ontology ;
        rdfs:comment     "This value set includes a smattering of Network type codes." ;
        rdfs:label       "Network Type Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/benefit-network/3.1.0" ;
        owl:versionInfo  "Network Type Codes(3.1.0)" .
