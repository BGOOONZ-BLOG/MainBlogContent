@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix v2-0278: <http://hl7.org/fhir/v2/0278/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v2-0278:Booked  a       owl:Class ;
        rdfs:label      "The indicated appointment is booked" ;
        skos:prefLabel  "The indicated appointment is booked" .

v2-0278:Blocked  a      owl:Class ;
        rdfs:label      "The indicated time slot(s) is(are) blocked" ;
        skos:prefLabel  "The indicated time slot(s) is(are) blocked" .

v2-0278:Complete  a     owl:Class ;
        rdfs:label      "The indicated appointment has completed normally (was not discontinued, canceled, or deleted)" ;
        skos:prefLabel  "The indicated appointment has completed normally (was not discontinued, canceled, or deleted)" .

v2-0278:Cancelled  a    owl:Class ;
        rdfs:label      "The indicated appointment was stopped from occurring (canceled prior to starting)" ;
        skos:prefLabel  "The indicated appointment was stopped from occurring (canceled prior to starting)" .

v2-0278:Waitlist  a     owl:Class ;
        rdfs:label      "Appointment has been placed on a waiting list for a particular slot, or set of slots" ;
        skos:prefLabel  "Appointment has been placed on a waiting list for a particular slot, or set of slots" .

v2-0278:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 0278 ( Filler status codes)" ;
        rdfs:label       "v2 Filler status codes" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/0278/2.8.2" ;
        owl:versionInfo  "v2 Filler status codes(2.8.2)" .

v2-0278:Dc  a           owl:Class ;
        rdfs:label      "The indicated appointment was discontinued (DC’ed while in progress, discontinued parent appointment, or discontinued child appointment)" ;
        skos:prefLabel  "The indicated appointment was discontinued (DC’ed while in progress, discontinued parent appointment, or discontinued child appointment)" .

v2-0278:Noshow  a       owl:Class ;
        rdfs:label      "The patient did not show up for the appointment" ;
        skos:prefLabel  "The patient did not show up for the appointment" .

v2-0278:Deleted  a      owl:Class ;
        rdfs:label      "The indicated appointment was deleted from the filler application" ;
        skos:prefLabel  "The indicated appointment was deleted from the filler application" .

v2-0278:Pending  a      owl:Class ;
        rdfs:label      "Appointment has not yet been confirmed" ;
        skos:prefLabel  "Appointment has not yet been confirmed" .

v2-0278:Started  a      owl:Class ;
        rdfs:label      "The indicated appointment has begun and is currently in progress" ;
        skos:prefLabel  "The indicated appointment has begun and is currently in progress" .

v2-0278:Overbook  a     owl:Class ;
        rdfs:label      "The appointment has been confirmed; however it is confirmed in an overbooked state" ;
        skos:prefLabel  "The appointment has been confirmed; however it is confirmed in an overbooked state" .
