@prefix medication-statement-category: <http://hl7.org/fhir/medication-statement-category/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

medication-statement-category:community
        a                owl:Class ;
        rdfs:label       "Community" ;
        skos:definition  "Includes orders for medications to be administered or consumed by the patient in their home (this would include long term care or nursing homes, hospices, etc)" ;
        skos:prefLabel   "Community" .

medication-statement-category:patientspecified
        a                owl:Class ;
        rdfs:label       "Patient Specified" ;
        skos:definition  "Includes statements about medication use, including over the counter medication, provided by the patient, agent or another provider" ;
        skos:prefLabel   "Patient Specified" .

medication-statement-category:
        a                owl:Ontology ;
        rdfs:comment     "A coded concept identifying where the medication included in the medicationstatement is expected to be consumed or administered" ;
        rdfs:label       "MedicationStatementCategory" ;
        owl:versionIRI   "http://hl7.org/fhir/medication-statement-category/3.1.0" ;
        owl:versionInfo  "MedicationStatementCategory(3.1.0)" .

medication-statement-category:outpatient
        a                owl:Class ;
        rdfs:label       "Outpatient" ;
        skos:definition  "Includes orders for medications to be administered or consumed in an outpatient setting (for example, Emergency Department, Outpatient Clinic, Outpatient Surgery, Doctor's office)" ;
        skos:prefLabel   "Outpatient" .

medication-statement-category:inpatient
        a                owl:Class ;
        rdfs:label       "Inpatient" ;
        skos:definition  "Includes orders for medications to be administered or consumed in an inpatient or acute care setting" ;
        skos:prefLabel   "Inpatient" .
