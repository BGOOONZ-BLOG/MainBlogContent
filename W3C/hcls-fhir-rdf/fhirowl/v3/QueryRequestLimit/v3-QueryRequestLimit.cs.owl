@prefix v3-QueryRequestLimit: <http://hl7.org/fhir/v3/QueryRequestLimit/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-QueryRequestLimit:
        a                owl:Ontology ;
        rdfs:comment     "  Definition:\r\nDefines the units associated with the magnitude of the maximum size limit of a query response that can be accepted by the requesting application." ;
        rdfs:label       "v3 Code System QueryRequestLimit" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/QueryRequestLimit/2016-11-11" ;
        owl:versionInfo  "v3 Code System QueryRequestLimit(2016-11-11)" .

v3-QueryRequestLimit:_QueryRequestLimit
        a                owl:Class ;
        rdfs:label       "QueryRequestLimit" ;
        skos:definition  "Definition: The number of matching instances (number of focal classes). The document header class is the focal class of a document, a record would therefore be equal to a document." ;
        skos:prefLabel   "QueryRequestLimit" .

v3-QueryRequestLimit:RD
        a                owl:Class ;
        rdfs:label       "record" ;
        rdfs:subClassOf  v3-QueryRequestLimit:_QueryRequestLimit ;
        skos:definition  "Definition: The number of matching instances (number of focal classes). The document header class is the focal class of a document, a record would therefore be equal to a document." ;
        skos:prefLabel   "record" .
