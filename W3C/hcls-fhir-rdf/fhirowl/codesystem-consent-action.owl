@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix consent-action: <http://hl7.org/fhir/consentaction/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

consent-action:  a       owl:Ontology ;
        rdfs:comment     "This value set includes sample Consent Action codes." ;
        rdfs:label       "Consent Action Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/consentaction/3.1.0" ;
        owl:versionInfo  "Consent Action Codes(3.1.0)" .

consent-action:collect
        a                owl:Class ;
        rdfs:label       "Collect" ;
        skos:definition  "Gather/acquire information by an entity to achieve a stated purpose" ;
        skos:prefLabel   "Collect" .

consent-action:correct
        a                owl:Class ;
        rdfs:label       "Access and Correct" ;
        skos:definition  "Allowing an entity to correct a patient's information" ;
        skos:prefLabel   "Access and Correct" .

consent-action:use  a    owl:Class ;
        rdfs:label       "Use" ;
        skos:definition  "Use of stored information by an entity fo the stated purpose" ;
        skos:prefLabel   "Use" .

consent-action:disclose
        a                owl:Class ;
        rdfs:label       "Disclose" ;
        skos:definition  "Release / transfer of information to an entity " ;
        skos:prefLabel   "Disclose" .

consent-action:access
        a                owl:Class ;
        rdfs:label       "Access" ;
        skos:definition  "Provide an entity access to information to achieve a stated purpose" ;
        skos:prefLabel   "Access" .
