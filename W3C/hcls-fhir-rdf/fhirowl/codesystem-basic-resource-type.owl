@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix basic-resource-type: <http://hl7.org/fhir/basic-resource-type/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

basic-resource-type:consent
        a                owl:Class ;
        rdfs:label       "Consent" ;
        skos:definition  "An assertion of permission for an activity or set of activities to occur, possibly subject to particular limitations; e.g. surgical consent, information disclosure consent, etc." ;
        skos:prefLabel   "Consent" .

basic-resource-type:account
        a                owl:Class ;
        rdfs:label       "Account" ;
        skos:definition  "A financial instrument used to track costs, charges or other amounts." ;
        skos:prefLabel   "Account" .

basic-resource-type:study
        a                owl:Class ;
        rdfs:label       "Study" ;
        skos:definition  "An investigation to determine information about a particular therapy or product" ;
        skos:prefLabel   "Study" .

basic-resource-type:  a  owl:Ontology ;
        rdfs:comment     "This value set defines codes for resources not yet supported by (or which will never be supported by) FHIR.  Many of the codes listed here will eventually be turned into official resources.  However, there is no guarantee that any particular resource will be created nor that the scope will be exactly as defined by the codes presented here.  Codes in this set will be deprecated if/when formal resources are defined that encompass these concepts." ;
        rdfs:label       "Basic Resource Types" ;
        owl:versionIRI   "http://hl7.org/fhir/basic-resource-type/3.1.0" ;
        owl:versionInfo  "Basic Resource Types(3.1.0)" .

basic-resource-type:advevent
        a                owl:Class ;
        rdfs:label       "Adverse Event" ;
        skos:definition  "An undesired reaction caused by exposure to some agent (e.g. a medication, immunization, food, or environmental agent)." ;
        skos:prefLabel   "Adverse Event" .

basic-resource-type:aptmtreq
        a                owl:Class ;
        rdfs:label       "Appointment Request" ;
        skos:definition  "A request that a time be scheduled for a type of service for a specified patient, potentially subject to other constraints" ;
        skos:prefLabel   "Appointment Request" .

basic-resource-type:adjudicat
        a                owl:Class ;
        rdfs:label       "Invoice Adjudication" ;
        skos:definition  "The determination of what will be paid against a particular invoice based on coverage, plan rules, etc." ;
        skos:prefLabel   "Invoice Adjudication" .

basic-resource-type:invoice
        a                owl:Class ;
        rdfs:label       "Invoice" ;
        skos:definition  "A request for payment for goods and/or services.  Includes the idea of a healthcare insurance claim." ;
        skos:prefLabel   "Invoice" .

basic-resource-type:transfer
        a                owl:Class ;
        rdfs:label       "Transfer" ;
        skos:definition  "The transition of a patient or set of material from one location to another" ;
        skos:prefLabel   "Transfer" .

basic-resource-type:protocol
        a                owl:Class ;
        rdfs:label       "Protocol" ;
        skos:definition  "A set of (possibly conditional) steps to be taken to achieve some aim.  Includes study protocols, treatment protocols, emergency protocols, etc." ;
        skos:prefLabel   "Protocol" .

basic-resource-type:referral
        a                owl:Class ;
        rdfs:label       "Referral" ;
        skos:definition  "A request that care of a particular type be provided to a patient.  Could involve the transfer of care, a consult, etc." ;
        skos:prefLabel   "Referral" .

basic-resource-type:investigation
        a                owl:Class ;
        rdfs:label       "Investigation" ;
        skos:definition  "A formalized inquiry into the circumstances surrounding a particular unplanned event or potential event for the purposes of identifying possible causes and contributing factors for the event" ;
        skos:prefLabel   "Investigation" .

basic-resource-type:predetermine
        a                owl:Class ;
        rdfs:label       "Predetermination" ;
        skos:definition  "An adjudication of what would be paid under an insurance plan for a hypothetical claim for goods or services" ;
        skos:prefLabel   "Predetermination" .

basic-resource-type:predetreq
        a                owl:Class ;
        rdfs:label       "Pre-determination Request" ;
        skos:definition  "A request for a pre-determination of the cost that would be paid under an insurance plan for a hypothetical claim for goods or services" ;
        skos:prefLabel   "Pre-determination Request" .

basic-resource-type:adminact
        a                owl:Class ;
        rdfs:label       "Administrative Activity" ;
        skos:definition  "An occurrence of a non-care-related event in the healthcare domain, such as approvals, reviews, etc." ;
        skos:prefLabel   "Administrative Activity" .

basic-resource-type:exposure
        a                owl:Class ;
        rdfs:label       "Exposure" ;
        skos:definition  "Record of a situation where a subject was exposed to a substance.  Usually of interest to public health." ;
        skos:prefLabel   "Exposure" .

basic-resource-type:diet
        a                owl:Class ;
        rdfs:label       "Diet" ;
        skos:definition  "The specification of a set of food and/or other nutritional material to be delivered to a patient." ;
        skos:prefLabel   "Diet" .
