@prefix encounter-participant-type: <http://hl7.org/fhir/participant-type/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

encounter-participant-type:translator
        a                owl:Class ;
        rdfs:label       "Translator" ;
        skos:definition  "A translator who is facilitating communication with the patient during the encounter." ;
        skos:prefLabel   "Translator" .

encounter-participant-type:
        a                owl:Ontology ;
        rdfs:comment     "This value set defines a set of codes that can be used to indicate how an individual participates in an encounter." ;
        rdfs:label       "ParticipantType" ;
        owl:versionIRI   "http://hl7.org/fhir/participant-type/3.1.0" ;
        owl:versionInfo  "ParticipantType(3.1.0)" .

encounter-participant-type:emergency
        a                owl:Class ;
        rdfs:label       "Emergency" ;
        skos:definition  "A person to be contacted in case of an emergency during the encounter." ;
        skos:prefLabel   "Emergency" .
