@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix definition-topic: <http://hl7.org/fhir/definition-topic/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

definition-topic:treatment
        a                owl:Class ;
        rdfs:label       "Treatment" ;
        skos:definition  "The definition is related to treatment of the patient" ;
        skos:prefLabel   "Treatment" .

definition-topic:education
        a                owl:Class ;
        rdfs:label       "Education" ;
        skos:definition  "The definition is related to education of the patient" ;
        skos:prefLabel   "Education" .

definition-topic:  a     owl:Ontology ;
        rdfs:comment     "High-level categorization of the definition, used for searching, sorting, and filtering" ;
        rdfs:label       "DefinitionTopic" ;
        owl:versionIRI   "http://hl7.org/fhir/definition-topic/3.1.0" ;
        owl:versionInfo  "DefinitionTopic(3.1.0)" .

definition-topic:assessment
        a                owl:Class ;
        rdfs:label       "Assessment" ;
        skos:definition  "The definition is related to assessment of the patient" ;
        skos:prefLabel   "Assessment" .
