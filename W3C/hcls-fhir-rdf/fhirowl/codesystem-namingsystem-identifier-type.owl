@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix namingsystem-identifier-type: <http://hl7.org/fhir/namingsystem-identifier-type/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

namingsystem-identifier-type:other
        a                owl:Class ;
        rdfs:label       "Other" ;
        skos:definition  "Some other type of unique identifier; e.g. HL7-assigned reserved string such as LN for LOINC." ;
        skos:prefLabel   "Other" .

namingsystem-identifier-type:uri
        a                owl:Class ;
        rdfs:label       "URI" ;
        skos:definition  "A uniform resource identifier (ideally a URL - uniform resource locator); e.g. http://unitsofmeasure.org." ;
        skos:prefLabel   "URI" .

namingsystem-identifier-type:uuid
        a                owl:Class ;
        rdfs:label       "UUID" ;
        skos:definition  "A universally unique identifier of the form a5afddf4-e880-459b-876e-e4591b0acc11." ;
        skos:prefLabel   "UUID" .

namingsystem-identifier-type:
        a                owl:Ontology ;
        rdfs:comment     "Identifies the style of unique identifier used to identify a namespace." ;
        rdfs:label       "NamingSystemIdentifierType" ;
        owl:versionIRI   "http://hl7.org/fhir/namingsystem-identifier-type/3.1.0" ;
        owl:versionInfo  "NamingSystemIdentifierType(3.1.0)" .

namingsystem-identifier-type:oid
        a                owl:Class ;
        rdfs:label       "OID" ;
        skos:definition  "An ISO object identifier; e.g. 1.2.3.4.5." ;
        skos:prefLabel   "OID" .
