@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix group-type: <http://hl7.org/fhir/group-type/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

group-type:animal  a     owl:Class ;
        rdfs:label       "Animal" ;
        skos:definition  "Group contains \"animal\" Patient resources" ;
        skos:prefLabel   "Animal" .

group-type:person  a     owl:Class ;
        rdfs:label       "Person" ;
        skos:definition  "Group contains \"person\" Patient resources" ;
        skos:prefLabel   "Person" .

group-type:practitioner
        a                owl:Class ;
        rdfs:label       "Practitioner" ;
        skos:definition  "Group contains healthcare practitioner resources" ;
        skos:prefLabel   "Practitioner" .

group-type:substance  a  owl:Class ;
        rdfs:label       "Substance" ;
        skos:definition  "Group contains Substance resources" ;
        skos:prefLabel   "Substance" .

group-type:  a           owl:Ontology ;
        rdfs:comment     "Types of resources that are part of group" ;
        rdfs:label       "GroupType" ;
        owl:versionIRI   "http://hl7.org/fhir/group-type/3.1.0" ;
        owl:versionInfo  "GroupType(3.1.0)" .

group-type:medication
        a                owl:Class ;
        rdfs:label       "Medication" ;
        skos:definition  "Group contains Medication resources" ;
        skos:prefLabel   "Medication" .

group-type:device  a     owl:Class ;
        rdfs:label       "Device" ;
        skos:definition  "Group contains Device resources" ;
        skos:prefLabel   "Device" .
