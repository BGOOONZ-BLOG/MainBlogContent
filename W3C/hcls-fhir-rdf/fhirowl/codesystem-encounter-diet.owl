@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .
@prefix encounter-diet: <http://hl7.org/fhir/diet/> .

encounter-diet:gluten-free
        a                owl:Class ;
        rdfs:label       "Gluten Free" ;
        skos:definition  "Excludes ingredients containing gluten." ;
        skos:prefLabel   "Gluten Free" .

encounter-diet:  a       owl:Ontology ;
        rdfs:comment     "This value set defines a set of codes that can be used to indicate dietary preferences or restrictions a patient may have." ;
        rdfs:label       "Diet" ;
        owl:versionIRI   "http://hl7.org/fhir/diet/3.1.0" ;
        owl:versionInfo  "Diet(3.1.0)" .

encounter-diet:dairy-free
        a                owl:Class ;
        rdfs:label       "Dairy Free" ;
        skos:definition  "Excludes dairy products." ;
        skos:prefLabel   "Dairy Free" .

encounter-diet:kosher
        a                owl:Class ;
        rdfs:label       "Kosher" ;
        skos:definition  "Foods that conform to Jewish dietary law." ;
        skos:prefLabel   "Kosher" .

encounter-diet:halal  a  owl:Class ;
        rdfs:label       "Halal" ;
        skos:definition  "Foods that conform to Islamic law." ;
        skos:prefLabel   "Halal" .

encounter-diet:vegan  a  owl:Class ;
        rdfs:label       "Vegan" ;
        skos:definition  "Food without meat, poultry, seafood, eggs, dairy products and other animal-derived substances." ;
        skos:prefLabel   "Vegan" .

encounter-diet:vegetarian
        a                owl:Class ;
        rdfs:label       "Vegetarian" ;
        skos:definition  "Food without meat, poultry or seafood." ;
        skos:prefLabel   "Vegetarian" .

encounter-diet:nut-free
        a                owl:Class ;
        rdfs:label       "Nut Free" ;
        skos:definition  "Excludes ingredients containing nuts." ;
        skos:prefLabel   "Nut Free" .
