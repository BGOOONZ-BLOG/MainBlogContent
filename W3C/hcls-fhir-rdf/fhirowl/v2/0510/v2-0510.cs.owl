@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix v2-0510: <http://hl7.org/fhir/v2/0510/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v2-0510:WA  a           owl:Class ;
        rdfs:label      "Wasted (product no longer viable)" ;
        skos:prefLabel  "Wasted (product no longer viable)" .

v2-0510:RA  a           owl:Class ;
        rdfs:label      "Returned unused/no longer needed" ;
        skos:prefLabel  "Returned unused/no longer needed" .

v2-0510:RD  a           owl:Class ;
        rdfs:label      "Reserved and ready to dispense" ;
        skos:prefLabel  "Reserved and ready to dispense" .

v2-0510:RI  a           owl:Class ;
        rdfs:label      "Received into inventory (for specified patient)" ;
        skos:prefLabel  "Received into inventory (for specified patient)" .

v2-0510:RS  a           owl:Class ;
        rdfs:label      "Reserved (ordered and product allocated for the patient)" ;
        skos:prefLabel  "Reserved (ordered and product allocated for the patient)" .

v2-0510:DS  a           owl:Class ;
        rdfs:label      "Dispensed to patient location" ;
        skos:prefLabel  "Dispensed to patient location" .

v2-0510:CR  a           owl:Class ;
        rdfs:label      "Released into inventory for general availability" ;
        skos:prefLabel  "Released into inventory for general availability" .

v2-0510:RL  a           owl:Class ;
        rdfs:label      "Returned unused/keep linked to patient for possible use later" ;
        skos:prefLabel  "Returned unused/keep linked to patient for possible use later" .

v2-0510:RQ  a           owl:Class ;
        rdfs:label      "Request to dispense blood product" ;
        skos:prefLabel  "Request to dispense blood product" .

v2-0510:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 0510 ( Blood Product Dispense Status)" ;
        rdfs:label       "v2 Blood Product Dispense Status" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/0510/2.8.2" ;
        owl:versionInfo  "v2 Blood Product Dispense Status(2.8.2)" .

v2-0510:PT  a           owl:Class ;
        rdfs:label      "Presumed transfused (dispensed and not returned)" ;
        skos:prefLabel  "Presumed transfused (dispensed and not returned)" .

v2-0510:RE  a           owl:Class ;
        rdfs:label      "Released (no longer allocated for the patient)" ;
        skos:prefLabel  "Released (no longer allocated for the patient)" .
