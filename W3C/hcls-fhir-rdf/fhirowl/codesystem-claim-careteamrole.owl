@prefix claim-careteamrole: <http://hl7.org/fhir/claimcareteamrole/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

claim-careteamrole:assist
        a                owl:Class ;
        rdfs:label       "Assisting Provider" ;
        skos:definition  "Assisting care provider." ;
        skos:prefLabel   "Assisting Provider" .

claim-careteamrole:other
        a                owl:Class ;
        rdfs:label       "Other" ;
        skos:definition  "Other role on the care team." ;
        skos:prefLabel   "Other" .

claim-careteamrole:primary
        a                owl:Class ;
        rdfs:label       "Primary provider" ;
        skos:definition  "The primary care provider." ;
        skos:prefLabel   "Primary provider" .

claim-careteamrole:supervisor
        a                owl:Class ;
        rdfs:label       "Supervising Provider" ;
        skos:definition  "Supervising care provider." ;
        skos:prefLabel   "Supervising Provider" .

claim-careteamrole:  a   owl:Ontology ;
        rdfs:comment     "This value set includes sample Claim Care Team Role codes." ;
        rdfs:label       "Claim Care Team Role Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/claimcareteamrole/3.1.0" ;
        owl:versionInfo  "Claim Care Team Role Codes(3.1.0)" .
