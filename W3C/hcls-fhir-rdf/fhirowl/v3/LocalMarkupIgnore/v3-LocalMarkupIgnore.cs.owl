@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix v3-LocalMarkupIgnore: <http://hl7.org/fhir/v3/LocalMarkupIgnore/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-LocalMarkupIgnore:
        a                owl:Ontology ;
        rdfs:comment     " Tells a receiver to ignore just the local markup tags (local_markup, local_header, local_attr) when value=\"markup\", or to ignore the local markup tags and all contained content when value=\"all\"" ;
        rdfs:label       "v3 Code System LocalMarkupIgnore" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/LocalMarkupIgnore/2016-11-11" ;
        owl:versionInfo  "v3 Code System LocalMarkupIgnore(2016-11-11)" .

v3-LocalMarkupIgnore:markup
        a                owl:Class ;
        rdfs:label       "markup" ;
        skos:definition  "markup" ;
        skos:prefLabel   "markup" .

v3-LocalMarkupIgnore:all
        a                owl:Class ;
        rdfs:label       "all" ;
        skos:definition  "all" ;
        skos:prefLabel   "all" .
