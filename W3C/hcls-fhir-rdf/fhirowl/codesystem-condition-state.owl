@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .
@prefix condition-state: <http://hl7.org/fhir/condition-state/> .

condition-state:resolved
        a                owl:Class ;
        rdfs:label       "Resolved" ;
        skos:definition  "The condition is resolved." ;
        skos:prefLabel   "Resolved" .

condition-state:active
        a                owl:Class ;
        rdfs:label       "Active" ;
        skos:definition  "The condition is active." ;
        skos:prefLabel   "Active" .

condition-state:inactive
        a                owl:Class ;
        rdfs:label       "Inactive" ;
        skos:definition  "The condition is inactive, but not resolved." ;
        skos:prefLabel   "Inactive" .

condition-state:  a      owl:Ontology ;
        rdfs:comment     "Enumeration indicating whether the condition is currently active, inactive, or has been resolved." ;
        rdfs:label       "ConditionState" ;
        owl:versionIRI   "http://hl7.org/fhir/condition-state/3.1.0" ;
        owl:versionInfo  "ConditionState(3.1.0)" .
