@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix v3-TelecommunicationCapabilities: <http://hl7.org/fhir/v3/TelecommunicationCapabilities/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-TelecommunicationCapabilities:data
        a                owl:Class ;
        rdfs:label       "data" ;
        skos:definition  "Description: This device can receive data calls (i.e. modem)." ;
        skos:prefLabel   "data" .

v3-TelecommunicationCapabilities:sms
        a                owl:Class ;
        rdfs:label       "sms" ;
        skos:definition  "Description: This device can receive SMS messages." ;
        skos:prefLabel   "sms" .

v3-TelecommunicationCapabilities:tty
        a                owl:Class ;
        rdfs:label       "text" ;
        skos:definition  "Description: This device is a text telephone." ;
        skos:prefLabel   "text" .

v3-TelecommunicationCapabilities:voice
        a                owl:Class ;
        rdfs:label       "voice" ;
        skos:definition  "Description: This device can receive voice calls (i.e. talking to another person, or a recording device, or a voice activated computer)." ;
        skos:prefLabel   "voice" .

v3-TelecommunicationCapabilities:fax
        a                owl:Class ;
        rdfs:label       "fax" ;
        skos:definition  "Description: This device can receive faxes." ;
        skos:prefLabel   "fax" .

v3-TelecommunicationCapabilities:
        a                owl:Ontology ;
        rdfs:comment     "  Description:\r\nConcepts that define the telecommunication capabilities of a particular device. Used to identify the expected capabilities to be found at a particular telecommunication address." ;
        rdfs:label       "v3 Code System TelecommunicationCapabilities" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/TelecommunicationCapabilities/2016-11-11" ;
        owl:versionInfo  "v3 Code System TelecommunicationCapabilities(2016-11-11)" .
