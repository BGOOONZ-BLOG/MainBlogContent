@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix days-of-week: <http://hl7.org/fhir/days-of-week/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

days-of-week:mon  a      owl:Class ;
        rdfs:label       "Monday" ;
        skos:definition  "Monday" ;
        skos:prefLabel   "Monday" .

days-of-week:thu  a      owl:Class ;
        rdfs:label       "Thursday" ;
        skos:definition  "Thursday" ;
        skos:prefLabel   "Thursday" .

days-of-week:sat  a      owl:Class ;
        rdfs:label       "Saturday" ;
        skos:definition  "Saturday" ;
        skos:prefLabel   "Saturday" .

days-of-week:wed  a      owl:Class ;
        rdfs:label       "Wednesday" ;
        skos:definition  "Wednesday" ;
        skos:prefLabel   "Wednesday" .

days-of-week:  a         owl:Ontology ;
        rdfs:comment     "The days of the week." ;
        rdfs:label       "DaysOfWeek" ;
        owl:versionIRI   "http://hl7.org/fhir/days-of-week/3.1.0" ;
        owl:versionInfo  "DaysOfWeek(3.1.0)" .

days-of-week:fri  a      owl:Class ;
        rdfs:label       "Friday" ;
        skos:definition  "Friday" ;
        skos:prefLabel   "Friday" .

days-of-week:tue  a      owl:Class ;
        rdfs:label       "Tuesday" ;
        skos:definition  "Tuesday" ;
        skos:prefLabel   "Tuesday" .

days-of-week:sun  a      owl:Class ;
        rdfs:label       "Sunday" ;
        skos:definition  "Sunday" ;
        skos:prefLabel   "Sunday" .
