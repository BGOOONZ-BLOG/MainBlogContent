	a [fhir:DeviceMetricReference]?,
	fhir:Element.id @<id>?,
	fhir:Element.extension @<Extension>*,
	fhir:link (@<DeviceMetric> OR IRI)?,
	fhir:Reference.reference @<string>?,
	fhir:Reference.display @<string>?
