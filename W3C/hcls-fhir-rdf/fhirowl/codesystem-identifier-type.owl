@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix identifier-type: <http://hl7.org/fhir/identifier-type/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

identifier-type:FILL  a  owl:Class ;
        rdfs:label       "Filler Identifier" ;
        skos:definition  "The Identifier associated with the person, or service, who produces the observations or fulfills the order requested by the requestor." ;
        skos:prefLabel   "Filler Identifier" .

identifier-type:SNO  a   owl:Class ;
        rdfs:label       "Serial Number" ;
        skos:definition  "An identifier affixed to an item by the manufacturer when it is first made, where each item has a different identifier." ;
        skos:prefLabel   "Serial Number" .

identifier-type:  a      owl:Ontology ;
        rdfs:comment     "A coded type for an identifier that can be used to determine which identifier to use for a specific purpose." ;
        rdfs:label       "Identifier Type Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/identifier-type/3.1.0" ;
        owl:versionInfo  "Identifier Type Codes(3.1.0)" .

identifier-type:UDI  a   owl:Class ;
        rdfs:label       "Universal Device Identifier" ;
        skos:definition  "A identifier assigned to a device using the Universal Device Identifier framework as defined by FDA (http://www.fda.gov/MedicalDevices/DeviceRegulationandGuidance/UniqueDeviceIdentification/)." ;
        skos:prefLabel   "Universal Device Identifier" .

identifier-type:SB  a    owl:Class ;
        rdfs:label       "Social Beneficiary Identifier" ;
        skos:definition  "An identifier issued by a governmental organization to an individual for the purpose of the receipt of social services and benefits." ;
        skos:prefLabel   "Social Beneficiary Identifier" .

identifier-type:PLAC  a  owl:Class ;
        rdfs:label       "Placer Identifier" ;
        skos:definition  "The identifier associated with the person or service that requests or places an order." ;
        skos:prefLabel   "Placer Identifier" .
