@prefix v3-PatientImportance: <http://hl7.org/fhir/v3/PatientImportance/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-PatientImportance:GOVT
        a                owl:Class ;
        rdfs:label       "Government Dignitary" ;
        skos:definition  "Government dignitary of interest to the organization" ;
        skos:prefLabel   "Government Dignitary" .

v3-PatientImportance:SFM
        a                owl:Class ;
        rdfs:label       "Staff Family Member" ;
        skos:definition  "Family member of staff member" ;
        skos:prefLabel   "Staff Family Member" .

v3-PatientImportance:BM
        a                owl:Class ;
        rdfs:label       "Board Member" ;
        skos:definition  "Board member of health care organization" ;
        skos:prefLabel   "Board Member" .

v3-PatientImportance:DFM
        a                owl:Class ;
        rdfs:label       "Physician Family Member" ;
        skos:definition  "Family member of staff physician" ;
        skos:prefLabel   "Physician Family Member" .

v3-PatientImportance:STF
        a                owl:Class ;
        rdfs:label       "Staff Member" ;
        skos:definition  "Staff member of the health care organization" ;
        skos:prefLabel   "Staff Member" .

v3-PatientImportance:DR
        a                owl:Class ;
        rdfs:label       "Staff Physician" ;
        skos:definition  "Member of the health care organization physician staff" ;
        skos:prefLabel   "Staff Physician" .

v3-PatientImportance:FOR
        a                owl:Class ;
        rdfs:label       "Foreign Dignitary" ;
        skos:definition  "Foreign citizen dignitary of interest to the health care organization" ;
        skos:prefLabel   "Foreign Dignitary" .

v3-PatientImportance:
        a                owl:Ontology ;
        rdfs:comment     " Patient VIP code" ;
        rdfs:label       "v3 Code System PatientImportance" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/PatientImportance/2016-11-11" ;
        owl:versionInfo  "v3 Code System PatientImportance(2016-11-11)" .

v3-PatientImportance:FD
        a                owl:Class ;
        rdfs:label       "Financial Donor" ;
        skos:definition  "Financial donor to the health care organization" ;
        skos:prefLabel   "Financial Donor" .

v3-PatientImportance:VIP
        a                owl:Class ;
        rdfs:label       "Very Important Person" ;
        skos:definition  "Very important person of interest to the health care organization" ;
        skos:prefLabel   "Very Important Person" .
