@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix adverse-event-outcome: <http://hl7.org/fhir/adverse-event-outcome/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

adverse-event-outcome:recovering
        a               owl:Class ;
        rdfs:label      "Recovering" ;
        skos:prefLabel  "Recovering" .

adverse-event-outcome:
        a                owl:Ontology ;
        rdfs:comment     "TODO (and should this be required?)" ;
        rdfs:label       "AdverseEventOutcome" ;
        owl:versionIRI   "http://hl7.org/fhir/adverse-event-outcome/3.1.0" ;
        owl:versionInfo  "AdverseEventOutcome(3.1.0)" .

adverse-event-outcome:unknown
        a               owl:Class ;
        rdfs:label      "Unknown" ;
        skos:prefLabel  "Unknown" .

adverse-event-outcome:fatal
        a               owl:Class ;
        rdfs:label      "Fatal" ;
        skos:prefLabel  "Fatal" .

adverse-event-outcome:resolved
        a               owl:Class ;
        rdfs:label      "Resolved" ;
        skos:prefLabel  "Resolved" .

adverse-event-outcome:ongoing
        a               owl:Class ;
        rdfs:label      "Ongoing" ;
        skos:prefLabel  "Ongoing" .

adverse-event-outcome:resolvedWithSequelae
        a               owl:Class ;
        rdfs:label      "Resolved with Sequelae" ;
        skos:prefLabel  "Resolved with Sequelae" .
