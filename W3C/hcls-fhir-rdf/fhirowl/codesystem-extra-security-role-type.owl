@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .
@prefix extra-security-role-type: <http://hl7.org/fhir/extra-security-role-type/> .

extra-security-role-type:authserver
        a                owl:Class ;
        rdfs:label       "authorization server" ;
        skos:definition  "An entity providing authorization services to enable the electronic sharing of health-related information based on resource owner's preapproved permissions. For example, an UMA Authorization Server[UMA]" ;
        skos:prefLabel   "authorization server" .

extra-security-role-type:datacollector
        a                owl:Class ;
        rdfs:label       "data collector" ;
        skos:definition  "An entity that collects information over which the data subject may have certain rights under policy or law to control that information's management and distribution by data collectors, including the right to access, retrieve, distribute, or delete that information. " ;
        skos:prefLabel   "data collector" .

extra-security-role-type:dataprocessor
        a                owl:Class ;
        rdfs:label       "data processor" ;
        skos:definition  "An entity that processes collected information over which the data subject may have certain rights under policy or law to control that information's management and distribution by data processors, including the right to access, retrieve, distribute, or delete that information." ;
        skos:prefLabel   "data processor" .

extra-security-role-type:
        a                owl:Ontology ;
        rdfs:comment     "This CodeSystem contains Additional FHIR-defined Security Role types not defined elsewhere" ;
        rdfs:label       "SecurityRoleType" ;
        owl:versionIRI   "http://hl7.org/fhir/extra-security-role-type/3.1.0" ;
        owl:versionInfo  "SecurityRoleType(3.1.0)" .

extra-security-role-type:datasubject
        a                owl:Class ;
        rdfs:label       "data subject" ;
        skos:definition  "A person whose personal information is collected or processed, and who may have certain rights under policy or law to control that information's management and distribution by data collectors or processors, including the right to access, retrieve, distribute, or delete that information." ;
        skos:prefLabel   "data subject" .
