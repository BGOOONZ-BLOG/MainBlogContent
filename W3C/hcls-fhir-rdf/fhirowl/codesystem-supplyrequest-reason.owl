@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix supplyrequest-reason: <http://hl7.org/fhir/supplyrequest-reason/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

supplyrequest-reason:
        a                owl:Ontology ;
        rdfs:comment     "Why the supply item was requested" ;
        rdfs:label       "SupplyRequestReason" ;
        owl:versionIRI   "http://hl7.org/fhir/supplyrequest-reason/3.1.0" ;
        owl:versionInfo  "SupplyRequestReason(3.1.0)" .

supplyrequest-reason:ward-stock
        a                owl:Class ;
        rdfs:label       "Ward Stock" ;
        skos:definition  "The supply has been requested for for creating or replenishing ward stock." ;
        skos:prefLabel   "Ward Stock" .

supplyrequest-reason:patient-care
        a                owl:Class ;
        rdfs:label       "Patient Care" ;
        skos:definition  "The supply has been requested for use in direct patient care." ;
        skos:prefLabel   "Patient Care" .
