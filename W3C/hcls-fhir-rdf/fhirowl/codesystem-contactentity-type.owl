@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix contactentity-type: <http://hl7.org/fhir/contactentity-type/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

contactentity-type:PRESS
        a                owl:Class ;
        rdfs:label       "Press" ;
        skos:definition  "Dedicated contact point for matters relating to press enquiries." ;
        skos:prefLabel   "Press" .

contactentity-type:  a   owl:Ontology ;
        rdfs:comment     "This example value set defines a set of codes that can be used to indicate the purpose for which you would contact a contact party." ;
        rdfs:label       "ContactEntityType" ;
        owl:versionIRI   "http://hl7.org/fhir/contactentity-type/3.1.0" ;
        owl:versionInfo  "ContactEntityType(3.1.0)" .

contactentity-type:ADMIN
        a                owl:Class ;
        rdfs:label       "Administrative" ;
        skos:definition  "Contact details for administrative enquiries." ;
        skos:prefLabel   "Administrative" .

contactentity-type:PAYOR
        a                owl:Class ;
        rdfs:label       "Payor" ;
        skos:definition  "Contact details for dealing with issues related to insurance claims/adjudication/payment." ;
        skos:prefLabel   "Payor" .

contactentity-type:PATINF
        a                owl:Class ;
        rdfs:label       "Patient" ;
        skos:definition  "Generic information contact for patients." ;
        skos:prefLabel   "Patient" .

contactentity-type:HR
        a                owl:Class ;
        rdfs:label       "Human Resource" ;
        skos:definition  "Contact details for issues related to Human Resources, such as staff matters, OH&S etc." ;
        skos:prefLabel   "Human Resource" .

contactentity-type:BILL
        a                owl:Class ;
        rdfs:label       "Billing" ;
        skos:definition  "Contact details for information regarding to billing/general finance enquiries." ;
        skos:prefLabel   "Billing" .
