@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix message-reason-encounter: <http://hl7.org/fhir/message-reasons-encounter/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

message-reason-encounter:admit
        a                owl:Class ;
        rdfs:label       "Admit" ;
        skos:definition  "The patient has been admitted." ;
        skos:prefLabel   "Admit" .

message-reason-encounter:
        a                owl:Ontology ;
        rdfs:comment     "Example Message Reasons. These are the set of codes that might be used an updating an encounter using admin-update." ;
        rdfs:label       "Example Message Reason Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/message-reasons-encounter/3.1.0" ;
        owl:versionInfo  "Example Message Reason Codes(3.1.0)" .

message-reason-encounter:absent
        a                owl:Class ;
        rdfs:label       "Absent" ;
        skos:definition  "The patient has temporarily left the institution." ;
        skos:prefLabel   "Absent" .

message-reason-encounter:edit
        a                owl:Class ;
        rdfs:label       "Edit" ;
        skos:definition  "Encounter details have been updated (e.g. to correct a coding error)." ;
        skos:prefLabel   "Edit" .

message-reason-encounter:return
        a                owl:Class ;
        rdfs:label       "Returned" ;
        skos:definition  "The patient has returned from a temporary absence." ;
        skos:prefLabel   "Returned" .

message-reason-encounter:discharge
        a                owl:Class ;
        rdfs:label       "Discharge" ;
        skos:definition  "The patient has been discharged." ;
        skos:prefLabel   "Discharge" .

message-reason-encounter:moved
        a                owl:Class ;
        rdfs:label       "Moved" ;
        skos:definition  "The patient has been moved to a new location." ;
        skos:prefLabel   "Moved" .
