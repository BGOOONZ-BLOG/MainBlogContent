@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix encounter-special-arrangements: <http://hl7.org/fhir/encounter-special-arrangements/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

encounter-special-arrangements:dog
        a                owl:Class ;
        rdfs:label       "Guide dog" ;
        skos:definition  "The patient has a guide-dog and the location used for the encounter should be able to support the presence of the service animal." ;
        skos:prefLabel   "Guide dog" .

encounter-special-arrangements:add-bed
        a                owl:Class ;
        rdfs:label       "Additional bedding" ;
        skos:definition  "An additional bed made available for a person accompanying the patient, for example a parent accompanying a child." ;
        skos:prefLabel   "Additional bedding" .

encounter-special-arrangements:wheel
        a                owl:Class ;
        rdfs:label       "Wheelchair" ;
        skos:definition  "The patient requires a wheelchair to be made available for the encounter." ;
        skos:prefLabel   "Wheelchair" .

encounter-special-arrangements:att
        a                owl:Class ;
        rdfs:label       "Attendant" ;
        skos:definition  "A person who accompanies a patient to provide assistive services necessary for the patient's care during the encounter." ;
        skos:prefLabel   "Attendant" .

encounter-special-arrangements:
        a                owl:Ontology ;
        rdfs:comment     "This value set defines a set of codes that can be used to indicate the kinds of special arrangements in place for a patients visit." ;
        rdfs:label       "SpecialArrangements" ;
        owl:versionIRI   "http://hl7.org/fhir/encounter-special-arrangements/3.1.0" ;
        owl:versionInfo  "SpecialArrangements(3.1.0)" .

encounter-special-arrangements:int
        a                owl:Class ;
        rdfs:label       "Interpreter" ;
        skos:definition  "The patient is not fluent in the local language and requires an interpreter to be available. Refer to the Patient.Language property for the type of interpreter required." ;
        skos:prefLabel   "Interpreter" .
