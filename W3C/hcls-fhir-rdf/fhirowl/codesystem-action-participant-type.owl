@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix action-participant-type: <http://hl7.org/fhir/action-participant-type/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

action-participant-type:practitioner
        a                owl:Class ;
        rdfs:label       "Practitioner" ;
        skos:definition  "The participant is a practitioner involved in the patient's care" ;
        skos:prefLabel   "Practitioner" .

action-participant-type:
        a                owl:Ontology ;
        rdfs:comment     "The type of participant for the action" ;
        rdfs:label       "ActionParticipantType" ;
        owl:versionIRI   "http://hl7.org/fhir/action-participant-type/3.1.0" ;
        owl:versionInfo  "ActionParticipantType(3.1.0)" .

action-participant-type:related-person
        a                owl:Class ;
        rdfs:label       "Related Person" ;
        skos:definition  "The participant is a person related to the patient" ;
        skos:prefLabel   "Related Person" .

action-participant-type:patient
        a                owl:Class ;
        rdfs:label       "Patient" ;
        skos:definition  "The participant is the patient under evaluation" ;
        skos:prefLabel   "Patient" .
