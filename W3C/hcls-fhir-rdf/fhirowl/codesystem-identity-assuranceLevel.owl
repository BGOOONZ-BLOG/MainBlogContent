@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix identity-assuranceLevel: <http://hl7.org/fhir/identity-assuranceLevel/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

identity-assuranceLevel:level1
        a                owl:Class ;
        rdfs:label       "Level 1" ;
        skos:definition  "Little or no confidence in the asserted identity's accuracy." ;
        skos:prefLabel   "Level 1" .

identity-assuranceLevel:level4
        a                owl:Class ;
        rdfs:label       "Level 4" ;
        skos:definition  "Very high confidence in the asserted identity's accuracy." ;
        skos:prefLabel   "Level 4" .

identity-assuranceLevel:level2
        a                owl:Class ;
        rdfs:label       "Level 2" ;
        skos:definition  "Some confidence in the asserted identity's accuracy." ;
        skos:prefLabel   "Level 2" .

identity-assuranceLevel:
        a                owl:Ontology ;
        rdfs:comment     "The level of confidence that this link represents the same actual person, based on NIST Authentication Levels." ;
        rdfs:label       "IdentityAssuranceLevel" ;
        owl:versionIRI   "http://hl7.org/fhir/identity-assuranceLevel/3.1.0" ;
        owl:versionInfo  "IdentityAssuranceLevel(3.1.0)" .

identity-assuranceLevel:level3
        a                owl:Class ;
        rdfs:label       "Level 3" ;
        skos:definition  "High confidence in the asserted identity's accuracy." ;
        skos:prefLabel   "Level 3" .
