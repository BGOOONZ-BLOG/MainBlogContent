@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix claim-type: <http://hl7.org/fhir/ex-claimtype/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

claim-type:  a           owl:Ontology ;
        rdfs:comment     "This value set includes sample Claim Type codes." ;
        rdfs:label       "Example Claim Type Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/ex-claimtype/3.1.0" ;
        owl:versionInfo  "Example Claim Type Codes(3.1.0)" .

claim-type:oral  a       owl:Class ;
        rdfs:label       "Oral" ;
        skos:definition  "Dental, Denture and Hygiene claims." ;
        skos:prefLabel   "Oral" .

claim-type:pharmacy  a   owl:Class ;
        rdfs:label       "Pharmacy" ;
        skos:definition  "Pharmacy claims for goods and services." ;
        skos:prefLabel   "Pharmacy" .

claim-type:professional
        a                owl:Class ;
        rdfs:label       "Professional" ;
        skos:definition  "Typically outpatient claims from Physician, Psychological, Chiropractor, Physiotherapy, Speech Pathology, rehabilitative, consulting." ;
        skos:prefLabel   "Professional" .

claim-type:vision  a     owl:Class ;
        rdfs:label       "Vision" ;
        skos:definition  "Vision claims for professional services and products such as glasses and contact lenses." ;
        skos:prefLabel   "Vision" .

claim-type:institutional
        a                owl:Class ;
        rdfs:label       "Institutional" ;
        skos:definition  "Hospital, clinic and typically inpatient claims." ;
        skos:prefLabel   "Institutional" .
