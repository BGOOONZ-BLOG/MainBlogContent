@prefix report-participant-type: <http://hl7.org/fhir/report-participant-type/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

report-participant-type:
        a                owl:Ontology ;
        rdfs:comment     "The type of participant." ;
        rdfs:label       "TestReportParticipantType" ;
        owl:versionIRI   "http://hl7.org/fhir/report-participant-type/3.1.0" ;
        owl:versionInfo  "TestReportParticipantType(3.1.0)" .

report-participant-type:test-engine
        a                owl:Class ;
        rdfs:label       "Test Engine" ;
        skos:definition  "The test execution engine." ;
        skos:prefLabel   "Test Engine" .

report-participant-type:client
        a                owl:Class ;
        rdfs:label       "Client" ;
        skos:definition  "A FHIR Client" ;
        skos:prefLabel   "Client" .

report-participant-type:server
        a                owl:Class ;
        rdfs:label       "Server" ;
        skos:definition  "A FHIR Server" ;
        skos:prefLabel   "Server" .
