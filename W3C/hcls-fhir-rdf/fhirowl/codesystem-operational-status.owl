@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix operational-status: <http://hl7.org/fhir/operational-status/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

operational-status:on
        a                owl:Class ;
        rdfs:label       "On" ;
        skos:definition  "The device is fully operational." ;
        skos:prefLabel   "On" .

operational-status:hw-discon
        a                owl:Class ;
        rdfs:label       "Hardware Disconnectd" ;
        skos:definition  "The device hardware is disconnected." ;
        skos:prefLabel   "Hardware Disconnectd" .

operational-status:entered-in-error
        a                owl:Class ;
        rdfs:label       "Entered In Error" ;
        skos:definition  "The device was entered in error." ;
        skos:prefLabel   "Entered In Error" .

operational-status:  a   owl:Ontology ;
        rdfs:comment     "Codes representing the current status of the device - on, off, suspended, etc." ;
        rdfs:label       "DeviceComponentOperationalStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/operational-status/3.1.0" ;
        owl:versionInfo  "DeviceComponentOperationalStatus(3.1.0)" .

operational-status:standby
        a                owl:Class ;
        rdfs:label       "Standby" ;
        skos:definition  "The device is ready but not actively operating." ;
        skos:prefLabel   "Standby" .

operational-status:not-ready
        a                owl:Class ;
        rdfs:label       "Not Ready" ;
        skos:definition  "The device is not ready." ;
        skos:prefLabel   "Not Ready" .

operational-status:off
        a                owl:Class ;
        rdfs:label       "Off" ;
        skos:definition  "The device is off." ;
        skos:prefLabel   "Off" .

operational-status:transduc-discon
        a                owl:Class ;
        rdfs:label       "Transducer Diconnected" ;
        skos:definition  "The device transducer is diconnected." ;
        skos:prefLabel   "Transducer Diconnected" .
