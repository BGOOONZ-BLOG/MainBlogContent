@prefix surface: <http://hl7.org/fhir/FDI-surface/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

surface:D  a             owl:Class ;
        rdfs:label       "Distal" ;
        skos:definition  "The surface of a tooth that faces away from the midline of the face." ;
        skos:prefLabel   "Distal" .

surface:I  a             owl:Class ;
        rdfs:label       "Incisal" ;
        skos:definition  "The biting edge of anterior teeth." ;
        skos:prefLabel   "Incisal" .

surface:B  a             owl:Class ;
        rdfs:label       "Buccal" ;
        skos:definition  "The surface of a posterior tooth facing the cheeks." ;
        skos:prefLabel   "Buccal" .

surface:DO  a            owl:Class ;
        rdfs:label       "Distoclusal" ;
        skos:definition  "The Distoclusal surfaces of a tooth." ;
        skos:prefLabel   "Distoclusal" .

surface:L  a             owl:Class ;
        rdfs:label       "Lingual" ;
        skos:definition  "The surface of a tooth facing the tongue." ;
        skos:prefLabel   "Lingual" .

surface:V  a             owl:Class ;
        rdfs:label       "Ventral" ;
        skos:definition  "The surface of a tooth facing the lips." ;
        skos:prefLabel   "Ventral" .

surface:MO  a            owl:Class ;
        rdfs:label       "Mesioclusal" ;
        skos:definition  "The Mesioclusal surfaces of a tooth." ;
        skos:prefLabel   "Mesioclusal" .

surface:O  a             owl:Class ;
        rdfs:label       "Occlusal" ;
        skos:definition  "The chewing surface of posterior teeth." ;
        skos:prefLabel   "Occlusal" .

surface:MOD  a           owl:Class ;
        rdfs:label       "Mesioclusodistal" ;
        skos:definition  "The Mesioclusodistal surfaces of a tooth." ;
        skos:prefLabel   "Mesioclusodistal" .

surface:  a              owl:Ontology ;
        rdfs:comment     "This value set includes a smattering of FDI tooth surface codes." ;
        rdfs:label       "Surface Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/FDI-surface/3.1.0" ;
        owl:versionInfo  "Surface Codes(3.1.0)" .

surface:M  a             owl:Class ;
        rdfs:label       "Mesial" ;
        skos:definition  "The surface of a tooth that is closest to the midline (middle) of the face." ;
        skos:prefLabel   "Mesial" .

surface:DI  a            owl:Class ;
        rdfs:label       "Distoincisal" ;
        skos:definition  "The Distoincisal surfaces of a tooth." ;
        skos:prefLabel   "Distoincisal" .
