@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix contract-actorrole: <http://www.hl7.org/fhir/contractactorrole/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

contract-actorrole:practitioner
        a                owl:Class ;
        rdfs:label       "Practitioner" ;
        skos:definition  "Someone who provides health care related services to people or animals including both clinical and support services." ;
        skos:prefLabel   "Practitioner" .

contract-actorrole:  a   owl:Ontology ;
        rdfs:comment     "This value set includes sample Contract Actor Role codes." ;
        rdfs:label       "Contract Actor Role Codes" ;
        owl:versionIRI   "http://www.hl7.org/fhir/contractactorrole/3.1.0" ;
        owl:versionInfo  "Contract Actor Role Codes(3.1.0)" .

contract-actorrole:patient
        a                owl:Class ;
        rdfs:label       "Patient" ;
        skos:definition  "A receiver, human or animal, of health care related goods and services." ;
        skos:prefLabel   "Patient" .
