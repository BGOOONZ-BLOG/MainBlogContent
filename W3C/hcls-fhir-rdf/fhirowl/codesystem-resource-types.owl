@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .
@prefix resource-types: <http://hl7.org/fhir/resource-types/> .

resource-types:Parameters
        a                owl:Class ;
        rdfs:label       "Parameters" ;
        skos:definition  "This special resource type is used to represent an operation request and response (operations.html). It has no other use, and there is no RESTful endpoint associated with it." ;
        skos:prefLabel   "Parameters" .

resource-types:ResearchStudy
        a                owl:Class ;
        rdfs:label       "ResearchStudy" ;
        skos:definition  "A process where a researcher or organization plans and then executes a series of steps intended to increase the field of healthcare-related knowledge.  This includes studies of safety, efficacy, comparative effectiveness and other information about medications, devices, therapies and other interventional and investigative techniques.  A ResearchStudy involves the gathering of information about human or animal subjects." ;
        skos:prefLabel   "ResearchStudy" .

resource-types:NutritionOrder
        a                owl:Class ;
        rdfs:label       "NutritionOrder" ;
        skos:definition  "A request to supply a diet, formula feeding (enteral) or oral nutritional supplement to a patient/resident." ;
        skos:prefLabel   "NutritionOrder" .

resource-types:PractitionerRole
        a                owl:Class ;
        rdfs:label       "PractitionerRole" ;
        skos:definition  "A specific set of Roles/Locations/specialties/services that a practitioner may perform at an organization for a period of time." ;
        skos:prefLabel   "PractitionerRole" .

resource-types:VisionPrescription
        a                owl:Class ;
        rdfs:label       "VisionPrescription" ;
        skos:definition  "An authorization for the supply of glasses and/or contact lenses to a patient." ;
        skos:prefLabel   "VisionPrescription" .

resource-types:ProcessRequest
        a                owl:Class ;
        rdfs:label       "ProcessRequest" ;
        skos:definition  "This resource provides the target, request and response, and action details for an action to be performed by the target on or about existing resources." ;
        skos:prefLabel   "ProcessRequest" .

resource-types:CapabilityStatement
        a                owl:Class ;
        rdfs:label       "CapabilityStatement" ;
        skos:definition  "A Capability Statement documents a set of capabilities (behaviors) of a FHIR Server that may be used as a statement of actual server functionality or a statement of required or desired server implementation." ;
        skos:prefLabel   "CapabilityStatement" .

resource-types:ImmunizationRecommendation
        a                owl:Class ;
        rdfs:label       "ImmunizationRecommendation" ;
        skos:definition  "A patient's point-in-time immunization and recommendation (i.e. forecasting a patient's immunization eligibility according to a published schedule) with optional supporting justification." ;
        skos:prefLabel   "ImmunizationRecommendation" .

resource-types:TestReport
        a                owl:Class ;
        rdfs:label       "TestReport" ;
        skos:definition  "A summary of information based on the results of executing a TestScript." ;
        skos:prefLabel   "TestReport" .

resource-types:ResearchSubject
        a                owl:Class ;
        rdfs:label       "ResearchSubject" ;
        skos:definition  "A process where a researcher or organization plans and then executes a series of steps intended to increase the field of healthcare-related knowledge.  This includes studies of safety, efficacy, comparative effectiveness and other information about medications, devices, therapies and other interventional and investigative techniques.  A ResearchStudy involves the gathering of information about human or animal subjects." ;
        skos:prefLabel   "ResearchSubject" .

resource-types:Appointment
        a                owl:Class ;
        rdfs:label       "Appointment" ;
        skos:definition  "A booking of a healthcare event among patient(s), practitioner(s), related person(s) and/or device(s) for a specific date/time. This may result in one or more Encounter(s)." ;
        skos:prefLabel   "Appointment" .

resource-types:Binary
        a                owl:Class ;
        rdfs:label       "Binary" ;
        skos:definition  "A binary resource can contain any content, whether text, image, pdf, zip archive, etc." ;
        skos:prefLabel   "Binary" .

resource-types:Linkage
        a                owl:Class ;
        rdfs:label       "Linkage" ;
        skos:definition  "Identifies two or more records (resource instances) that are referring to the same real-world \"occurrence\"." ;
        skos:prefLabel   "Linkage" .

resource-types:EligibilityResponse
        a                owl:Class ;
        rdfs:label       "EligibilityResponse" ;
        skos:definition  "This resource provides eligibility and plan details from the processing of an Eligibility resource." ;
        skos:prefLabel   "EligibilityResponse" .

resource-types:Goal  a   owl:Class ;
        rdfs:label       "Goal" ;
        skos:definition  "Describes the intended objective(s) for a patient, group or organization care, for example, weight loss, restoring an activity of daily living, obtaining herd immunity via immunization, meeting a process improvement objective, etc." ;
        skos:prefLabel   "Goal" .

resource-types:DomainResource
        a                owl:Class ;
        rdfs:label       "DomainResource" ;
        skos:definition  "A resource that includes narrative, extensions, and contained resources." ;
        skos:prefLabel   "DomainResource" .

resource-types:ClinicalImpression
        a                owl:Class ;
        rdfs:label       "ClinicalImpression" ;
        skos:definition  "A record of a clinical assessment performed to determine what problem(s) may affect the patient and before planning the treatments or management strategies that are best to manage a patient's condition. Assessments are often 1:1 with a clinical consultation / encounter,  but this varies greatly depending on the clinical workflow. This resource is called \"ClinicalImpression\" rather than \"ClinicalAssessment\" to avoid confusion with the recording of assessment tools such as Apgar score." ;
        skos:prefLabel   "ClinicalImpression" .

resource-types:MessageDefinition
        a                owl:Class ;
        rdfs:label       "MessageDefinition" ;
        skos:definition  "Defines the characteristics of a message that can be shared between systems, including the type of event that initiates the message, the content to be transmitted and what response(s), if any, are permitted." ;
        skos:prefLabel   "MessageDefinition" .

resource-types:GraphDefinition
        a                owl:Class ;
        rdfs:label       "GraphDefinition" ;
        skos:definition  "A formal computable definition of a graph of resources - that is, a coherent set of resources that form a graph by following references. The Graph Definition resource defines a set and makes rules about the set." ;
        skos:prefLabel   "GraphDefinition" .

resource-types:Procedure
        a                owl:Class ;
        rdfs:label       "Procedure" ;
        skos:definition  "An action that is or was performed on a patient. This can be a physical intervention like an operation, or less invasive like counseling or hypnotherapy." ;
        skos:prefLabel   "Procedure" .

resource-types:Slot  a   owl:Class ;
        rdfs:label       "Slot" ;
        skos:definition  "A slot of time on a schedule that may be available for booking appointments." ;
        skos:prefLabel   "Slot" .

resource-types:AllergyIntolerance
        a                owl:Class ;
        rdfs:label       "AllergyIntolerance" ;
        skos:definition  "Risk of harmful or undesirable, physiological response which is unique to an individual and associated with exposure to a substance." ;
        skos:prefLabel   "AllergyIntolerance" .

resource-types:Composition
        a                owl:Class ;
        rdfs:label       "Composition" ;
        skos:definition  "A set of healthcare-related information that is assembled together into a single logical document that provides a single coherent statement of meaning, establishes its own context and that has clinical attestation with regard to who is making the statement. While a Composition defines the structure, it does not actually contain the content: rather the full content of a document is contained in a Bundle, of which the Composition is the first resource contained." ;
        skos:prefLabel   "Composition" .

resource-types:NamingSystem
        a                owl:Class ;
        rdfs:label       "NamingSystem" ;
        skos:definition  "A curated namespace that issues unique symbols within that namespace for the identification of concepts, people, devices, etc.  Represents a \"System\" used within the Identifier and Coding data types." ;
        skos:prefLabel   "NamingSystem" .

resource-types:ExplanationOfBenefit
        a                owl:Class ;
        rdfs:label       "ExplanationOfBenefit" ;
        skos:definition  "This resource provides: the claim details; adjudication details from the processing of a Claim; and optionally account balance information, for informing the subscriber of the benefits provided." ;
        skos:prefLabel   "ExplanationOfBenefit" .

resource-types:Questionnaire
        a                owl:Class ;
        rdfs:label       "Questionnaire" ;
        skos:definition  "A structured set of questions intended to guide the collection of answers from end-users. Questionnaires provide detailed control over order, presentation, phraseology and grouping to allow coherent, consistent data collection." ;
        skos:prefLabel   "Questionnaire" .

resource-types:Bundle
        a                owl:Class ;
        rdfs:label       "Bundle" ;
        skos:definition  "A container for a collection of resources." ;
        skos:prefLabel   "Bundle" .

resource-types:Task  a   owl:Class ;
        rdfs:label       "Task" ;
        skos:definition  "A task to be performed." ;
        skos:prefLabel   "Task" .

resource-types:Subscription
        a                owl:Class ;
        rdfs:label       "Subscription" ;
        skos:definition  "The subscription resource is used to define a push based subscription from a server to another system. Once a subscription is registered with the server, the server checks every resource that is created or updated, and if the resource matches the given criteria, it sends a message on the defined \"channel\" so that another system is able to take an appropriate action." ;
        skos:prefLabel   "Subscription" .

resource-types:Flag  a   owl:Class ;
        rdfs:label       "Flag" ;
        skos:definition  "Prospective warnings of potential issues when providing care to the patient." ;
        skos:prefLabel   "Flag" .

resource-types:List  a   owl:Class ;
        rdfs:label       "List" ;
        skos:definition  "A set of information summarized from a list of other resources." ;
        skos:prefLabel   "List" .

resource-types:CompartmentDefinition
        a                owl:Class ;
        rdfs:label       "CompartmentDefinition" ;
        skos:definition  "A compartment definition that defines how resources are accessed on a server." ;
        skos:prefLabel   "CompartmentDefinition" .

resource-types:ConceptMap
        a                owl:Class ;
        rdfs:label       "ConceptMap" ;
        skos:definition  "A statement of relationships from one set of concepts to one or more other concepts - either code systems or data elements, or classes in class models." ;
        skos:prefLabel   "ConceptMap" .

resource-types:PaymentReconciliation
        a                owl:Class ;
        rdfs:label       "PaymentReconciliation" ;
        skos:definition  "This resource provides payment details and claim references supporting a bulk payment." ;
        skos:prefLabel   "PaymentReconciliation" .

resource-types:QuestionnaireResponse
        a                owl:Class ;
        rdfs:label       "QuestionnaireResponse" ;
        skos:definition  "A structured set of questions and their answers. The questions are ordered and grouped into coherent subsets, corresponding to the structure of the grouping of the questionnaire being responded to." ;
        skos:prefLabel   "QuestionnaireResponse" .

resource-types:ProcessResponse
        a                owl:Class ;
        rdfs:label       "ProcessResponse" ;
        skos:definition  "This resource provides processing status, errors and notes from the processing of a resource." ;
        skos:prefLabel   "ProcessResponse" .

resource-types:HealthcareService
        a                owl:Class ;
        rdfs:label       "HealthcareService" ;
        skos:definition  "The details of a healthcare service available at a location." ;
        skos:prefLabel   "HealthcareService" .

resource-types:ClaimResponse
        a                owl:Class ;
        rdfs:label       "ClaimResponse" ;
        skos:definition  "This resource provides the adjudication details from the processing of a Claim resource." ;
        skos:prefLabel   "ClaimResponse" .

resource-types:TestScript
        a                owl:Class ;
        rdfs:label       "TestScript" ;
        skos:definition  "A structured set of tests against a FHIR server implementation to determine compliance against the FHIR specification." ;
        skos:prefLabel   "TestScript" .

resource-types:ImplementationGuide
        a                owl:Class ;
        rdfs:label       "ImplementationGuide" ;
        skos:definition  "A set of rules of how FHIR is used to solve a particular problem. This resource is used to gather all the parts of an implementation guide into a logical whole and to publish a computable definition of all the parts." ;
        skos:prefLabel   "ImplementationGuide" .

resource-types:StructureMap
        a                owl:Class ;
        rdfs:label       "StructureMap" ;
        skos:definition  "A Map of relationships between 2 structures that can be used to transform data." ;
        skos:prefLabel   "StructureMap" .

resource-types:EligibilityRequest
        a                owl:Class ;
        rdfs:label       "EligibilityRequest" ;
        skos:definition  "The EligibilityRequest provides patient and insurance coverage information to an insurer for them to respond, in the form of an EligibilityResponse, with information regarding whether the stated coverage is valid and in-force and optionally to provide the insurance details of the policy." ;
        skos:prefLabel   "EligibilityRequest" .

resource-types:Medication
        a                owl:Class ;
        rdfs:label       "Medication" ;
        skos:definition  "This resource is primarily used for the identification and definition of a medication. It covers the ingredients and the packaging for a medication." ;
        skos:prefLabel   "Medication" .

resource-types:Group  a  owl:Class ;
        rdfs:label       "Group" ;
        skos:definition  "Represents a defined collection of entities that may be discussed or acted upon collectively but which are not expected to act collectively and are not formally or legally recognized; i.e. a collection of entities that isn't an Organization." ;
        skos:prefLabel   "Group" .

resource-types:Patient
        a                owl:Class ;
        rdfs:label       "Patient" ;
        skos:definition  "Demographics and other administrative information about an individual or animal receiving care or other health-related services." ;
        skos:prefLabel   "Patient" .

resource-types:Person
        a                owl:Class ;
        rdfs:label       "Person" ;
        skos:definition  "Demographics and administrative information about a person independent of a specific health-related context." ;
        skos:prefLabel   "Person" .

resource-types:RelatedPerson
        a                owl:Class ;
        rdfs:label       "RelatedPerson" ;
        skos:definition  "Information about a person that is involved in the care for a patient, but who is not the target of healthcare, nor has a formal responsibility in the care process." ;
        skos:prefLabel   "RelatedPerson" .

resource-types:DeviceMetric
        a                owl:Class ;
        rdfs:label       "DeviceMetric" ;
        skos:definition  "Describes a measurement, calculation or setting capability of a medical device." ;
        skos:prefLabel   "DeviceMetric" .

resource-types:Practitioner
        a                owl:Class ;
        rdfs:label       "Practitioner" ;
        skos:definition  "A person who is directly or indirectly involved in the provisioning of healthcare." ;
        skos:prefLabel   "Practitioner" .

resource-types:MedicationAdministration
        a                owl:Class ;
        rdfs:label       "MedicationAdministration" ;
        skos:definition  "Describes the event of a patient consuming or otherwise being administered a medication.  This may be as simple as swallowing a tablet or it may be a long running infusion.  Related resources tie this event to the authorizing prescription, and the specific encounter between patient and health care practitioner." ;
        skos:prefLabel   "MedicationAdministration" .

resource-types:DiagnosticReport
        a                owl:Class ;
        rdfs:label       "DiagnosticReport" ;
        skos:definition  "The findings and interpretation of diagnostic  tests performed on patients, groups of patients, devices, and locations, and/or specimens derived from these. The report includes clinical context such as requesting and provider information, and some mix of atomic results, images, textual and coded interpretations, and formatted representation of diagnostic reports." ;
        skos:prefLabel   "DiagnosticReport" .

resource-types:Claim  a  owl:Class ;
        rdfs:label       "Claim" ;
        skos:definition  "A provider issued list of services and products provided, or to be provided, to a patient which is provided to an insurer for payment recovery." ;
        skos:prefLabel   "Claim" .

resource-types:Sequence
        a                owl:Class ;
        rdfs:label       "Sequence" ;
        skos:definition  "Raw data describing a biological sequence." ;
        skos:prefLabel   "Sequence" .

resource-types:  a       owl:Ontology ;
        rdfs:comment     "One of the resource types defined as part of FHIR." ;
        rdfs:label       "ResourceType" ;
        owl:versionIRI   "http://hl7.org/fhir/resource-types/3.1.0" ;
        owl:versionInfo  "ResourceType(3.1.0)" .

resource-types:PlanDefinition
        a                owl:Class ;
        rdfs:label       "PlanDefinition" ;
        skos:definition  "This resource allows for the definition of various types of plans as a sharable, consumable, and executable artifact. The resource is general enough to support the description of a broad range of clinical artifacts such as clinical decision support rules, order sets and protocols." ;
        skos:prefLabel   "PlanDefinition" .

resource-types:Encounter
        a                owl:Class ;
        rdfs:label       "Encounter" ;
        skos:definition  "An interaction between a patient and healthcare provider(s) for the purpose of providing healthcare service(s) or assessing the health status of a patient." ;
        skos:prefLabel   "Encounter" .

resource-types:DocumentReference
        a                owl:Class ;
        rdfs:label       "DocumentReference" ;
        skos:definition  "A reference to a document." ;
        skos:prefLabel   "DocumentReference" .

resource-types:GuidanceResponse
        a                owl:Class ;
        rdfs:label       "GuidanceResponse" ;
        skos:definition  "A guidance response is the formal response to a guidance request, including any output parameters returned by the evaluation, as well as the description of any proposed actions to be taken." ;
        skos:prefLabel   "GuidanceResponse" .

resource-types:OperationDefinition
        a                owl:Class ;
        rdfs:label       "OperationDefinition" ;
        skos:definition  "A formal computable definition of an operation (on the RESTful interface) or a named query (using the search interaction)." ;
        skos:prefLabel   "OperationDefinition" .

resource-types:OperationOutcome
        a                owl:Class ;
        rdfs:label       "OperationOutcome" ;
        skos:definition  "A collection of error, warning or information messages that result from a system action." ;
        skos:prefLabel   "OperationOutcome" .

resource-types:Observation
        a                owl:Class ;
        rdfs:label       "Observation" ;
        skos:definition  "Measurements and simple assertions made about a patient, device or other subject." ;
        skos:prefLabel   "Observation" .

resource-types:ExpansionProfile
        a                owl:Class ;
        rdfs:label       "ExpansionProfile" ;
        skos:definition  "Resource to define constraints on the Expansion of a FHIR ValueSet." ;
        skos:prefLabel   "ExpansionProfile" .

resource-types:BodyStructure
        a                owl:Class ;
        rdfs:label       "BodyStructure" ;
        skos:definition  "Record details about an anatomical structure.  This resource may be used when a coded concept does not provide the necessary detail needed for the use case." ;
        skos:prefLabel   "BodyStructure" .

resource-types:ActivityDefinition
        a                owl:Class ;
        rdfs:label       "ActivityDefinition" ;
        skos:definition  "This resource allows for the definition of some activity to be performed, independent of a particular patient, practitioner, or other performance context." ;
        skos:prefLabel   "ActivityDefinition" .

resource-types:MeasureReport
        a                owl:Class ;
        rdfs:label       "MeasureReport" ;
        skos:definition  "The MeasureReport resource contains the results of evaluating a measure." ;
        skos:prefLabel   "MeasureReport" .

resource-types:StructureDefinition
        a                owl:Class ;
        rdfs:label       "StructureDefinition" ;
        skos:definition  "A definition of a FHIR structure. This resource is used to describe the underlying resources, data types defined in FHIR, and also for describing extensions and constraints on resources and data types." ;
        skos:prefLabel   "StructureDefinition" .

resource-types:EnrollmentResponse
        a                owl:Class ;
        rdfs:label       "EnrollmentResponse" ;
        skos:definition  "This resource provides enrollment and plan details from the processing of an Enrollment resource." ;
        skos:prefLabel   "EnrollmentResponse" .

resource-types:Contract
        a                owl:Class ;
        rdfs:label       "Contract" ;
        skos:definition  "A formal agreement between parties regarding the conduct of business, exchange of information or other matters." ;
        skos:prefLabel   "Contract" .

resource-types:Condition
        a                owl:Class ;
        rdfs:label       "Condition" ;
        skos:definition  "A clinical condition, problem, diagnosis, or other event, situation, issue, or clinical concept that has risen to a level of concern." ;
        skos:prefLabel   "Condition" .

resource-types:Device
        a                owl:Class ;
        rdfs:label       "Device" ;
        skos:definition  "This resource identifies an instance or a type of a manufactured item that is used in the provision of healthcare without being substantially changed through that activity. The device may be a medical or non-medical device.  Medical devices include durable (reusable) medical equipment, implantable devices, as well as disposable equipment used for diagnostic, treatment, and research for healthcare and public health.  Non-medical devices may include items such as a machine, cellphone, computer, application, etc." ;
        skos:prefLabel   "Device" .

resource-types:AdverseEvent
        a                owl:Class ;
        rdfs:label       "AdverseEvent" ;
        skos:definition  "Actual or  potential/avoided event causing unintended physical injury resulting from or contributed to by medical care, a research study or other healthcare setting factors that requires additional monitoring, treatment, or hospitalization, or that results in death." ;
        skos:prefLabel   "AdverseEvent" .

resource-types:ImagingStudy
        a                owl:Class ;
        rdfs:label       "ImagingStudy" ;
        skos:definition  "Representation of the content produced in a DICOM imaging study. A study comprises a set of series, each of which includes a set of Service-Object Pair Instances (SOP Instances - images or other data) acquired or produced in a common context.  A series is of only one modality (e.g. X-ray, CT, MR, ultrasound), but a study may have multiple series of different modalities." ;
        skos:prefLabel   "ImagingStudy" .

resource-types:ProcedureRequest
        a                owl:Class ;
        rdfs:label       "ProcedureRequest" ;
        skos:definition  "A record of a request for diagnostic investigations, treatments, or operations to be performed." ;
        skos:prefLabel   "ProcedureRequest" .

resource-types:ChargeItem
        a                owl:Class ;
        rdfs:label       "ChargeItem" ;
        skos:definition  "The resource ChargeItem describes the provision of healthcare provider products for a certain patient, therefore referring not only to the product, but containing in addition details of the provision, like date, time, amounts and participating organizations and persons. Main Usage of the ChargeItem is to enable the billing process and internal cost allocation." ;
        skos:prefLabel   "ChargeItem" .

resource-types:Communication
        a                owl:Class ;
        rdfs:label       "Communication" ;
        skos:definition  "An occurrence of information being transmitted; e.g. an alert that was sent to a responsible provider, a public health agency was notified about a reportable condition." ;
        skos:prefLabel   "Communication" .

resource-types:Consent
        a                owl:Class ;
        rdfs:label       "Consent" ;
        skos:definition  "A record of a healthcare consumer’s policy choices, which permits or denies identified recipient(s) or recipient role(s) to perform one or more actions within a given policy context, for specific purposes and periods of time." ;
        skos:prefLabel   "Consent" .

resource-types:Basic  a  owl:Class ;
        rdfs:label       "Basic" ;
        skos:definition  "Basic is used for handling concepts not yet defined in FHIR, narrative-only resources that don't map to an existing resource, and custom resources not appropriate for inclusion in the FHIR specification." ;
        skos:prefLabel   "Basic" .

resource-types:CommunicationRequest
        a                owl:Class ;
        rdfs:label       "CommunicationRequest" ;
        skos:definition  "A request to convey information; e.g. the CDS system proposes that an alert be sent to a responsible provider, the CDS system proposes that the public health agency be notified about a reportable condition." ;
        skos:prefLabel   "CommunicationRequest" .

resource-types:CareTeam
        a                owl:Class ;
        rdfs:label       "CareTeam" ;
        skos:definition  "The Care Team includes all the people and organizations who plan to participate in the coordination and delivery of care for a patient." ;
        skos:prefLabel   "CareTeam" .

resource-types:CodeSystem
        a                owl:Class ;
        rdfs:label       "CodeSystem" ;
        skos:definition  "A code system resource specifies a set of codes drawn from one or more code systems." ;
        skos:prefLabel   "CodeSystem" .

resource-types:CarePlan
        a                owl:Class ;
        rdfs:label       "CarePlan" ;
        skos:definition  "Describes the intention of how one or more practitioners intend to deliver care for a particular patient, group or community for a period of time, possibly limited to care for a specific condition or set of conditions." ;
        skos:prefLabel   "CarePlan" .

resource-types:Specimen
        a                owl:Class ;
        rdfs:label       "Specimen" ;
        skos:definition  "A sample to be used for analysis." ;
        skos:prefLabel   "Specimen" .

resource-types:Resource
        a                owl:Class ;
        rdfs:label       "Resource" ;
        skos:definition  "This is the base resource type for everything." ;
        skos:prefLabel   "Resource" .

resource-types:Account
        a                owl:Class ;
        rdfs:label       "Account" ;
        skos:definition  "A financial tool for tracking value accrued for a particular purpose.  In the healthcare field, used to track charges for a patient, cost centers, etc." ;
        skos:prefLabel   "Account" .

resource-types:EventDefinition
        a                owl:Class ;
        rdfs:label       "EventDefinition" ;
        skos:definition  "The EventDefinition resource provides a reusable description of when a particular event can occur." ;
        skos:prefLabel   "EventDefinition" .

resource-types:EnrollmentRequest
        a                owl:Class ;
        rdfs:label       "EnrollmentRequest" ;
        skos:definition  "This resource provides the insurance enrollment details to the insurer regarding a specified coverage." ;
        skos:prefLabel   "EnrollmentRequest" .

resource-types:PaymentNotice
        a                owl:Class ;
        rdfs:label       "PaymentNotice" ;
        skos:definition  "This resource provides the status of the payment for goods and services rendered, and the request and response resource references." ;
        skos:prefLabel   "PaymentNotice" .

resource-types:SupplyRequest
        a                owl:Class ;
        rdfs:label       "SupplyRequest" ;
        skos:definition  "A record of a request for a medication, substance or device used in the healthcare setting." ;
        skos:prefLabel   "SupplyRequest" .

resource-types:MessageHeader
        a                owl:Class ;
        rdfs:label       "MessageHeader" ;
        skos:definition  "The header for a message exchange that is either requesting or responding to an action.  The reference(s) that are the subject of the action as well as other information related to the action are typically transmitted in a bundle in which the MessageHeader resource instance is the first resource in the bundle." ;
        skos:prefLabel   "MessageHeader" .

resource-types:Substance
        a                owl:Class ;
        rdfs:label       "Substance" ;
        skos:definition  "A homogeneous material with a definite composition." ;
        skos:prefLabel   "Substance" .

resource-types:Measure
        a                owl:Class ;
        rdfs:label       "Measure" ;
        skos:definition  "The Measure resource provides the definition of a quality measure." ;
        skos:prefLabel   "Measure" .

resource-types:AppointmentResponse
        a                owl:Class ;
        rdfs:label       "AppointmentResponse" ;
        skos:definition  "A reply to an appointment request for a patient and/or practitioner(s), such as a confirmation or rejection." ;
        skos:prefLabel   "AppointmentResponse" .

resource-types:SupplyDelivery
        a                owl:Class ;
        rdfs:label       "SupplyDelivery" ;
        skos:definition  "Record of delivery of what is supplied." ;
        skos:prefLabel   "SupplyDelivery" .

resource-types:RequestGroup
        a                owl:Class ;
        rdfs:label       "RequestGroup" ;
        skos:definition  "A group of related requests that can be used to capture intended activities that have inter-dependencies such as \"give this medication after that one\"." ;
        skos:prefLabel   "RequestGroup" .

resource-types:Organization
        a                owl:Class ;
        rdfs:label       "Organization" ;
        skos:definition  "A formally or informally recognized grouping of people or organizations formed for the purpose of achieving some form of collective action.  Includes companies, institutions, corporations, departments, community groups, healthcare practice groups, etc." ;
        skos:prefLabel   "Organization" .

resource-types:EpisodeOfCare
        a                owl:Class ;
        rdfs:label       "EpisodeOfCare" ;
        skos:definition  "An association between a patient and an organization / healthcare provider(s) during which time encounters may occur. The managing organization assumes a level of responsibility for the patient during this time." ;
        skos:prefLabel   "EpisodeOfCare" .

resource-types:SearchParameter
        a                owl:Class ;
        rdfs:label       "SearchParameter" ;
        skos:definition  "A search parameter that defines a named search item that can be used to search/filter on a resource." ;
        skos:prefLabel   "SearchParameter" .

resource-types:DetectedIssue
        a                owl:Class ;
        rdfs:label       "DetectedIssue" ;
        skos:definition  "Indicates an actual or potential clinical issue with or between one or more active or proposed clinical actions for a patient; e.g. Drug-drug interaction, Ineffective treatment frequency, Procedure-condition conflict, etc." ;
        skos:prefLabel   "DetectedIssue" .

resource-types:ValueSet
        a                owl:Class ;
        rdfs:label       "ValueSet" ;
        skos:definition  "A value set specifies a set of codes drawn from one or more code systems." ;
        skos:prefLabel   "ValueSet" .

resource-types:ServiceDefinition
        a                owl:Class ;
        rdfs:label       "ServiceDefinition" ;
        skos:definition  "The ServiceDefinition describes a unit of decision support functionality that is made available as a service, such as immunization modules or drug-drug interaction checking." ;
        skos:prefLabel   "ServiceDefinition" .

resource-types:MedicationRequest
        a                owl:Class ;
        rdfs:label       "MedicationRequest" ;
        skos:definition  "An order or request for both supply of the medication and the instructions for administration of the medication to a patient. The resource is called \"MedicationRequest\" rather than \"MedicationPrescription\" or \"MedicationOrder\" to generalize the use across inpatient and outpatient settings, including care plans, etc., and to harmonize with workflow patterns." ;
        skos:prefLabel   "MedicationRequest" .

resource-types:Coverage
        a                owl:Class ;
        rdfs:label       "Coverage" ;
        skos:definition  "Financial instrument which may be used to reimburse or pay for health care products and services." ;
        skos:prefLabel   "Coverage" .

resource-types:AuditEvent
        a                owl:Class ;
        rdfs:label       "AuditEvent" ;
        skos:definition  "A record of an event made for purposes of maintaining a security log. Typical uses include detection of intrusion attempts and monitoring for inappropriate usage." ;
        skos:prefLabel   "AuditEvent" .

resource-types:Endpoint
        a                owl:Class ;
        rdfs:label       "Endpoint" ;
        skos:definition  "The technical details of an endpoint that can be used for electronic services, such as for web services providing XDS.b or a REST endpoint for another FHIR server. This may include any security context information." ;
        skos:prefLabel   "Endpoint" .

resource-types:Provenance
        a                owl:Class ;
        rdfs:label       "Provenance" ;
        skos:definition  "Provenance of a resource is a record that describes entities and processes involved in producing and delivering or otherwise influencing that resource. Provenance provides a critical foundation for assessing authenticity, enabling trust, and allowing reproducibility. Provenance assertions are a form of contextual metadata and can themselves become important records with their own provenance. Provenance statement indicates clinical significance in terms of confidence in authenticity, reliability, and trustworthiness, integrity, and stage in lifecycle (e.g. Document Completion - has the artifact been legally authenticated), all of which may impact security, privacy, and trust policies." ;
        skos:prefLabel   "Provenance" .

resource-types:Library
        a                owl:Class ;
        rdfs:label       "Library" ;
        skos:definition  "The Library resource is a general-purpose container for knowledge asset definitions. It can be used to describe and expose existing knowledge assets such as logic libraries and information model descriptions, as well as to describe a collection of knowledge assets." ;
        skos:prefLabel   "Library" .

resource-types:Immunization
        a                owl:Class ;
        rdfs:label       "Immunization" ;
        skos:definition  "Describes the event of a patient being administered a vaccination or a record of a vaccination as reported by a patient, a clinician or another party and may include vaccine reaction information and what vaccination protocol was followed." ;
        skos:prefLabel   "Immunization" .

resource-types:DocumentManifest
        a                owl:Class ;
        rdfs:label       "DocumentManifest" ;
        skos:definition  "A collection of documents compiled for a purpose together with metadata that applies to the collection." ;
        skos:prefLabel   "DocumentManifest" .

resource-types:Location
        a                owl:Class ;
        rdfs:label       "Location" ;
        skos:definition  "Details and position information for a physical place where services are provided  and resources and participants may be stored, found, contained or accommodated." ;
        skos:prefLabel   "Location" .

resource-types:Schedule
        a                owl:Class ;
        rdfs:label       "Schedule" ;
        skos:definition  "A container for slots of time that may be available for booking appointments." ;
        skos:prefLabel   "Schedule" .

resource-types:FamilyMemberHistory
        a                owl:Class ;
        rdfs:label       "FamilyMemberHistory" ;
        skos:definition  "Significant health conditions for a person related to the patient relevant in the context of care for the patient." ;
        skos:prefLabel   "FamilyMemberHistory" .

resource-types:ImagingManifest
        a                owl:Class ;
        rdfs:label       "ImagingManifest" ;
        skos:definition  "A text description of the DICOM SOP instances selected in the ImagingManifest; or the reason for, or significance of, the selection." ;
        skos:prefLabel   "ImagingManifest" .

resource-types:DeviceUseStatement
        a                owl:Class ;
        rdfs:label       "DeviceUseStatement" ;
        skos:definition  "A record of a device being used by a patient where the record is the result of a report from the patient or another clinician." ;
        skos:prefLabel   "DeviceUseStatement" .

resource-types:MedicationStatement
        a                owl:Class ;
        rdfs:label       "MedicationStatement" ;
        skos:definition  "A record of a medication that is being consumed by a patient.   A MedicationStatement may indicate that the patient may be taking the medication now, or has taken the medication in the past or will be taking the medication in the future.  The source of this information can be the patient, significant other (such as a family member or spouse), or a clinician.  A common scenario where this information is captured is during the history taking process during a patient visit or stay.   The medication information may come from sources such as the patient's memory, from a prescription bottle,  or from a list of medications the patient, clinician or other party maintains. \r\rThe primary difference between a medication statement and a medication administration is that the medication administration has complete administration information and is based on actual administration information from the person who administered the medication.  A medication statement is often, if not always, less specific.  There is no required date/time when the medication was administered, in fact we only know that a source has reported the patient is taking this medication, where details such as time, quantity, or rate or even medication product may be incomplete or missing or less precise.  As stated earlier, the medication statement information may come from the patient's memory, from a prescription bottle or from a list of medications the patient, clinician or other party maintains.  Medication administration is more formal and is not missing detailed information." ;
        skos:prefLabel   "MedicationStatement" .

resource-types:RiskAssessment
        a                owl:Class ;
        rdfs:label       "RiskAssessment" ;
        skos:definition  "An assessment of the likely outcome(s) for a patient or other subject as well as the likelihood of each outcome." ;
        skos:prefLabel   "RiskAssessment" .

resource-types:DeviceComponent
        a                owl:Class ;
        rdfs:label       "DeviceComponent" ;
        skos:definition  "The characteristics, operational status and capabilities of a medical-related component of a medical device." ;
        skos:prefLabel   "DeviceComponent" .

resource-types:Media  a  owl:Class ;
        rdfs:label       "Media" ;
        skos:definition  "A photo, video, or audio recording acquired or used in healthcare. The actual content may be inline or provided by direct reference." ;
        skos:prefLabel   "Media" .

resource-types:DeviceRequest
        a                owl:Class ;
        rdfs:label       "DeviceRequest" ;
        skos:definition  "Represents a request for a patient to employ a medical device. The device may be an implantable device, or an external assistive device, such as a walker." ;
        skos:prefLabel   "DeviceRequest" .

resource-types:MedicationDispense
        a                owl:Class ;
        rdfs:label       "MedicationDispense" ;
        skos:definition  "Indicates that a medication product is to be or has been dispensed for a named person/patient.  This includes a description of the medication product (supply) provided and the instructions for administering the medication.  The medication dispense is the result of a pharmacy system responding to a medication order." ;
        skos:prefLabel   "MedicationDispense" .
