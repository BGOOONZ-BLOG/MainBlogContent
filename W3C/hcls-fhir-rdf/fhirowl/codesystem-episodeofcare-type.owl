        rdfs:label      "Post co-ordinated diabetes program" ;
        skos:prefLabel  "Post co-ordinated diabetes program" .

episodeofcare-type:pac
        a               owl:Class ;
        rdfs:label      "Post Acute Care" ;
        skos:prefLabel  "Post Acute Care" .

