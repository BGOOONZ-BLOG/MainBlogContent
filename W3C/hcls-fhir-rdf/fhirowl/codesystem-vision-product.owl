@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .
@prefix vision-product: <http://hl7.org/fhir/ex-visionprescriptionproduct/> .

vision-product:contact
        a                owl:Class ;
        rdfs:label       "Contact Lens" ;
        skos:definition  "A lens to be fitted for wearing directly on an eye." ;
        skos:prefLabel   "Contact Lens" .

vision-product:lens  a   owl:Class ;
        rdfs:label       "Lens" ;
        skos:definition  "A lens to be fitted to a frame to comprise a pair of glasses." ;
        skos:prefLabel   "Lens" .

vision-product:  a       owl:Ontology ;
        rdfs:comment     "This value set includes a smattering of Prescription Product codes." ;
        rdfs:label       "Example Vision Prescription Product Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/ex-visionprescriptionproduct/3.1.0" ;
        owl:versionInfo  "Example Vision Prescription Product Codes(3.1.0)" .
