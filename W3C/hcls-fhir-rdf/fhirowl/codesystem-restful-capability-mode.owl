@prefix restful-capability-mode: <http://hl7.org/fhir/restful-capability-mode/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

restful-capability-mode:server
        a                owl:Class ;
        rdfs:label       "Server" ;
        skos:definition  "The application acts as a server for this resource." ;
        skos:prefLabel   "Server" .

restful-capability-mode:
        a                owl:Ontology ;
        rdfs:comment     "The mode of a RESTful capability statement." ;
        rdfs:label       "RestfulCapabilityMode" ;
        owl:versionIRI   "http://hl7.org/fhir/restful-capability-mode/3.1.0" ;
        owl:versionInfo  "RestfulCapabilityMode(3.1.0)" .

restful-capability-mode:client
        a                owl:Class ;
        rdfs:label       "Client" ;
        skos:definition  "The application acts as a client for this resource." ;
        skos:prefLabel   "Client" .
