@prefix adjudication: <http://hl7.org/fhir/adjudication/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

adjudication:eligpercent
        a                owl:Class ;
        rdfs:label       "Eligible %" ;
        skos:definition  "Eligible Percentage" ;
        skos:prefLabel   "Eligible %" .

adjudication:benefit  a  owl:Class ;
        rdfs:label       "Benefit Amount" ;
        skos:definition  "Amount payable under the coverage" ;
        skos:prefLabel   "Benefit Amount" .

adjudication:total  a    owl:Class ;
        rdfs:label       "Total" ;
        skos:definition  "Total submitted" ;
        skos:prefLabel   "Total" .

adjudication:deductible
        a                owl:Class ;
        rdfs:label       "Deductable" ;
        skos:definition  "Amount deducted from the eligible amount prior to adjudication" ;
        skos:prefLabel   "Deductable" .

adjudication:eligible
        a                owl:Class ;
        rdfs:label       "Eligible Amount" ;
        skos:definition  "Amount of the change which is considered for adjudication" ;
        skos:prefLabel   "Eligible Amount" .

adjudication:  a         owl:Ontology ;
        rdfs:comment     "This value set includes a smattering of Adjudication Value codes which includes codes to indicate the amounts eligible under the plan, the amount of benefit, copays etc." ;
        rdfs:label       "Adjudication Value Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/adjudication/3.1.0" ;
        owl:versionInfo  "Adjudication Value Codes(3.1.0)" .

adjudication:tax  a      owl:Class ;
        rdfs:label       "Emergency Department" ;
        skos:definition  "Emergency Department" ;
        skos:prefLabel   "Emergency Department" .

adjudication:copay  a    owl:Class ;
        rdfs:label       "CoPay" ;
        skos:definition  "Patient Co-Payment" ;
        skos:prefLabel   "CoPay" .
