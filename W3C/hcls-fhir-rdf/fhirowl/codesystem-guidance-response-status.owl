@prefix guidance-response-status: <http://hl7.org/fhir/guidance-response-status/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

guidance-response-status:success
        a                owl:Class ;
        rdfs:label       "Success" ;
        skos:definition  "The request was processed successfully" ;
        skos:prefLabel   "Success" .

guidance-response-status:entered-in-error
        a                owl:Class ;
        rdfs:label       "Entered In Error" ;
        skos:definition  "The response was entered in error" ;
        skos:prefLabel   "Entered In Error" .

guidance-response-status:in-progress
        a                owl:Class ;
        rdfs:label       "In Progress" ;
        skos:definition  "The request is currently being processed" ;
        skos:prefLabel   "In Progress" .

guidance-response-status:data-required
        a                owl:Class ;
        rdfs:label       "Data Required" ;
        skos:definition  "The request was processed, but more data is required to complete the evaluation" ;
        skos:prefLabel   "Data Required" .

guidance-response-status:failure
        a                owl:Class ;
        rdfs:label       "Failure" ;
        skos:definition  "The request was not processed successfully" ;
        skos:prefLabel   "Failure" .

guidance-response-status:data-requested
        a                owl:Class ;
        rdfs:label       "Data Requested" ;
        skos:definition  "The request was processed successfully, but more data may result in a more complete evaluation" ;
        skos:prefLabel   "Data Requested" .

guidance-response-status:
        a                owl:Ontology ;
        rdfs:comment     "The status of a guidance response" ;
        rdfs:label       "GuidanceResponseStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/guidance-response-status/3.1.0" ;
        owl:versionInfo  "GuidanceResponseStatus(3.1.0)" .
