@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix codesystem-content-mode: <http://hl7.org/fhir/codesystem-content-mode/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

codesystem-content-mode:fragment
        a                owl:Class ;
        rdfs:label       "Fragment" ;
        skos:definition  "A subset of the code system concepts are included in the code system resource" ;
        skos:prefLabel   "Fragment" .

codesystem-content-mode:not-present
        a                owl:Class ;
        rdfs:label       "Not Present" ;
        skos:definition  "None of the concepts defined by the code system are included in the code system resource" ;
        skos:prefLabel   "Not Present" .

codesystem-content-mode:complete
        a                owl:Class ;
        rdfs:label       "Complete" ;
        skos:definition  "All the concepts defined by the code system are included in the code system resource" ;
        skos:prefLabel   "Complete" .

codesystem-content-mode:
        a                owl:Ontology ;
        rdfs:comment     "How much of the content of the code system - the concepts and codes it defines - are represented in a code system resource" ;
        rdfs:label       "CodeSystemContentMode" ;
        owl:versionIRI   "http://hl7.org/fhir/codesystem-content-mode/3.1.0" ;
        owl:versionInfo  "CodeSystemContentMode(3.1.0)" .

codesystem-content-mode:example
        a                owl:Class ;
        rdfs:label       "Example" ;
        skos:definition  "A few representative concepts are included in the code system resource" ;
        skos:prefLabel   "Example" .
