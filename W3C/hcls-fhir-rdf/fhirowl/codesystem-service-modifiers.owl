@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix service-modifiers: <http://hl7.org/fhir/ex-servicemodifier/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

service-modifiers:sr  a  owl:Class ;
        rdfs:label       "Side of the Road" ;
        skos:definition  "Services provided on the side of the road or such other non-conventional setting." ;
        skos:prefLabel   "Side of the Road" .

service-modifiers:  a    owl:Ontology ;
        rdfs:comment     "This value set includes sample Service Modifier codes which may support differential payment." ;
        rdfs:label       "Example Service Modifier Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/ex-servicemodifier/3.1.0" ;
        owl:versionInfo  "Example Service Modifier Codes(3.1.0)" .

service-modifiers:ah  a  owl:Class ;
        rdfs:label       "After hours" ;
        skos:definition  "Services provided outside or normal business hours." ;
        skos:prefLabel   "After hours" .
