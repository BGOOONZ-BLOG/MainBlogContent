@prefix entformula-additive: <http://hl7.org/fhir/entformula-additive/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

entformula-additive:carbohydrate
        a                owl:Class ;
        rdfs:label       "Carbohydrate" ;
        skos:definition  "Modular carbohydrate enteral formula component" ;
        skos:prefLabel   "Carbohydrate" .

entformula-additive:lipid
        a                owl:Class ;
        rdfs:label       "Lipid" ;
        skos:definition  "Modular lipid enteral formula component" ;
        skos:prefLabel   "Lipid" .

entformula-additive:water
        a                owl:Class ;
        rdfs:label       "Water" ;
        skos:definition  "Added water" ;
        skos:prefLabel   "Water" .

entformula-additive:  a  owl:Ontology ;
        rdfs:comment     "EnteralFormulaAdditiveType: Codes for the type of modular component such as protein, carbohydrate or fiber to be provided in addition to or mixed with the base formula. This value set is provided as a suggestive example." ;
        rdfs:label       "Enteral Formula Additive Type Code" ;
        owl:versionIRI   "http://hl7.org/fhir/entformula-additive/3.1.0" ;
        owl:versionInfo  "Enteral Formula Additive Type Code(3.1.0)" .

entformula-additive:protein
        a                owl:Class ;
        rdfs:label       "Protein" ;
        skos:definition  "Modular protein enteral formula component" ;
        skos:prefLabel   "Protein" .

entformula-additive:fiber
        a                owl:Class ;
        rdfs:label       "Fiber" ;
        skos:definition  "Modular fiber enteral formula component" ;
        skos:prefLabel   "Fiber" .
