@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v3-ResponseModality: <http://hl7.org/fhir/v3/ResponseModality/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-ResponseModality:B
        a                owl:Class ;
        rdfs:label       "Batch" ;
        skos:definition  "Query response to be sent as an HL7 Batch." ;
        skos:prefLabel   "Batch" .

v3-ResponseModality:R
        a                owl:Class ;
        rdfs:label       "Real Time" ;
        skos:definition  "Query response to occur in real time." ;
        skos:prefLabel   "Real Time" .

v3-ResponseModality:  a  owl:Ontology ;
        rdfs:comment     " Defines the timing and grouping of the response instances.  OpenIssue:\r\nDescription copied from Concept Domain of same name.  Must be verified." ;
        rdfs:label       "v3 Code System ResponseModality" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/ResponseModality/2016-11-11" ;
        owl:versionInfo  "v3 Code System ResponseModality(2016-11-11)" .

v3-ResponseModality:T
        a                owl:Class ;
        rdfs:label       "Bolus" ;
        skos:definition  "Query response to sent as a series of responses at the same time without the use of batch formatting." ;
        skos:prefLabel   "Bolus" .
