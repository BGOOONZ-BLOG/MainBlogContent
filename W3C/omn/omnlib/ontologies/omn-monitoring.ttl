###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#isMeasurementMetricOf

:isMeasurementMetricOf rdf:type owl:ObjectProperty ;

                       rdfs:comment "Is a measurement metric of any observed object, e.g. resource, component or service"^^xsd:string ;

                       owl:inverseOf :hasMeasurementMetric ;
              
                       rdfs:domain :Metric ;
              
                       rdfs:range [ rdf:type owl:Class ;
                                     owl:unionOf ( omn:Component
                                                   omn:Resource
                                                   omn:Service
                                                 )
                                   ] .



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring-metric#hasMeasurementMetric

:hasMeasurementMetric rdf:type owl:ObjectProperty ;

                      owl:inverseOf :isMeasurementMetricOf ;
                      
                      rdfs:range :Metric ;
                      
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( omn:Component
                                                  omn:Resource
                                                  omn:Service
                                                )
                                  ] .



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#isMeasurementOf

:isMeasurementOf rdf:type owl:ObjectProperty ;
                    
                 rdfs:comment "Defines the relation between a measurement and a metric"^^xsd:string ;
                    
                 rdfs:domain :Measurement ;
                    
                 rdfs:range :Metric .



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#measuredBy

:measuredBy rdf:type owl:ObjectProperty ;
              
            owl:inverseOf :measuresMetric ;

            rdfs:domain :Metric ;
              
            rdfs:range :Tool .



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#measuresMetric

:measuresMetric rdf:type owl:ObjectProperty ;
                
                owl:inverseOf :measuredBy ;

                rdfs:domain :Tool ;

                rdfs:range :Metric .                



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#monitors

:monitors rdf:type owl:FunctionalProperty ,
                         owl:ObjectProperty ;
                
          rdfs:domain :Tool ;

          rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( omn:Component
                                               omn:Resource
                                               omn:Service
                                               omn-federation:Infrastructure
                                             )
                      ] .


###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#hasMetricAttribute

:hasMetricAttribute rdf:type owl:ObjectProperty ;

                    rdfs:domain :Metric ;
                      
                    rdfs:range :Measurement .



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#hasUnit

:hasUnit rdf:type owl:ObjectProperty ;
         
         rdfs:domain :Data ;

         rdfs:range :Unit .



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#hasLifetime

:hasLifetime rdf:type owl:ObjectProperty ;
                      
             rdfs:range :Lifetime ;

             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( omn:Service
                                         omn:Resource
                                       )
                         ] .



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#sentBy

:sentBy rdf:type owl:ObjectProperty ;
                      
        rdfs:range :Tool ;

        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :Data
                                    omn-lifecycle:Request
                                  )
                    ] .




###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#sendTo

:sendTo rdf:type owl:ObjectProperty ;
                      
        rdfs:domain [ rdf:type owl:Class ;
                     owl:unionOf ( :Data
                                   omn-lifecycle:Request
                                 )
                    ] ;

        rdfs:range [ rdf:type owl:Class ;
                     owl:unionOf ( :Tool
                                   omn:Service
                                 )
                   ] .



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#sentFrom

:sentFrom rdf:type owl:ObjectProperty ;

          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Data
                                      omn-lifecycle:Request
                                    )
                      ] ;

          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :Tool
                                     omn-federation:Infrastructure
                                   )
                     ] .



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#retrievedBy

:retrievedBy rdf:type owl:ObjectProperty ;

             rdfs:domain :Data ;

             rdfs:range :Tool .

###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#retrievedFrom

:retrievedFrom rdf:type owl:ObjectProperty ;

               rdfs:domain :Data ;

               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( :Tool
                                          omn:Service
                                          omn-federation:Infrastructure
                                        )
                          ] .      



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#pushesDataTo

:pushesDataTo rdf:type owl:ObjectProperty ;

        rdfs:domain :Tool ;

        rdfs:range [ rdf:type owl:Class ;
                     owl:unionOf ( :Tool
                                   omn:Service
                                 )
                   ] .



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#pushedBy

:pushedBy rdf:type owl:ObjectProperty ;

          rdfs:domain :Data ;

          rdfs:range :Tool .





