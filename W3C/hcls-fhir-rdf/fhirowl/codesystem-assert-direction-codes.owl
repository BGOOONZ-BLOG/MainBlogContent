@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix assert-direction-codes: <http://hl7.org/fhir/assert-direction-codes/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

assert-direction-codes:
        a                owl:Ontology ;
        rdfs:comment     "The type of direction to use for assertion." ;
        rdfs:label       "AssertionDirectionType" ;
        owl:versionIRI   "http://hl7.org/fhir/assert-direction-codes/3.1.0" ;
        owl:versionInfo  "AssertionDirectionType(3.1.0)" .

assert-direction-codes:request
        a                owl:Class ;
        rdfs:label       "request" ;
        skos:definition  "The assertion is evaluated on the request." ;
        skos:prefLabel   "request" .

assert-direction-codes:response
        a                owl:Class ;
        rdfs:label       "response" ;
        skos:definition  "The assertion is evaluated on the response. This is the default value." ;
        skos:prefLabel   "response" .
