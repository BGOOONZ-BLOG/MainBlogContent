@prefix observation-statistics: <http://hl7.org/fhir/observation-statistics/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

observation-statistics:kurtosis
        a                owl:Class ;
        rdfs:label       "Kurtosis" ;
        skos:definition  "Kurtosis  is a measure of the \"tailedness\" of the probability distribution of a real-valued random variable.   Source: [Wikipedia](https://en.wikipedia.org/wiki/Kurtosis)" ;
        skos:prefLabel   "Kurtosis" .

observation-statistics:80-percent
        a                owl:Class ;
        rdfs:label       "80th Percentile" ;
        skos:definition  "The 80th [Percentile](https://en.wikipedia.org/wiki/Percentile) of N measurements over the stated period" ;
        skos:prefLabel   "80th Percentile" .

observation-statistics:median
        a                owl:Class ;
        rdfs:label       "Median" ;
        skos:definition  "The [median](https://en.wikipedia.org/wiki/Median) of N measurements over the stated period" ;
        skos:prefLabel   "Median" .

observation-statistics:minimum
        a                owl:Class ;
        rdfs:label       "Minimum" ;
        skos:definition  "The [minimum](https://en.wikipedia.org/wiki/Minimal_element) value of N measurements over the stated period" ;
        skos:prefLabel   "Minimum" .

observation-statistics:5-2
        a                owl:Class ;
        rdfs:label       "2nd Quintile" ;
        skos:definition  "The second of four values that divide the N measurements into a frequency distribution of five classes with each containing one fifth of the total population" ;
        skos:prefLabel   "2nd Quintile" .

observation-statistics:count
        a                owl:Class ;
        rdfs:label       "Count" ;
        skos:definition  "The [number] of valid measurements over the stated period that contributed to the other statistical outputs" ;
        skos:prefLabel   "Count" .

observation-statistics:average
        a                owl:Class ;
        rdfs:label       "Average" ;
        skos:definition  "The [mean](https://en.wikipedia.org/wiki/Arithmetic_mean) of N measurements over the stated period" ;
        skos:prefLabel   "Average" .

observation-statistics:variance
        a                owl:Class ;
        rdfs:label       "Variance" ;
        skos:definition  "The [variance](https://en.wikipedia.org/wiki/Variance) of N measurements over the stated period" ;
        skos:prefLabel   "Variance" .

observation-statistics:5-1
        a                owl:Class ;
        rdfs:label       "1st Quintile" ;
        skos:definition  "The lowest of four values that divide the N measurements into a frequency distribution of five classes with each containing one fifth of the total population" ;
        skos:prefLabel   "1st Quintile" .

observation-statistics:maximum
        a                owl:Class ;
        rdfs:label       "Maximum" ;
        skos:definition  "The [maximum](https://en.wikipedia.org/wiki/Maximal_element) value of N measurements over the stated period" ;
        skos:prefLabel   "Maximum" .

observation-statistics:sum
        a                owl:Class ;
        rdfs:label       "Sum" ;
        skos:definition  "The [sum](https://en.wikipedia.org/wiki/Summation) of N measurements over the stated period" ;
        skos:prefLabel   "Sum" .

observation-statistics:
        a                owl:Ontology ;
        rdfs:comment     "The statistical operation parameter -\"statistic\" -  codes" ;
        rdfs:label       "StatisticsCode" ;
        owl:versionIRI   "http://hl7.org/fhir/observation-statistics/3.1.0" ;
        owl:versionInfo  "StatisticsCode(3.1.0)" .

observation-statistics:4-upper
        a                owl:Class ;
        rdfs:label       "Upper Quartile" ;
        skos:definition  "The upper [Quartile](https://en.wikipedia.org/wiki/Quartile) Boundary of N measurements over the stated period" ;
        skos:prefLabel   "Upper Quartile" .

observation-statistics:skew
        a                owl:Class ;
        rdfs:label       "Skew" ;
        skos:definition  "Skewness is a measure of the asymmetry of the probability distribution of a real-valued random variable about its mean. The skewness value can be positive or negative, or even undefined.  Source: [Wikipedia](https://en.wikipedia.org/wiki/Skewness)" ;
        skos:prefLabel   "Skew" .

observation-statistics:20-percent
        a                owl:Class ;
        rdfs:label       "20th Percentile" ;
        skos:definition  "The 20th [Percentile](https://en.wikipedia.org/wiki/Percentile) of N measurements over the stated period" ;
        skos:prefLabel   "20th Percentile" .

observation-statistics:totalcount
        a                owl:Class ;
        rdfs:label       "Total Count" ;
        skos:definition  "The total [number] of valid measurements over the stated period, including observations that were ignored because they did not contain valid result values" ;
        skos:prefLabel   "Total Count" .

observation-statistics:std-dev
        a                owl:Class ;
        rdfs:label       "Standard Deviation" ;
        skos:definition  "The [standard deviation](https://en.wikipedia.org/wiki/Standard_deviation) of N measurements over the stated period" ;
        skos:prefLabel   "Standard Deviation" .

observation-statistics:4-dev
        a                owl:Class ;
        rdfs:label       "Quartile Deviation" ;
        skos:definition  "The difference between the upper and lower [Quartiles](https://en.wikipedia.org/wiki/Quartile) is called the Interquartile range. (IQR = Q3-Q1) Quartile deviation or Semi-interquartile range is one-half the difference between the first and the third quartiles." ;
        skos:prefLabel   "Quartile Deviation" .

observation-statistics:5-4
        a                owl:Class ;
        rdfs:label       "4th Quintile" ;
        skos:definition  "The fourth of four values that divide the N measurements into a frequency distribution of five classes with each containing one fifth of the total population" ;
        skos:prefLabel   "4th Quintile" .

observation-statistics:4-lower
        a                owl:Class ;
        rdfs:label       "Lower Quartile" ;
        skos:definition  "The lower [Quartile](https://en.wikipedia.org/wiki/Quartile) Boundary of N measurements over the stated period" ;
        skos:prefLabel   "Lower Quartile" .

observation-statistics:regression
        a                owl:Class ;
        rdfs:label       "Regression" ;
        skos:definition  "Linear regression is an approach for modeling two-dimensional sample points with one independent variable and one dependent variable (conventionally, the x and y coordinates in a Cartesian coordinate system) and finds a linear function (a non-vertical straight line) that, as accurately as possible, predicts the dependent variable values as a function of the independent variables. Source: [Wikipedia](https://en.wikipedia.org/wiki/Simple_linear_regression)  This Statistic code will return both a gradient and an intercept value." ;
        skos:prefLabel   "Regression" .

observation-statistics:5-3
        a                owl:Class ;
        rdfs:label       "3rd Quintile" ;
        skos:definition  "The third of four values that divide the N measurements into a frequency distribution of five classes with each containing one fifth of the total population" ;
        skos:prefLabel   "3rd Quintile" .
