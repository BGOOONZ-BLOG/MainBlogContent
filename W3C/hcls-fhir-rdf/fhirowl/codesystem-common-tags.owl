@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix common-tags: <http://hl7.org/fhir/common-tags/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

common-tags:actionable
        a                owl:Class ;
        rdfs:label       "Actionable" ;
        skos:definition  "This request is intended to be acted upon, not merely stored" ;
        skos:prefLabel   "Actionable" .

common-tags:  a          owl:Ontology ;
        rdfs:comment     "Common Tag Codes defined by FHIR project" ;
        rdfs:label       "Common Tags" ;
        owl:versionIRI   "http://hl7.org/fhir/common-tags/3.1.0" ;
        owl:versionInfo  "Common Tags(3.1.0)" .
