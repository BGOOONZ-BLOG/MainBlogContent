@prefix questionnaire-display-category: <http://hl7.org/fhir/questionnaire-display-category/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

questionnaire-display-category:
        a                owl:Ontology ;
        rdfs:comment     "Codes defining the purpose of a Questionnaire item of type 'text'." ;
        rdfs:label       "Questionnaire Text Categories" ;
        owl:versionIRI   "http://hl7.org/fhir/questionnaire-display-category/3.1.0" ;
        owl:versionInfo  "Questionnaire Text Categories(3.1.0)" .

questionnaire-display-category:security
        a                owl:Class ;
        rdfs:label       "Security" ;
        skos:definition  "The text provides guidance on how the information should be or will be handled from a security/confidentiality/access control perspective when completed" ;
        skos:prefLabel   "Security" .

questionnaire-display-category:instructions
        a                owl:Class ;
        rdfs:label       "Instructions" ;
        skos:definition  "The text provides guidances on how to populate or use a portion of the questionnaire (or the questionnaire as a whole)." ;
        skos:prefLabel   "Instructions" .
