@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix measure-report-type: <http://hl7.org/fhir/measure-report-type/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

measure-report-type:summary
        a                owl:Class ;
        rdfs:label       "Summary" ;
        skos:definition  "A summary report that returns the number of patients in each population criteria for the measure" ;
        skos:prefLabel   "Summary" .

measure-report-type:  a  owl:Ontology ;
        rdfs:comment     "The type of the measure report" ;
        rdfs:label       "MeasureReportType" ;
        owl:versionIRI   "http://hl7.org/fhir/measure-report-type/3.1.0" ;
        owl:versionInfo  "MeasureReportType(3.1.0)" .

measure-report-type:patient-list
        a                owl:Class ;
        rdfs:label       "Patient List" ;
        skos:definition  "A patient list report that includes a listing of patients that satisfied each population criteria in the measure" ;
        skos:prefLabel   "Patient List" .

measure-report-type:individual
        a                owl:Class ;
        rdfs:label       "Individual" ;
        skos:definition  "An individual report that provides information on the performance for a given measure with respect to a single patient" ;
        skos:prefLabel   "Individual" .
