@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix testscript-profile-origin-types: <http://hl7.org/fhir/testscript-profile-origin-types/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

testscript-profile-origin-types:FHIR-Client
        a                owl:Class ;
        rdfs:label       "FHIR Client" ;
        skos:definition  "General FHIR client used to initiate operations against a FHIR server." ;
        skos:prefLabel   "FHIR Client" .

testscript-profile-origin-types:
        a                owl:Ontology ;
        rdfs:comment     "This value set defines a set of codes that are used to indicate the profile type of a test system when acting as the origin within a TestScript." ;
        rdfs:label       "TestScriptProfileOriginType" ;
        owl:versionIRI   "http://hl7.org/fhir/testscript-profile-origin-types/3.1.0" ;
        owl:versionInfo  "TestScriptProfileOriginType(3.1.0)" .

testscript-profile-origin-types:FHIR-SDC-FormFiller
        a                owl:Class ;
        rdfs:label       "FHIR SDC FormFiller" ;
        skos:definition  "A FHIR client acting as a Structured Data Capture Form Filler." ;
        skos:prefLabel   "FHIR SDC FormFiller" .
