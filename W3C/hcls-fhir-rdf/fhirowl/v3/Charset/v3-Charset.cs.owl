@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v3-Charset: <http://hl7.org/fhir/v3/Charset/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-Charset:US-ASCII  a   owl:Class ;
        rdfs:label       "US-ASCII" ;
        skos:definition  "Required for HL7 use." ;
        skos:prefLabel   "US-ASCII" .

v3-Charset:  a           owl:Ontology ;
        rdfs:comment     " Internet Assigned Numbers Authority (IANA) Charset Types" ;
        rdfs:label       "v3 Code System Charset" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/Charset/2016-11-11" ;
        owl:versionInfo  "v3 Code System Charset(2016-11-11)" .

v3-Charset:UTF-8  a      owl:Class ;
        rdfs:label       "UTF-8" ;
        skos:definition  "Required for Unicode support." ;
        skos:prefLabel   "UTF-8" .

v3-Charset:ISO-8859-1
        a                owl:Class ;
        rdfs:label       "ISO-8859-1" ;
        skos:definition  "HL7 is indifferent to the use of this Charset." ;
        skos:prefLabel   "ISO-8859-1" .

v3-Charset:JIS-2022-JP
        a                owl:Class ;
        rdfs:label       "JIS-2022-JP" ;
        skos:definition  "HL7 is indifferent to the use of this Charset." ;
        skos:prefLabel   "JIS-2022-JP" .

v3-Charset:ISO-10646-UCS-4
        a                owl:Class ;
        rdfs:label       "ISO-10646-UCS-4" ;
        skos:definition  "Deprecated for HL7 use." ;
        skos:prefLabel   "ISO-10646-UCS-4" .

v3-Charset:ISO-8859-2
        a                owl:Class ;
        rdfs:label       "ISO-8859-2" ;
        skos:definition  "HL7 is indifferent to the use of this Charset." ;
        skos:prefLabel   "ISO-8859-2" .

v3-Charset:ISO-10646-UCS-2
        a                owl:Class ;
        rdfs:label       "ISO-10646-UCS-2" ;
        skos:definition  "Deprecated for HL7 use." ;
        skos:prefLabel   "ISO-10646-UCS-2" .

v3-Charset:UTF-7  a      owl:Class ;
        rdfs:label       "UTF-7" ;
        skos:definition  "HL7 is indifferent to the use of this Charset." ;
        skos:prefLabel   "UTF-7" .

v3-Charset:EBCDIC  a     owl:Class ;
        rdfs:label       "EBCDIC" ;
        skos:definition  "HL7 is indifferent to the use of this Charset." ;
        skos:prefLabel   "EBCDIC" .

v3-Charset:ISO-8859-5
        a                owl:Class ;
        rdfs:label       "ISO-8859-5" ;
        skos:definition  "HL7 is indifferent to the use of this Charset." ;
        skos:prefLabel   "ISO-8859-5" .
