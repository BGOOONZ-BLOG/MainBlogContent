@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix v3-EquipmentAlertLevel: <http://hl7.org/fhir/v3/EquipmentAlertLevel/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-EquipmentAlertLevel:N
        a                owl:Class ;
        rdfs:label       "Normal" ;
        skos:definition  "No Corrective Action Needed" ;
        skos:prefLabel   "Normal" .

v3-EquipmentAlertLevel:S
        a                owl:Class ;
        rdfs:label       "Serious" ;
        skos:definition  "Corrective Action Required" ;
        skos:prefLabel   "Serious" .

v3-EquipmentAlertLevel:C
        a                owl:Class ;
        rdfs:label       "Critical" ;
        skos:definition  "Shut Down, Fix Problem and Re-init" ;
        skos:prefLabel   "Critical" .

v3-EquipmentAlertLevel:
        a                owl:Ontology ;
        rdfs:comment     "**** MISSING DEFINITIONS ****" ;
        rdfs:label       "v3 Code System EquipmentAlertLevel" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/EquipmentAlertLevel/2016-11-11" ;
        owl:versionInfo  "v3 Code System EquipmentAlertLevel(2016-11-11)" .

v3-EquipmentAlertLevel:W
        a                owl:Class ;
        rdfs:label       "Warning" ;
        skos:definition  "Corrective Action Anticipated" ;
        skos:prefLabel   "Warning" .
