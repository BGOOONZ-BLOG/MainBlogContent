        "concept": [
            {
                "code": "M",
                "definition": "The surface of a tooth that is closest to the midline (middle) of the face.",
                "display": "Mesial"
            },
            {
                "code": "O",
                "definition": "The chewing surface of posterior teeth.",
                "display": "Occlusal"
            },
            {
                "code": "I",
                "definition": "The biting edge of anterior teeth.",
                "display": "Incisal"
            },
            {
                "code": "D",
