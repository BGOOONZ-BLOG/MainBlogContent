                </object>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>px</label>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <cols>5</cols>
              <rows>2</rows>
              <vgap>4</vgap>
              <hgap>4</hgap>
            </object>
          </object>
        </object>
        <flag>wxEXPAND</flag>
      </object>
    
    </object>
