@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix v2-0205: <http://hl7.org/fhir/v2/0205/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v2-0205:AP  a           owl:Class ;
        rdfs:label      "administrative price or handling fee" ;
        skos:prefLabel  "administrative price or handling fee" .

v2-0205:IC  a           owl:Class ;
        rdfs:label      "indirect unit cost" ;
        skos:prefLabel  "indirect unit cost" .

v2-0205:UP  a           owl:Class ;
        rdfs:label      "unit price, may be based on length of procedure or service" ;
        skos:prefLabel  "unit price, may be based on length of procedure or service" .

v2-0205:PF  a           owl:Class ;
        rdfs:label      "professional fee for performing provider" ;
        skos:prefLabel  "professional fee for performing provider" .

v2-0205:DC  a           owl:Class ;
        rdfs:label      "direct unit cost" ;
        skos:prefLabel  "direct unit cost" .

v2-0205:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 0205 ( Price Type)" ;
        rdfs:label       "v2 Price Type" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/0205/2.8.2" ;
        owl:versionInfo  "v2 Price Type(2.8.2)" .

v2-0205:TF  a           owl:Class ;
        rdfs:label      "technology fee for use of equipment" ;
        skos:prefLabel  "technology fee for use of equipment" .

v2-0205:TP  a           owl:Class ;
        rdfs:label      "total price" ;
        skos:prefLabel  "total price" .
