@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix metric-category: <http://hl7.org/fhir/metric-category/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

metric-category:setting
        a                owl:Class ;
        rdfs:label       "Setting" ;
        skos:definition  "DeviceObservations generated for this DeviceMetric is a setting that will influence the behavior of the Device." ;
        skos:prefLabel   "Setting" .

metric-category:unspecified
        a                owl:Class ;
        rdfs:label       "Unspecified" ;
        skos:definition  "The category of this DeviceMetric is unspecified." ;
        skos:prefLabel   "Unspecified" .

metric-category:calculation
        a                owl:Class ;
        rdfs:label       "Calculation" ;
        skos:definition  "DeviceObservations generated for this DeviceMetric are calculated." ;
        skos:prefLabel   "Calculation" .

metric-category:measurement
        a                owl:Class ;
        rdfs:label       "Measurement" ;
        skos:definition  "DeviceObservations generated for this DeviceMetric are measured." ;
        skos:prefLabel   "Measurement" .

metric-category:  a      owl:Ontology ;
        rdfs:comment     "Describes the category of the metric." ;
        rdfs:label       "DeviceMetricCategory" ;
        owl:versionIRI   "http://hl7.org/fhir/metric-category/3.1.0" ;
        owl:versionInfo  "DeviceMetricCategory(3.1.0)" .
