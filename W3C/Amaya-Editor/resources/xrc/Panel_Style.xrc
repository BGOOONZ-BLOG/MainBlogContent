                <object class="wxStaticText">
                  <label>%</label>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxCheckBox" name="wxID_SVG_STROKE_NONE">
                  <label>Stroke</label>
                  <checked>1</checked>
                </object>
              </object>
              <object class="sizeritem">
                <object class="unknown" name="wxID_SVG_STROKE_COLOR">
                  <size>16,16</size>
                </object>
                <flag>wxALIGN_CENTRE</flag>
              </object>
              <object class="sizeritem">
                <object class="wxBitmapButton" name="wxID_BUTTON_SVG_STROKE_COLOR">
