<?xml version="1.0" encoding="UTF-8"?>
<rspec type="request" 
        xmlns="http://www.geni.net/resources/rspec/3" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
        xsi:schemaLocation="http://www.geni.net/resources/rspec/3 
        http://www.geni.net/resources/rspec/3/request.xsd">

  <node client_id="ig-miss" component_manager_id="urn:publicid:IDN+instageni.rnet.missouri.edu+authority+cm" >
    <sliver_type name="emulab-xen"/>
   <services> <execute command="sudo yum install iperf -y" shell="sh"/> </services>
    <interface client_id="ig-miss:if0">
      <ip address="192.168.1.1" netmask="255.255.255.0" type="ipv4"/>
    </interface>
  </node>
  <node client_id="ig-ill" component_manager_id="urn:publicid:IDN+instageni.illinois.edu+authority+cm" >
    <sliver_type name="emulab-xen"/>
   <services> <execute command="sudo yum install iperf -y" shell="sh"/> </services>
    <interface client_id="ig-ill:if0">
      <ip address="192.168.1.2" netmask="255.255.255.0" type="ipv4"/>
    </interface>
  </node>
  <link client_id="link">
    <component_manager name="urn:publicid:IDN+instageni.rnet.missouri.edu+authority+cm"/>
    <component_manager name="urn:publicid:IDN+instageni.illinois.edu+authority+cm"/>
    <interface_ref client_id="ig-miss:if0"/>
    <interface_ref client_id="ig-ill:if0"/>
    <property capacity="100000" source_id="ig-miss:if0" dest_id="ig-ill:if0" />
    <property capacity="100000" source_id="ig-ill:if0" dest_id="ig-miss:if0" />
  </link>
</rspec>
