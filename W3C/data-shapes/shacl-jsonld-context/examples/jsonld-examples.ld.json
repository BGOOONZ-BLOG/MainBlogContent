    },
    "sourceConstraintComponent" : {
      "@type" : "@id"
    },
    "prefixes" : {
      "@type" : "@id"
    },
    "declare" : {
      "@type" : "@id"
    },
    "namespace" : {
      "@type" : "xsd:anyURI"
    },
    "shapesGraph" : {
      "@type" : "@id"
    },
    "path" : {
      "@type" : "@id"
    },
    "inversePath" : {
      "@type" : "@id"
    },
    "alternativePath" : {
      "@type" : "@id",
      "@container" : "@list"
    },
    "zeroOrMorePath" : {
      "@type" : "@id"
    },
    "oneOrMorePath" : {
      "@type" : "@id"
    },
    "zeroOrOnePath" : {
      "@type" : "@id"
    },
    "parameter" : {
      "@type" : "@id"
    },
    "validator" : {
      "@type" : "@id"
    },
    "nodeValidator" : {
      "@type" : "@id"
    },
    "propertyValidator" : {
      "@type" : "@id"
    },
    "and" : {
      "@type" : "@id",
      "@container" : "@list"
    },
    "class" : {
      "@type" : "@id"
    },
    "ignoredProperties" :  {
      "@type" : "@id",
      "@container" : "@list"
    },
    "datatype" : {
      "@type" : "@id"
    },
    "disjoint" : {
      "@type" : "@id"
