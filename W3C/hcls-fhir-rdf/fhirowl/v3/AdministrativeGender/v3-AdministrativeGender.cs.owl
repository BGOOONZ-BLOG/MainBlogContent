@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix v3-AdministrativeGender: <http://hl7.org/fhir/v3/AdministrativeGender/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-AdministrativeGender:
        a                owl:Ontology ;
        rdfs:comment     " The gender of a person used for adminstrative purposes (as opposed to clinical gender)" ;
        rdfs:label       "v3 Code System AdministrativeGender" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/AdministrativeGender/2016-11-11" ;
        owl:versionInfo  "v3 Code System AdministrativeGender(2016-11-11)" .

v3-AdministrativeGender:M
        a                owl:Class ;
        rdfs:label       "Male" ;
        skos:definition  "Male" ;
        skos:prefLabel   "Male" .

v3-AdministrativeGender:F
        a                owl:Class ;
        rdfs:label       "Female" ;
        skos:definition  "Female" ;
        skos:prefLabel   "Female" .

v3-AdministrativeGender:UN
        a                owl:Class ;
        rdfs:label       "Undifferentiated" ;
        skos:definition  "The gender of a person could not be uniquely defined as male or female, such as hermaphrodite." ;
        skos:prefLabel   "Undifferentiated" .
