@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .
@prefix sequence-type: <http://hl7.org/fhir/sequence-type/> .

sequence-type:rna  a     owl:Class ;
        rdfs:label       "RNA Sequence" ;
        skos:definition  "RNA Sequence" ;
        skos:prefLabel   "RNA Sequence" .

sequence-type:dna  a     owl:Class ;
        rdfs:label       "DNA Sequence" ;
        skos:definition  "DNA Sequence" ;
        skos:prefLabel   "DNA Sequence" .

sequence-type:aa  a      owl:Class ;
        rdfs:label       "AA Sequence" ;
        skos:definition  "Amino acid sequence" ;
        skos:prefLabel   "AA Sequence" .

sequence-type:  a        owl:Ontology ;
        rdfs:comment     "Type if a sequence -- DNA, RNA, or amino acid sequence" ;
        rdfs:label       "sequenceType" ;
        owl:versionIRI   "http://hl7.org/fhir/sequence-type/3.1.0" ;
        owl:versionInfo  "sequenceType(3.1.0)" .
