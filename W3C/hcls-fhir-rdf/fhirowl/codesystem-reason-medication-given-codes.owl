@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .
@prefix reason-medication-given-codes: <http://hl7.org/fhir/reason-medication-given/> .

reason-medication-given-codes:c
        a                owl:Class ;
        rdfs:label       "Emergency" ;
        skos:definition  "The administration was needed to treat an emergency." ;
        skos:prefLabel   "Emergency" .

reason-medication-given-codes:b
        a                owl:Class ;
        rdfs:label       "Given as Ordered" ;
        skos:definition  "The administration was following an ordered protocol." ;
        skos:prefLabel   "Given as Ordered" .

reason-medication-given-codes:
        a                owl:Ontology ;
        rdfs:comment     "This value set is provided as an example. The value set to instantiate this attribute should be drawn from a robust terminology code system that consists of or contains concepts to support the medication process." ;
        rdfs:label       "Reason Medication Given Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/reason-medication-given/3.1.0" ;
        owl:versionInfo  "Reason Medication Given Codes(3.1.0)" .

reason-medication-given-codes:a
        a                owl:Class ;
        rdfs:label       "None" ;
        skos:definition  "No reason known." ;
        skos:prefLabel   "None" .
