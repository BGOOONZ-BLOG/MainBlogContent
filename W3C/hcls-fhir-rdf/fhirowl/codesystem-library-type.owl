@prefix fhir:  <http://hl7.org/fhir/> .
@prefix library-type: <http://hl7.org/fhir/library-type/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

library-type:logic-library
        a                owl:Class ;
        rdfs:label       "Logic Library" ;
        skos:definition  "The resource is a shareable library of formalized knowledge" ;
        skos:prefLabel   "Logic Library" .

library-type:asset-collection
        a                owl:Class ;
        rdfs:label       "Asset Collection" ;
        skos:definition  "The resource is a collection of knowledge assets" ;
        skos:prefLabel   "Asset Collection" .

library-type:model-definition
        a                owl:Class ;
        rdfs:label       "Model Definition" ;
        skos:definition  "The resource is a definition of an information model" ;
        skos:prefLabel   "Model Definition" .

library-type:module-definition
        a                owl:Class ;
        rdfs:label       "Module Definition" ;
        skos:definition  "The resource defines the dependencies, parameters, and data requirements for a particular module or evaluation context" ;
        skos:prefLabel   "Module Definition" .

library-type:  a         owl:Ontology ;
        rdfs:comment     "The type of knowledge asset this library contains" ;
        rdfs:label       "LibraryType" ;
        owl:versionIRI   "http://hl7.org/fhir/library-type/3.1.0" ;
        owl:versionInfo  "LibraryType(3.1.0)" .
