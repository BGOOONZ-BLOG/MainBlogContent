<?xml version="1.0" encoding="UTF-8"?>
<resource version="2.3.0.1" xmlns="http://www.wxwindows.org/wxxrc">
  <object class="wxDialog" name="ImageDlgWX">
    <title>Create Image</title>
    <centered>1</centered>
    <style>wxDEFAULT_DIALOG_STYLE|wxRESIZE_BORDER</style>
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxStaticText" name="wxID_LABEL">
              <label>*Get the Image name*</label>
            </object>
            <option>0</option>
            <flag>wxALL|wxEXPAND</flag>
            <border>10</border>
          </object>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <orient>wxHORIZONTAL</orient>
              <object class="sizeritem">
                <object class="wxBitmapButton" name="wxID_CLEAR">
                  <tooltip>*Browse*</tooltip>
                  <bitmap stock_id="wxART_CROSS_MARK"/>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
                <border>5</border>
              </object>
              <object class="spacer">
                <size>2,0</size>
              </object>
              <object class="sizeritem">
                <object class="wxTextCtrl" name="wxID_URL">
                  <size>400, -1</size>
                  <focused>1</focused>
                </object>
                <option>1</option>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxBitmapButton" name="wxID_BROWSEBUTTON">
                  <tooltip>*Browse*</tooltip>
                  <bitmap stock_id="wxART_FILE_OPEN"/>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
                <border>5</border>
              </object>
            </object>
            <option>0</option>
            <flag>wxALL|wxEXPAND</flag>
          </object>
          <object class="spacer">
            <size>0,10</size>
          </object>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <orient>wxHORIZONTAL</orient>
              <object class="sizeritem">
                <object class="wxStaticText" name="wxID_ALT_LABEL">
                  <size>120,-1</size>
                  <label>*Get the ALT value*</label>
                </object>
                <option>1</option>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
                <border>5</border>
              </object>
              <object class="spacer">
                <size>2,0</size>
              </object>
              <object class="sizeritem">
                <object class="wxTextCtrl" name="wxID_ALT"/>
                <option>1</option>
                <flag>wxEXPAND|wxALIGN_CENTRE_VERTICAL</flag>
              </object>
            </object>
            <flag>wxALL|wxEXPAND</flag>
          </object>
          <object class="spacer">
            <size>0,10</size>
          </object>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <orient>wxHORIZONTAL</orient>
              <object class="sizeritem">
                <object class="wxStaticText" name="wxID_POSITION_LABEL">
                  <label>Position of the new created image</label>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="spacer">
                <size>5,0</size>
              </object>
              <object class="sizeritem">
                <object class="wxToolBar" name="wxID_TOOL">
                  <object class="tool" name="wxID_INLINE">
                    <bitmap>../icons/16x16/inline_image.png</bitmap>
                    <toggle>1</toggle>
                  </object>
                  <bitmapsize>16,16</bitmapsize>
                  <object class="tool" name="wxID_LEFT">
                    <bitmap>../icons/16x16/left_image.png</bitmap>
                    <toggle>1</toggle>
                  </object>
                  <object class="tool" name="wxID_CENTER">
                    <bitmap>../icons/16x16/center_image.png</bitmap>
                    <toggle>1</toggle>
                  </object>
                  <object class="tool" name="wxID_RIGHT">
                    <bitmap>../icons/16x16/right_image.png</bitmap>
                    <toggle>1</toggle>
                  </object>
                  <margins>4,4</margins>
                  <style>wxTB_FLAT</style>
                </object>
              </object>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxStaticText" name="wxID_MANDATORY">
              <label>*ALT Mandatory*</label>
              <size>250,-1</size>
              <fg>#7A1919</fg>
            </object>
            <flag>wxALL|wxEXPAND</flag>
          </object>
          <object class="spacer">
            <size>0,10</size>
          </object>
        </object>
        <flag>wxALL|wxEXPAND</flag>
        <border>5</border>
      </object>
      <object class="sizeritem">
        <flag>wxALL|wxEXPAND|wxALIGN_CENTRE_VERTICAL|wxADJUST_MINSIZE</flag>
        <border>5</border>
        <object class="wxBoxSizer">
          <orient>wxHORIZONTAL</orient>
          <object class="spacer">
            <option>1</option>
          </object>
          <object class="sizeritem">
            <flag>wxRIGHT</flag>
            <border>5</border>
            <object class="wxButton" name="wxID_OPENBUTTON">
              <label>OPENBUTTON</label>
              <default>1</default>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxButton" name="wxID_CANCEL">
              <label>CANCELBUTTON</label>
            </object>
          </object>
        </object>
      </object>
    </object>
  </object>
</resource>