@prefix v3-HtmlLinkType: <http://hl7.org/fhir/v3/HtmlLinkType/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-HtmlLinkType:start
        a                owl:Class ;
        rdfs:label       "start" ;
        skos:definition  "Refers to the first document in a collection of documents. This link type tells search engines which document is considered by the author to be the starting point of the collection." ;
        skos:prefLabel   "start" .

v3-HtmlLinkType:prev  a  owl:Class ;
        rdfs:label       "prev" ;
        skos:definition  "Refers to the previous document in an ordered series of documents. Some user agents also support the synonym \"Previous\"." ;
        skos:prefLabel   "prev" .

v3-HtmlLinkType:glossary
        a                owl:Class ;
        rdfs:label       "glossary" ;
        skos:definition  "Refers to a document providing a glossary of terms that pertain to the current document." ;
        skos:prefLabel   "glossary" .

v3-HtmlLinkType:copyright
        a                owl:Class ;
        rdfs:label       "copyright" ;
        skos:definition  "Refers to a copyright statement for the current document." ;
        skos:prefLabel   "copyright" .

v3-HtmlLinkType:contents
        a                owl:Class ;
        rdfs:label       "contents" ;
        skos:definition  "Refers to a document serving as a table of contents. Some user agents also support the synonym ToC (from \"Table of Contents\")." ;
        skos:prefLabel   "contents" .

v3-HtmlLinkType:bookmark
        a                owl:Class ;
        rdfs:label       "bookmark" ;
        skos:definition  "Refers to a bookmark. A bookmark is a link to a key entry point within an extended document. The title attribute may be used, for example, to label the bookmark. Note that several bookmarks may be defined in each document." ;
        skos:prefLabel   "bookmark" .

v3-HtmlLinkType:appendix
        a                owl:Class ;
        rdfs:label       "appendix" ;
        skos:definition  "Refers to a document serving as an appendix in a collection of documents." ;
        skos:prefLabel   "appendix" .

v3-HtmlLinkType:subsection
        a                owl:Class ;
        rdfs:label       "subsection" ;
        skos:definition  "Refers to a document serving as a subsection in a collection of documents." ;
        skos:prefLabel   "subsection" .

v3-HtmlLinkType:stylesheet
        a                owl:Class ;
        rdfs:label       "stylesheet" ;
        skos:definition  "Refers to an external style sheet. See the section on external style sheets for details. This is used together with the link type \"Alternate\" for user-selectable alternate style sheets." ;
        skos:prefLabel   "stylesheet" .

v3-HtmlLinkType:chapter
        a                owl:Class ;
        rdfs:label       "chapter" ;
        skos:definition  "Refers to a document serving as a chapter in a collection of documents." ;
        skos:prefLabel   "chapter" .

v3-HtmlLinkType:  a      owl:Ontology ;
        rdfs:comment     " HtmlLinkType values are drawn from HTML 4.0 and describe the relationship between the current document and the anchor that is the target of the link" ;
        rdfs:label       "v3 Code System HtmlLinkType" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/HtmlLinkType/2016-11-11" ;
        owl:versionInfo  "v3 Code System HtmlLinkType(2016-11-11)" .

v3-HtmlLinkType:next  a  owl:Class ;
        rdfs:label       "next" ;
        skos:definition  "Refers to the next document in a linear sequence of documents. User agents may choose to preload the \"next\" document, to reduce the perceived load time." ;
        skos:prefLabel   "next" .

v3-HtmlLinkType:help  a  owl:Class ;
        rdfs:label       "help" ;
        skos:definition  "Refers to a document offering help (more information, links to other sources of information, etc.)." ;
        skos:prefLabel   "help" .

v3-HtmlLinkType:index
        a                owl:Class ;
        rdfs:label       "index" ;
        skos:definition  "Refers to a document providing an index for the current document." ;
        skos:prefLabel   "index" .

v3-HtmlLinkType:section
        a                owl:Class ;
        rdfs:label       "section" ;
        skos:definition  "Refers to a document serving as a section in a collection of documents." ;
        skos:prefLabel   "section" .

v3-HtmlLinkType:alternate
        a                owl:Class ;
        rdfs:label       "alternate" ;
        skos:definition  "Designates substitute versions for the document in which the link occurs. When used together with the lang attribute, it implies a translated version of the document. When used together with the media attribute, it implies a version designed for a different medium (or media)." ;
        skos:prefLabel   "alternate" .
