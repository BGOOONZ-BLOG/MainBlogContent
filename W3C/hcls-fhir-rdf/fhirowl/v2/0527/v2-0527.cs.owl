@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix v2-0527: <http://hl7.org/fhir/v2/0527/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v2-0527:DY  a           owl:Class ;
        rdfs:label      "day of the year" ;
        skos:prefLabel  "day of the year" .

v2-0527:SN  a           owl:Class ;
        rdfs:label      "second of the minute" ;
        skos:prefLabel  "second of the minute" .

v2-0527:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 0527 ( Calendar Alignment)" ;
        rdfs:label       "v2 Calendar Alignment" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/0527/2.8.2" ;
        owl:versionInfo  "v2 Calendar Alignment(2.8.2)" .

v2-0527:MY  a           owl:Class ;
        rdfs:label      "month of the year" ;
        skos:prefLabel  "month of the year" .

v2-0527:DM  a           owl:Class ;
        rdfs:label      "day of the month" ;
        skos:prefLabel  "day of the month" .

v2-0527:DW  a           owl:Class ;
        rdfs:label      "day of the week (begins with Monday)" ;
        skos:prefLabel  "day of the week (begins with Monday)" .

v2-0527:WY  a           owl:Class ;
        rdfs:label      "week of the year" ;
        skos:prefLabel  "week of the year" .

v2-0527:HD  a           owl:Class ;
        rdfs:label      "hour of the day" ;
        skos:prefLabel  "hour of the day" .

v2-0527:NH  a           owl:Class ;
        rdfs:label      "minute of the hour" ;
        skos:prefLabel  "minute of the hour" .
