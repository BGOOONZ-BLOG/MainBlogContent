@prefix fhir:  <http://hl7.org/fhir/> .
@prefix vision-base-codes: <http://hl7.org/fhir/vision-base-codes/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

vision-base-codes:in  a  owl:Class ;
        rdfs:label       "In" ;
        skos:definition  "inner edge" ;
        skos:prefLabel   "In" .

vision-base-codes:out
        a                owl:Class ;
        rdfs:label       "Out" ;
        skos:definition  "outer edge" ;
        skos:prefLabel   "Out" .

vision-base-codes:down
        a                owl:Class ;
        rdfs:label       "Down" ;
        skos:definition  "bottom" ;
        skos:prefLabel   "Down" .

vision-base-codes:up  a  owl:Class ;
        rdfs:label       "Up" ;
        skos:definition  "top" ;
        skos:prefLabel   "Up" .

vision-base-codes:  a    owl:Ontology ;
        rdfs:comment     "A coded concept listing the base codes." ;
        rdfs:label       "VisionBase" ;
        owl:versionIRI   "http://hl7.org/fhir/vision-base-codes/3.1.0" ;
        owl:versionInfo  "VisionBase(3.1.0)" .
