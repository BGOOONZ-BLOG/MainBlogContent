@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v3-PersonDisabilityType: <http://hl7.org/fhir/v3/PersonDisabilityType/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-PersonDisabilityType:
        a                owl:Ontology ;
        rdfs:comment     " A code identifying a person's disability." ;
        rdfs:label       "v3 Code System PersonDisabilityType" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/PersonDisabilityType/2016-11-11" ;
        owl:versionInfo  "v3 Code System PersonDisabilityType(2016-11-11)" .

v3-PersonDisabilityType:1
        a                owl:Class ;
        rdfs:label       "Vision impaired" ;
        skos:definition  "Vision impaired" ;
        skos:prefLabel   "Vision impaired" .

v3-PersonDisabilityType:WC
        a                owl:Class ;
        rdfs:label       "Requires wheelchair" ;
        rdfs:subClassOf  v3-PersonDisabilityType:5 ;
        skos:definition  "Person requires a wheelchair to be ambulatory" ;
        skos:prefLabel   "Requires wheelchair" .

v3-PersonDisabilityType:4
        a                owl:Class ;
        rdfs:label       "Mentally impaired" ;
        skos:definition  "Mentally impaired" ;
        skos:prefLabel   "Mentally impaired" .

v3-PersonDisabilityType:WK
        a                owl:Class ;
        rdfs:label       "Requires walker" ;
        rdfs:subClassOf  v3-PersonDisabilityType:5 ;
        skos:definition  "Person requires a walker to ambulate" ;
        skos:prefLabel   "Requires walker" .

v3-PersonDisabilityType:CB
        a                owl:Class ;
        rdfs:label       "Requires crib" ;
        rdfs:subClassOf  v3-PersonDisabilityType:5 ;
        skos:definition  "A crib is required to move the person" ;
        skos:prefLabel   "Requires crib" .

v3-PersonDisabilityType:2
        a                owl:Class ;
        rdfs:label       "Hearing impaired" ;
        skos:definition  "Hearing impaired" ;
        skos:prefLabel   "Hearing impaired" .

v3-PersonDisabilityType:5
        a                owl:Class ;
        rdfs:label       "Mobility impaired" ;
        skos:definition  "Mobility impaired" ;
        skos:prefLabel   "Mobility impaired" .

v3-PersonDisabilityType:CR
        a                owl:Class ;
        rdfs:label       "Requires crutches" ;
        rdfs:subClassOf  v3-PersonDisabilityType:5 ;
        skos:definition  "Person requires crutches to ambulate" ;
        skos:prefLabel   "Requires crutches" .

v3-PersonDisabilityType:3
        a                owl:Class ;
        rdfs:label       "Speech impaired" ;
        skos:definition  "Speech impaired" ;
        skos:prefLabel   "Speech impaired" .

v3-PersonDisabilityType:G
        a                owl:Class ;
        rdfs:label       "Requires gurney" ;
        rdfs:subClassOf  v3-PersonDisabilityType:5 ;
        skos:definition  "A gurney is required to move the person" ;
        skos:prefLabel   "Requires gurney" .
