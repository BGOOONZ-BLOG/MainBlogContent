@prefix diagnosis-role: <http://hl7.org/fhir/diagnosis-role/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

diagnosis-role:  a       owl:Ontology ;
        rdfs:comment     "This value set defines a set of codes that can be used to express the role of a diagnosis on the Encounter or EpisodeOfCare record." ;
        rdfs:label       "DiagnosisRole" ;
        owl:versionIRI   "http://hl7.org/fhir/diagnosis-role/3.1.0" ;
        owl:versionInfo  "DiagnosisRole(3.1.0)" .

diagnosis-role:pre-op
        a               owl:Class ;
        rdfs:label      "pre-op diagnosis" ;
        skos:prefLabel  "pre-op diagnosis" .

diagnosis-role:DD  a    owl:Class ;
        rdfs:label      "Discharge diagnosis" ;
        skos:prefLabel  "Discharge diagnosis" .

diagnosis-role:CC  a    owl:Class ;
        rdfs:label      "Chief complaint" ;
        skos:prefLabel  "Chief complaint" .

diagnosis-role:CM  a    owl:Class ;
        rdfs:label      "Comorbidity diagnosis" ;
        skos:prefLabel  "Comorbidity diagnosis" .

diagnosis-role:AD  a    owl:Class ;
        rdfs:label      "Admission diagnosis" ;
        skos:prefLabel  "Admission diagnosis" .

diagnosis-role:post-op
        a               owl:Class ;
        rdfs:label      "post-op diagnosis" ;
        skos:prefLabel  "post-op diagnosis" .

diagnosis-role:billing
        a               owl:Class ;
        rdfs:label      "Billing" ;
        skos:prefLabel  "Billing" .
