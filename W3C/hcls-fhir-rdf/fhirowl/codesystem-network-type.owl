@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .
@prefix network-type: <http://hl7.org/fhir/network-type/> .

network-type:5  a        owl:Class ;
        rdfs:label       "URI" ;
        skos:definition  "URI (User directory, HTTP-PUT, ftp, etc.)." ;
        skos:prefLabel   "URI" .

network-type:3  a        owl:Class ;
        rdfs:label       "Telephone Number" ;
        skos:definition  "The assigned telephone number." ;
        skos:prefLabel   "Telephone Number" .

network-type:  a         owl:Ontology ;
        rdfs:comment     "The type of network access point of this agent in the audit event" ;
        rdfs:label       "AuditEventAgentNetworkType" ;
        owl:versionIRI   "http://hl7.org/fhir/network-type/3.1.0" ;
        owl:versionInfo  "AuditEventAgentNetworkType(3.1.0)" .

network-type:1  a        owl:Class ;
        rdfs:label       "Machine Name" ;
        skos:definition  "The machine name, including DNS name." ;
        skos:prefLabel   "Machine Name" .

network-type:4  a        owl:Class ;
        rdfs:label       "Email address" ;
        skos:definition  "The assigned email address." ;
        skos:prefLabel   "Email address" .

network-type:2  a        owl:Class ;
        rdfs:label       "IP Address" ;
        skos:definition  "The assigned Internet Protocol (IP) address." ;
        skos:prefLabel   "IP Address" .
