@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix implant-status: <http://hl7.org/fhir/implant-status/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

implant-status:unknown
        a                owl:Class ;
        rdfs:label       "Unknown" ;
        skos:definition  "the functional status of the implant has not been determined" ;
        skos:prefLabel   "Unknown" .

implant-status:non-functional
        a                owl:Class ;
        rdfs:label       "Non-Functional" ;
        skos:definition  "The implanted device is not working" ;
        skos:prefLabel   "Non-Functional" .

implant-status:functional
        a                owl:Class ;
        rdfs:label       "Functional" ;
        skos:definition  "The implanted device is working normally" ;
        skos:prefLabel   "Functional" .

implant-status:disabled
        a                owl:Class ;
        rdfs:label       "Disabled" ;
        skos:definition  "The implanted device has been turned off" ;
        skos:prefLabel   "Disabled" .

implant-status:  a       owl:Ontology ;
        rdfs:comment     "A set codes that define the functional status of an implanted device." ;
        rdfs:label       "Implant Status" ;
        owl:versionIRI   "http://hl7.org/fhir/implant-status/3.1.0" ;
        owl:versionInfo  "Implant Status(3.1.0)" .
