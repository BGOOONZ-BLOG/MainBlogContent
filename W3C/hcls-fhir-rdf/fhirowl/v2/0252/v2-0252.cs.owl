        skos:prefLabel  "Effect observed when patient receives placebo" .

v2-0252:BE  a           owl:Class ;
        rdfs:label      "Event recurred after product reintroduced" ;
        skos:prefLabel  "Event recurred after product reintroduced" .

v2-0252:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 0252 ( Causality Observations)" ;
        rdfs:label       "v2 Causality Observations" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/0252/2.8.2" ;
        owl:versionInfo  "v2 Causality Observations(2.8.2)" .

v2-0252:LI  a           owl:Class ;
        rdfs:label      "Literature reports association of product with event" ;
        skos:prefLabel  "Literature reports association of product with event" .
