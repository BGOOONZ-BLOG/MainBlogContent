@prefix metric-calibration-state: <http://hl7.org/fhir/metric-calibration-state/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

metric-calibration-state:
        a                owl:Ontology ;
        rdfs:comment     "Describes the state of a metric calibration." ;
        rdfs:label       "DeviceMetricCalibrationState" ;
        owl:versionIRI   "http://hl7.org/fhir/metric-calibration-state/3.1.0" ;
        owl:versionInfo  "DeviceMetricCalibrationState(3.1.0)" .

metric-calibration-state:not-calibrated
        a                owl:Class ;
        rdfs:label       "Not Calibrated" ;
        skos:definition  "The metric has not been calibrated." ;
        skos:prefLabel   "Not Calibrated" .

metric-calibration-state:calibration-required
        a                owl:Class ;
        rdfs:label       "Calibration Required" ;
        skos:definition  "The metric needs to be calibrated." ;
        skos:prefLabel   "Calibration Required" .

metric-calibration-state:calibrated
        a                owl:Class ;
        rdfs:label       "Calibrated" ;
        skos:definition  "The metric has been calibrated." ;
        skos:prefLabel   "Calibrated" .

metric-calibration-state:unspecified
        a                owl:Class ;
        rdfs:label       "Unspecified" ;
        skos:definition  "The state of calibration of this metric is unspecified." ;
        skos:prefLabel   "Unspecified" .
