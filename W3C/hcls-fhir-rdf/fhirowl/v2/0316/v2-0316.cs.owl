@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v2-0316: <http://hl7.org/fhir/v2/0316/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v2-0316:Y  a            owl:Class ;
        rdfs:label      "Yes, patient is a documented donor and documentation is on file" ;
        skos:prefLabel  "Yes, patient is a documented donor and documentation is on file" .

v2-0316:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 0316 ( Organ Donor Code)" ;
        rdfs:label       "v2 Organ Donor Code" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/0316/2.8.2" ;
        owl:versionInfo  "v2 Organ Donor Code(2.8.2)" .

v2-0316:R  a            owl:Class ;
        rdfs:label      "Patient leaves organ donation decision to relatives" ;
        skos:prefLabel  "Patient leaves organ donation decision to relatives" .

v2-0316:F  a            owl:Class ;
        rdfs:label      "Yes, patient is a documented donor, but documentation is not on file" ;
        skos:prefLabel  "Yes, patient is a documented donor, but documentation is not on file" .

v2-0316:P  a            owl:Class ;
        rdfs:label      "Patient leaves organ donation decision to a specific person" ;
        skos:prefLabel  "Patient leaves organ donation decision to a specific person" .

v2-0316:U  a            owl:Class ;
        rdfs:label      "Unknown" ;
        skos:prefLabel  "Unknown" .

v2-0316:I  a            owl:Class ;
        rdfs:label      "No, patient is not a documented donor, but information was provided" ;
        skos:prefLabel  "No, patient is not a documented donor, but information was provided" .

v2-0316:N  a            owl:Class ;
        rdfs:label      "No, patient has not agreed to be a donor" ;
        skos:prefLabel  "No, patient has not agreed to be a donor" .
