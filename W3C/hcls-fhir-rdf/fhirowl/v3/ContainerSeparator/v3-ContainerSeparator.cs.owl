@prefix v3-ContainerSeparator: <http://hl7.org/fhir/v3/ContainerSeparator/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-ContainerSeparator:NONE
        a                owl:Class ;
        rdfs:label       "None" ;
        skos:definition  "No separator material is present in the container." ;
        skos:prefLabel   "None" .

v3-ContainerSeparator:
        a                owl:Ontology ;
        rdfs:comment     " A material in a blood collection container that facilites the separation of of blood cells from serum or plasma" ;
        rdfs:label       "v3 Code System ContainerSeparator" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/ContainerSeparator/2016-11-11" ;
        owl:versionInfo  "v3 Code System ContainerSeparator(2016-11-11)" .

v3-ContainerSeparator:GEL
        a                owl:Class ;
        rdfs:label       "Gel" ;
        skos:definition  "A gelatinous type of separator material." ;
        skos:prefLabel   "Gel" .
