@prefix fhir:  <http://hl7.org/fhir/> .
@prefix extension-context: <http://hl7.org/fhir/extension-context/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

extension-context:datatype
        a                owl:Class ;
        rdfs:label       "Datatype" ;
        skos:definition  "The context is all nodes matching a particular data type element path (root or repeating element) or all elements referencing a particular primitive data type (expressed as the datatype name)." ;
        skos:prefLabel   "Datatype" .

extension-context:  a    owl:Ontology ;
        rdfs:comment     "How an extension context is interpreted." ;
        rdfs:label       "ExtensionContext" ;
        owl:versionIRI   "http://hl7.org/fhir/extension-context/3.1.0" ;
        owl:versionInfo  "ExtensionContext(3.1.0)" .

extension-context:extension
        a                owl:Class ;
        rdfs:label       "Extension" ;
        skos:definition  "The context is a particular extension from a particular profile, a uri that identifies the extension definition." ;
        skos:prefLabel   "Extension" .

extension-context:resource
        a                owl:Class ;
        rdfs:label       "Resource" ;
        skos:definition  "The context is all elements matching a particular resource element path." ;
        skos:prefLabel   "Resource" .
