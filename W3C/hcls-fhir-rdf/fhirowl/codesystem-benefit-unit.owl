@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix benefit-unit: <http://hl7.org/fhir/benefit-unit/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

benefit-unit:  a         owl:Ontology ;
        rdfs:comment     "This value set includes a smattering of Unit type codes." ;
        rdfs:label       "Unit Type Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/benefit-unit/3.1.0" ;
        owl:versionInfo  "Unit Type Codes(3.1.0)" .

benefit-unit:individual
        a                owl:Class ;
        rdfs:label       "Individual" ;
        skos:definition  "A single individual" ;
        skos:prefLabel   "Individual" .

benefit-unit:family  a   owl:Class ;
        rdfs:label       "Family" ;
        skos:definition  "A family, typically includes self, spouse(s) and children to a defined age" ;
        skos:prefLabel   "Family" .
