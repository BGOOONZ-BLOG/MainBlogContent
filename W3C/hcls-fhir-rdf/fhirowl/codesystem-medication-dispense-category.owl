@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .
@prefix medication-dispense-category: <http://hl7.org/fhir/medication-dispense-category/> .

medication-dispense-category:community
        a                owl:Class ;
        rdfs:label       "Community" ;
        skos:definition  "Includes dispenses for medications to be administered or consumed by the patient in their home (this would include long term care or nursing homes, hospices, etc)" ;
        skos:prefLabel   "Community" .

medication-dispense-category:
        a                owl:Ontology ;
        rdfs:comment     "A code describing where the dispensed medication is expected to be consumed or administered" ;
        rdfs:label       "MedicationDispenseCategory" ;
        owl:versionIRI   "http://hl7.org/fhir/medication-dispense-category/3.1.0" ;
        owl:versionInfo  "MedicationDispenseCategory(3.1.0)" .

medication-dispense-category:outpatient
        a                owl:Class ;
        rdfs:label       "Outpatient" ;
        skos:definition  "Includes dispenses for medications to be administered or consumed in an outpatient setting (for example, Emergency Department, Outpatient Clinic, Outpatient Surgery, Doctor's office)" ;
        skos:prefLabel   "Outpatient" .

medication-dispense-category:inpatient
        a                owl:Class ;
        rdfs:label       "Inpatient" ;
        skos:definition  "Includes dispenses for medications to be administered or consumed in an inpatient or acute care setting" ;
        skos:prefLabel   "Inpatient" .
