@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix http-operations: <http://hl7.org/fhir/http-operations/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

http-operations:get  a   owl:Class ;
        rdfs:label       "GET" ;
        skos:definition  "HTTP GET operation" ;
        skos:prefLabel   "GET" .

http-operations:post  a  owl:Class ;
        rdfs:label       "POST" ;
        skos:definition  "HTTP POST operation" ;
        skos:prefLabel   "POST" .

http-operations:put  a   owl:Class ;
        rdfs:label       "PUT" ;
        skos:definition  "HTTP PUT operation" ;
        skos:prefLabel   "PUT" .

http-operations:options
        a                owl:Class ;
        rdfs:label       "OPTIONS" ;
        skos:definition  "HTTP OPTIONS operation" ;
        skos:prefLabel   "OPTIONS" .

http-operations:patch
        a                owl:Class ;
        rdfs:label       "PATCH" ;
        skos:definition  "HTTP PATCH operation" ;
        skos:prefLabel   "PATCH" .

http-operations:delete
        a                owl:Class ;
        rdfs:label       "DELETE" ;
        skos:definition  "HTTP DELETE operation" ;
        skos:prefLabel   "DELETE" .

http-operations:  a      owl:Ontology ;
        rdfs:comment     "The allowable request method or HTTP operation codes." ;
        rdfs:label       "TestScriptRequestMethodCode" ;
        owl:versionIRI   "http://hl7.org/fhir/http-operations/3.1.0" ;
        owl:versionInfo  "TestScriptRequestMethodCode(3.1.0)" .
