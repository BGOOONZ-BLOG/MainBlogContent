@prefix fhir:  <http://hl7.org/fhir/> .
@prefix copy-number-event: <http://hl7.org/fhir/copy-number-event/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

copy-number-event:lof
        a                owl:Class ;
        rdfs:label       "loss of function" ;
        skos:definition  "loss of function" ;
        skos:prefLabel   "loss of function" .

copy-number-event:  a    owl:Ontology ;
        rdfs:comment     "Copy Number Event" ;
        rdfs:label       "CopyNumberEvent" ;
        owl:versionIRI   "http://hl7.org/fhir/copy-number-event/3.1.0" ;
        owl:versionInfo  "CopyNumberEvent(3.1.0)" .

copy-number-event:del
        a                owl:Class ;
        rdfs:label       "deletion" ;
        skos:definition  "deletion" ;
        skos:prefLabel   "deletion" .

copy-number-event:amp
        a                owl:Class ;
        rdfs:label       "amplificaiton" ;
        skos:definition  "amplificaiton" ;
        skos:prefLabel   "amplificaiton" .
