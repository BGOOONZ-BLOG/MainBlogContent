@prefix action-condition-kind: <http://hl7.org/fhir/action-condition-kind/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

action-condition-kind:applicability
        a                owl:Class ;
        rdfs:label       "Applicability" ;
        skos:definition  "The condition describes whether or not a given action is applicable" ;
        skos:prefLabel   "Applicability" .

action-condition-kind:start
        a                owl:Class ;
        rdfs:label       "Start" ;
        skos:definition  "The condition is a starting condition for the action" ;
        skos:prefLabel   "Start" .

action-condition-kind:stop
        a                owl:Class ;
        rdfs:label       "Stop" ;
        skos:definition  "The condition is a stop, or exit condition for the action" ;
        skos:prefLabel   "Stop" .

action-condition-kind:
        a                owl:Ontology ;
        rdfs:comment     "Defines the kinds of conditions that can appear on actions" ;
        rdfs:label       "ActionConditionKind" ;
        owl:versionIRI   "http://hl7.org/fhir/action-condition-kind/3.1.0" ;
        owl:versionInfo  "ActionConditionKind(3.1.0)" .
