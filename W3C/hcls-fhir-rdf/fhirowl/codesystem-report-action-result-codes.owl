@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix report-action-result-codes: <http://hl7.org/fhir/report-action-result-codes/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

report-action-result-codes:error
        a                owl:Class ;
        rdfs:label       "Error" ;
        skos:definition  "The action encountered a fatal error and the engine was unable to process." ;
        skos:prefLabel   "Error" .

report-action-result-codes:skip
        a                owl:Class ;
        rdfs:label       "Skip" ;
        skos:definition  "The action was skipped." ;
        skos:prefLabel   "Skip" .

report-action-result-codes:warning
        a                owl:Class ;
        rdfs:label       "Warning" ;
        skos:definition  "The action passed but with warnings." ;
        skos:prefLabel   "Warning" .

report-action-result-codes:fail
        a                owl:Class ;
        rdfs:label       "Fail" ;
        skos:definition  "The action failed." ;
        skos:prefLabel   "Fail" .

report-action-result-codes:pass
        a                owl:Class ;
        rdfs:label       "Pass" ;
        skos:definition  "The action was successful." ;
        skos:prefLabel   "Pass" .

report-action-result-codes:
        a                owl:Ontology ;
        rdfs:comment     "The results of executing an action." ;
        rdfs:label       "TestReportActionResult" ;
        owl:versionIRI   "http://hl7.org/fhir/report-action-result-codes/3.1.0" ;
        owl:versionInfo  "TestReportActionResult(3.1.0)" .
