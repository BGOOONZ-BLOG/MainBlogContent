@prefix participantrequired: <http://hl7.org/fhir/participantrequired/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

participantrequired:information-only
        a                owl:Class ;
        rdfs:label       "Information Only" ;
        skos:definition  "The participant is excluded from the appointment, and may not be informed of the appointment taking place. (Appointment is about them, not for them - such as 2 doctors discussing results about a patient's test)." ;
        skos:prefLabel   "Information Only" .

participantrequired:  a  owl:Ontology ;
        rdfs:comment     "Is the Participant required to attend the appointment." ;
        rdfs:label       "ParticipantRequired" ;
        owl:versionIRI   "http://hl7.org/fhir/participantrequired/3.1.0" ;
        owl:versionInfo  "ParticipantRequired(3.1.0)" .

participantrequired:required
        a                owl:Class ;
        rdfs:label       "Required" ;
        skos:definition  "The participant is required to attend the appointment." ;
        skos:prefLabel   "Required" .

participantrequired:optional
        a                owl:Class ;
        rdfs:label       "Optional" ;
        skos:definition  "The participant may optionally attend the appointment." ;
        skos:prefLabel   "Optional" .
