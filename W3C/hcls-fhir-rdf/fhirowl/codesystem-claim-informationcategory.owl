@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix claim-informationcategory: <http://hl7.org/fhir/claiminformationcategory/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

claim-informationcategory:related
        a                owl:Class ;
        rdfs:label       "Related Services" ;
        skos:definition  "Nature and date of the related event eg. Last exam, service, Xray etc." ;
        skos:prefLabel   "Related Services" .

claim-informationcategory:onset
        a                owl:Class ;
        rdfs:label       "Onset" ;
        skos:definition  "Period, start or end dates of aspects of the Condition." ;
        skos:prefLabel   "Onset" .

claim-informationcategory:material
        a                owl:Class ;
        rdfs:label       "Materials Forwarded" ;
        skos:definition  "Materials being forwarded, eg. Models, molds, images, documents." ;
        skos:prefLabel   "Materials Forwarded" .

claim-informationcategory:prosthesis
        a                owl:Class ;
        rdfs:label       "Prosthesis" ;
        skos:definition  "The type of prosthesis and date of supply if a previously supplied prosthesis." ;
        skos:prefLabel   "Prosthesis" .

claim-informationcategory:info
        a                owl:Class ;
        rdfs:label       "Information" ;
        skos:definition  "Codes conveying additional situation and condition information." ;
        skos:prefLabel   "Information" .

claim-informationcategory:
        a                owl:Ontology ;
        rdfs:comment     "This value set includes sample Information Category codes." ;
        rdfs:label       "Claim Information Category Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/claiminformationcategory/3.1.0" ;
        owl:versionInfo  "Claim Information Category Codes(3.1.0)" .

claim-informationcategory:other
        a                owl:Class ;
        rdfs:label       "Other" ;
        skos:definition  "Other information identified by the type.system." ;
        skos:prefLabel   "Other" .

claim-informationcategory:discharge
        a                owl:Class ;
        rdfs:label       "Discharge" ;
        skos:definition  "Discharge status and discharge to locations." ;
        skos:prefLabel   "Discharge" .

claim-informationcategory:missingtooth
        a                owl:Class ;
        rdfs:label       "Missing Tooth" ;
        skos:definition  "Teeth which are missing for any reason, for example: prior extraction, never developed." ;
        skos:prefLabel   "Missing Tooth" .

claim-informationcategory:exception
        a                owl:Class ;
        rdfs:label       "Exception" ;
        skos:definition  "Insurance policy exceptions." ;
        skos:prefLabel   "Exception" .

claim-informationcategory:attachment
        a                owl:Class ;
        rdfs:label       "Attachment" ;
        skos:definition  "Materials attached such as images, documents and resources." ;
        skos:prefLabel   "Attachment" .
