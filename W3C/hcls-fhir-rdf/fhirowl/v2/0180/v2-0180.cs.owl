@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix v2-0180: <http://hl7.org/fhir/v2/0180/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v2-0180:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 0180 ( Record-level Event Code)" ;
        rdfs:label       "v2 Record-level Event Code" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/0180/2.8.2" ;
        owl:versionInfo  "v2 Record-level Event Code(2.8.2)" .

v2-0180:MDL  a          owl:Class ;
        rdfs:label      "Delete record from master file" ;
        skos:prefLabel  "Delete record from master file" .

v2-0180:MAD  a          owl:Class ;
        rdfs:label      "Add record to master file" ;
        skos:prefLabel  "Add record to master file" .

v2-0180:MUP  a          owl:Class ;
        rdfs:label      "Update record for master file" ;
        skos:prefLabel  "Update record for master file" .

v2-0180:MDC  a          owl:Class ;
        rdfs:label      "Deactivate: discontinue using record in master file, but do not delete from database" ;
        skos:prefLabel  "Deactivate: discontinue using record in master file, but do not delete from database" .

v2-0180:MAC  a          owl:Class ;
        rdfs:label      "Reactivate deactivated record" ;
        skos:prefLabel  "Reactivate deactivated record" .
