@prefix allergy-intolerance-criticality: <http://hl7.org/fhir/allergy-intolerance-criticality/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

allergy-intolerance-criticality:low
        a                owl:Class ;
        rdfs:label       "Low Risk" ;
        skos:definition  "Worst case result of a future exposure is not assessed to be life-threatening or having high potential for organ system failure." ;
        skos:prefLabel   "Low Risk" .

allergy-intolerance-criticality:high
        a                owl:Class ;
        rdfs:label       "High Risk" ;
        skos:definition  "Worst case result of a future exposure is assessed to be life-threatening or having high potential for organ system failure." ;
        skos:prefLabel   "High Risk" .

allergy-intolerance-criticality:
        a                owl:Ontology ;
        rdfs:comment     "Estimate of the potential clinical harm, or seriousness, of a reaction to an identified substance." ;
        rdfs:label       "AllergyIntoleranceCriticality" ;
        owl:versionIRI   "http://hl7.org/fhir/allergy-intolerance-criticality/3.1.0" ;
        owl:versionInfo  "AllergyIntoleranceCriticality(3.1.0)" .

allergy-intolerance-criticality:unable-to-assess
        a                owl:Class ;
        rdfs:label       "Unable to Assess Risk" ;
        skos:definition  "Unable to assess the worst case result of a future exposure." ;
        skos:prefLabel   "Unable to Assess Risk" .
