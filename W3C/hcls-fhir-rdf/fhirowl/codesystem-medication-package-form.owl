@prefix medication-package-form: <http://hl7.org/fhir/medication-package-form/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

medication-package-form:vial
        a                owl:Class ;
        rdfs:label       "Vial" ;
        skos:definition  "A small container, typically cylindrical and made of glass, used especially for holding liquid medications." ;
        skos:prefLabel   "Vial" .

medication-package-form:box
        a                owl:Class ;
        rdfs:label       "Box" ;
        skos:definition  "A container with a flat base and sides, typically square or rectangular and having a lid." ;
        skos:prefLabel   "Box" .

medication-package-form:tube
        a                owl:Class ;
        rdfs:label       "Tube" ;
        skos:definition  "A long, hollow cylinder of metal, plastic, glass, etc., for holding medications, typically creams or ointments" ;
        skos:prefLabel   "Tube" .

medication-package-form:bottle
        a                owl:Class ;
        rdfs:label       "Bottle" ;
        skos:definition  "A container, typically made of glass or plastic and with a narrow neck, used for storing liquids." ;
        skos:prefLabel   "Bottle" .

medication-package-form:container
        a                owl:Class ;
        rdfs:label       "Container" ;
        skos:definition  "A package intended to store pharmaceuticals." ;
        skos:prefLabel   "Container" .

medication-package-form:cartridge
        a                owl:Class ;
        rdfs:label       "Cartridge" ;
        skos:definition  "A device of various configuration and composition used with a syringe for the application of anesthetic or other materials to a patient." ;
        skos:prefLabel   "Cartridge" .

medication-package-form:
        a                owl:Ontology ;
        rdfs:comment     "A coded concept defining the kind of container a medication package is packaged in" ;
        rdfs:label       "MedicationContainer" ;
        owl:versionIRI   "http://hl7.org/fhir/medication-package-form/3.1.0" ;
        owl:versionInfo  "MedicationContainer(3.1.0)" .

medication-package-form:unitdose
        a                owl:Class ;
        rdfs:label       "Unit Dose Blister" ;
        skos:definition  "A dose of medicine prepared in an individual package for convenience, safety or monitoring." ;
        skos:prefLabel   "Unit Dose Blister" .

medication-package-form:ampoule
        a                owl:Class ;
        rdfs:label       "Ampoule" ;
        skos:definition  "A sealed glass capsule containing a liquid" ;
        skos:prefLabel   "Ampoule" .
