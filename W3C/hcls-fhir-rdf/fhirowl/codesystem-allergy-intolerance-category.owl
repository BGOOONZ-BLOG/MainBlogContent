@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix allergy-intolerance-category: <http://hl7.org/fhir/allergy-intolerance-category/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

allergy-intolerance-category:
        a                owl:Ontology ;
        rdfs:comment     "Category of an identified substance." ;
        rdfs:label       "AllergyIntoleranceCategory" ;
        owl:versionIRI   "http://hl7.org/fhir/allergy-intolerance-category/3.1.0" ;
        owl:versionInfo  "AllergyIntoleranceCategory(3.1.0)" .

allergy-intolerance-category:food
        a                owl:Class ;
        rdfs:label       "Food" ;
        skos:definition  "Any substance consumed to provide nutritional support for the body." ;
        skos:prefLabel   "Food" .

allergy-intolerance-category:environment
        a                owl:Class ;
        rdfs:label       "Environment" ;
        skos:definition  "Any substances that are encountered in the environment, including any substance not already classified as food, medication, or biologic." ;
        skos:prefLabel   "Environment" .

allergy-intolerance-category:medication
        a                owl:Class ;
        rdfs:label       "Medication" ;
        skos:definition  "Substances administered to achieve a physiological effect." ;
        skos:prefLabel   "Medication" .

allergy-intolerance-category:biologic
        a                owl:Class ;
        rdfs:label       "Biologic" ;
        skos:definition  "A preparation that is synthesized from living organisms or their products, especially a human or animal protein, such as a hormone or antitoxin, that is used as a diagnostic, preventive, or therapeutic agent. Examples of biologic medications include: vaccines; allergenic extracts, which are used for both diagnosis and treatment (for example, allergy shots); gene therapies; cellular therapies.  There are other biologic products, such as tissues, that are not typically associated with allergies." ;
        skos:prefLabel   "Biologic" .
