@prefix map-target-list-mode: <http://hl7.org/fhir/map-target-list-mode/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

map-target-list-mode:share
        a                owl:Class ;
        rdfs:label       "Share" ;
        skos:definition  "the target instance is shared with the target instances generated by another rule (up to the first common n items, then create new ones)" ;
        skos:prefLabel   "Share" .

map-target-list-mode:collate
        a                owl:Class ;
        rdfs:label       "Collate" ;
        skos:definition  "re-use the first item in the list, and keep adding content to it" ;
        skos:prefLabel   "Collate" .

map-target-list-mode:
        a                owl:Ontology ;
        rdfs:comment     "If field is a list, how to manage the production" ;
        rdfs:label       "StructureMapTargetListMode" ;
        owl:versionIRI   "http://hl7.org/fhir/map-target-list-mode/3.1.0" ;
        owl:versionInfo  "StructureMapTargetListMode(3.1.0)" .

map-target-list-mode:last
        a                owl:Class ;
        rdfs:label       "Last" ;
        skos:definition  "when the target list is being assembled, the items for this rule go last. If more that one rule defines a last item (for a given instance of mapping) then this is an error" ;
        skos:prefLabel   "Last" .

map-target-list-mode:first
        a                owl:Class ;
        rdfs:label       "First" ;
        skos:definition  "when the target list is being assembled, the items for this rule go first. If more that one rule defines a first item (for a given instance of mapping) then this is an error" ;
        skos:prefLabel   "First" .
