{
  "@vocab": "http://www.w3.org/ns/csvm",
  "xsd": "http://www.w3.org/2001/XMLSchema#",

  "table-direction": {
    "@id": "table-direction",
    "@type": "@id"
  },
  "rtl": { "@id": "rtl" },
  "ltr": { "@id": "ltr" },
  "default": { "@id": "default" },

  "schema": {
    "@id": "schema",
    "@type": "@id"
  },
  "notes": {
    "@id": "notes",
    "@type": "@id"
  },

  // RDFS Properties
  "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
  "label": { "@id": "rdfs:label", "@container": "@language" },
  "comment": { "@id": "rdfs:comment", "@container": "@language" },

  // Dublin Core Terms
  "dct": "http://purl.org/dc/terms/",
  "abstract": { "@id": "dct:abstract", "@container": "@language" }, 
  "accessRights": { "@id": "dct:accessRights", "@type": "@id" },
  "accrualMethod": { "@id": "dct:accrualMethod", "@type": "@id" }, 
  "accrualPeriodicity": { "@id": "dct:accrualPeriodicity", "@type": "@id" }, 
  "accrualPolicy": { "@id": "dct:accrualPolicy", "@type": "@id" }, 
  "alternative": { "@id": "dct:alternative", "@container": "@language" }, 
  "audience": { "@id": "dct:audience", "@type": "@id" }, 
  "available": { "@id": "dct:available", "@type": "xsd:date" },
  "bibliographicCitation": { "@id": "dct:bibliographicCitation", "@container": "@language" },
  "conformsTo": { "@id": "dct:conformsTo", "@type": "@id" },
  "contributor": { "@id": "dct:contributor", "@type": "@id" },
  "coverage": { "@id": "dct:coverage", "@type": "@id" },
  "created": { "@id": "dct:created", "@type": "xsd:date" },
  "creator": { "@id": "dct:creator", "@type": "@id" },
  "date": { "@id": "dct:date", "@type": "xsd:date" },
  "dateAccepted": { "@id": "dct:dateAccepted", "@type": "xsd:date" },
  "dateCopyrighted": { "@id": "dct:dateCopyrighted", "@type": "xsd:date" },
  "dateSubmitted": { "@id": "dct:dateSubmitted", "@type": "xsd:date" },
  "description": { "@id": "dct:description", "@container": "@language" },
  "educationLevel": { "@id": "dct:educationLevel", "@type": "@id" },
  "extent": { "@id": "dct:extent", "@type": "@id" },
  "format": { "@id": "dct:format", "@type": "@id" },
  "hasFormat": { "@id": "dct:hasFormat", "@type": "@id" },
  "hasPart": { "@id": "dct:hasPart", "@type": "@id" }, 
  "hasVersion": { "@id": "dct:hasVersion", "@type": "@id" },
  "identifier": { "@id": "dct:identifier", "@type": "xsd:anyURI" },
  "instructionalMethod": { "@id": "dct:instructionalMethod", "@type": "@id" },
  "isFormatOf": { "@id": "dct:isFormatOf", "@type": "@id" },
  "isPartOf": { "@id": "dct:isPartOf", "@type": "@id" },
  "isReferencedBy": { "@id": "dct:isReferencedBy", "@type": "@id" },
  "isReplacedBy": { "@id": "dct:isReplacedBy", "@type": "@id" },
  "isRequiredBy": { "@id": "dct:isRequiredBy", "@type": "@id" },
  "issued": { "@id": "dct:issued", "@type": "xsd:date" },
  "isVersionOf": { "@id": "dct:isVersionOf", "@type": "@id" },
  "language": { "@id": "dct:language", "@type": "xsd:language" }, // should be @type=@id according to DCT
  "license": { "@id": "dct:license", "@type": "@id" },
  "mediator": { "@id": "dct:mediator", "@type": "@id" },
  "medium": { "@id": "dct:medium", "@type": "@id" },
  "modified": { "@id": "dct:modified", "@type": "xsd:date" },
  "provenance": { "@id": "dct:provenance", "@type": "@id" },
  "publisher": { "@id": "dct:publisher", "@type": "@id" },
  "references": { "@id": "dct:references", "@type": "@id" },
  "relation": { "@id": "dct:relation", "@type": "@id" },
  "replaces": { "@id": "dct:replaces", "@type": "@id" },
  "requires": { "@id": "dct:requires", "@type": "@id" },
  "rights": { "@id": "dct:rights", "@type": "@id" },
  "rightsHolder": { "@id": "dct:rightsHolder", "@type": "@id" },
  "source": { "@id": "dct:source", "@type": "@id" },
  "spatial": { "@id": "dct:spatial", "@type": "@id" },
  "subject": { "@id": "dct:subject", "@type": "@id" },
  "tableOfContents": { "@id": "dct:tableOfContents", "@container": "@language" },
  "temporal": { "@id": "dct:temporal", "@type": "@id" },
  "title": { "@id": "dct:title", "@container": "@language" },
  "type": { "@id": "dct:type", "@type": "@id" },
  "valid": { "@id": "dct:valid", "@type": "xsd:date" }
}
