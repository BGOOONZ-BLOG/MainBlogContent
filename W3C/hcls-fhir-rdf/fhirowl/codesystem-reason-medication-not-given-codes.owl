@prefix reason-medication-not-given-codes: <http://hl7.org/fhir/reason-medication-not-given/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

reason-medication-not-given-codes:
        a                owl:Ontology ;
        rdfs:comment     "This value set includes all medication refused, medication not administered, and non administration of necessary drug or medicine codes from SNOMED CT - provided as an exemplar value set." ;
        rdfs:label       "SNOMED CT Reason Medication Not Given Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/reason-medication-not-given/3.1.0" ;
        owl:versionInfo  "SNOMED CT Reason Medication Not Given Codes(3.1.0)" .

reason-medication-not-given-codes:b
        a                owl:Class ;
        rdfs:label       "Away" ;
        skos:definition  "The patient was not available when the dose was scheduled." ;
        skos:prefLabel   "Away" .

reason-medication-not-given-codes:c
        a                owl:Class ;
        rdfs:label       "Asleep" ;
        skos:definition  "The patient was asleep when the dose was scheduled." ;
        skos:prefLabel   "Asleep" .

reason-medication-not-given-codes:a
        a                owl:Class ;
        rdfs:label       "None" ;
        skos:definition  "No reason known." ;
        skos:prefLabel   "None" .

reason-medication-not-given-codes:d
        a                owl:Class ;
        rdfs:label       "Vomit" ;
        skos:definition  "The patient was given the medication and immediately vomited it back." ;
        skos:prefLabel   "Vomit" .
