@prefix fhir:  <http://hl7.org/fhir/> .
@prefix ex-paymenttype: <http://hl7.org/fhir/ex-paymenttype/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

ex-paymenttype:partial
        a                owl:Class ;
        rdfs:label       "Partial" ;
        skos:definition  "Partial payment of the benefit under the Claim less any adjustments." ;
        skos:prefLabel   "Partial" .

ex-paymenttype:complete
        a                owl:Class ;
        rdfs:label       "Complete" ;
        skos:definition  "Complete (final) payment of the benefit under the Claim less any adjustments." ;
        skos:prefLabel   "Complete" .

ex-paymenttype:  a       owl:Ontology ;
        rdfs:comment     "This value set includes example Payment Type codes." ;
        rdfs:label       "Example Payment Type Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/ex-paymenttype/3.1.0" ;
        owl:versionInfo  "Example Payment Type Codes(3.1.0)" .
