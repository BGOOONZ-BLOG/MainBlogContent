@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix data-types: <http://hl7.org/fhir/data-types/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

data-types:ElementDefinition
        a                owl:Class ;
        rdfs:label       "ElementDefinition" ;
        skos:definition  "Captures constraints on each element within the resource, profile, or extension." ;
        skos:prefLabel   "ElementDefinition" .

data-types:string  a     owl:Class ;
        rdfs:label       "string" ;
        skos:definition  "A sequence of Unicode characters" ;
        skos:prefLabel   "string" .

data-types:Signature  a  owl:Class ;
        rdfs:label       "Signature" ;
        skos:definition  "A digital signature along with supporting context. The signature may be electronic/cryptographic in nature, or a graphical image representing a hand-written signature, or a signature process. Different signature approaches have different utilities." ;
        skos:prefLabel   "Signature" .

data-types:Meta  a       owl:Class ;
        rdfs:label       "Meta" ;
        skos:definition  "The metadata about a resource. This is content in the resource that is maintained by the infrastructure. Changes to the content may not always be associated with version changes to the resource." ;
        skos:prefLabel   "Meta" .

data-types:uri  a        owl:Class ;
        rdfs:label       "uri" ;
        skos:definition  "String of characters used to identify a name or a resource" ;
        skos:prefLabel   "uri" .

data-types:Identifier
        a                owl:Class ;
        rdfs:label       "Identifier" ;
        skos:definition  "A technical identifier - identifies some entity uniquely and unambiguously." ;
        skos:prefLabel   "Identifier" .

data-types:Timing  a     owl:Class ;
        rdfs:label       "Timing" ;
        skos:definition  "Specifies an event that may occur multiple times. Timing schedules are used to record when things are planned, expected or requested to occur. The most common usage is in dosage instructions for medications. They are also used when planning care of various kinds, and may be used for reporting the schedule to which past regular activities were carried out." ;
        skos:prefLabel   "Timing" .

data-types:Distance  a   owl:Class ;
        rdfs:label       "Distance" ;
        skos:definition  "A length - a value with a unit that is a physical distance." ;
        skos:prefLabel   "Distance" .

data-types:Address  a    owl:Class ;
        rdfs:label       "Address" ;
        skos:definition  "An address expressed using postal conventions (as opposed to GPS or other location definition formats).  This data type may be used to convey addresses for use in delivering mail as well as for visiting locations which might not be valid for mail delivery.  There are a variety of postal address formats defined around the world." ;
        skos:prefLabel   "Address" .

data-types:integer  a    owl:Class ;
        rdfs:label       "integer" ;
        skos:definition  "A whole number" ;
        skos:prefLabel   "integer" .

data-types:dateTime  a   owl:Class ;
        rdfs:label       "dateTime" ;
        skos:definition  "A date, date-time or partial date (e.g. just year or year + month).  If hours and minutes are specified, a time zone SHALL be populated. The format is a union of the schema types gYear, gYearMonth, date and dateTime. Seconds must be provided due to schema type constraints but may be zero-filled and may be ignored.                 Dates SHALL be valid dates." ;
        skos:prefLabel   "dateTime" .

data-types:oid  a        owl:Class ;
        rdfs:label       "oid" ;
        skos:definition  "An OID represented as a URI" ;
        skos:prefLabel   "oid" .

data-types:Period  a     owl:Class ;
        rdfs:label       "Period" ;
        skos:definition  "A time period defined by a start and end date and optionally time." ;
        skos:prefLabel   "Period" .

data-types:Extension  a  owl:Class ;
        rdfs:label       "Extension" ;
        skos:definition  "Optional Extension Element - found in all resources." ;
        skos:prefLabel   "Extension" .

data-types:positiveInt
        a                owl:Class ;
        rdfs:label       "positiveInt" ;
        skos:definition  "An integer with a value that is positive (e.g. >0)" ;
        skos:prefLabel   "positiveInt" .

data-types:Money  a      owl:Class ;
        rdfs:label       "Money" ;
        skos:definition  "An amount of economic utility in some recognized currency." ;
        skos:prefLabel   "Money" .

data-types:uuid  a       owl:Class ;
        rdfs:label       "uuid" ;
        skos:definition  "A UUID, represented as a URI" ;
        skos:prefLabel   "uuid" .

data-types:Annotation
        a                owl:Class ;
        rdfs:label       "Annotation" ;
        skos:definition  "A  text note which also  contains information about who made the statement and when." ;
        skos:prefLabel   "Annotation" .

data-types:code  a       owl:Class ;
        rdfs:label       "code" ;
        skos:definition  "A string which has at least one character and no leading or trailing whitespace and where there is no whitespace other than single spaces in the contents" ;
        skos:prefLabel   "code" .

data-types:Range  a      owl:Class ;
        rdfs:label       "Range" ;
        skos:definition  "A set of ordered Quantities defined by a low and high limit." ;
        skos:prefLabel   "Range" .

data-types:ContactPoint
        a                owl:Class ;
        rdfs:label       "ContactPoint" ;
        skos:definition  "Details for all kinds of technology mediated contact points for a person or organization, including telephone, email, etc." ;
        skos:prefLabel   "ContactPoint" .

data-types:markdown  a   owl:Class ;
        rdfs:label       "markdown" ;
        skos:definition  "A string that may contain markdown syntax for optional processing by a mark down presentation engine" ;
        skos:prefLabel   "markdown" .

data-types:Attachment
        a                owl:Class ;
        rdfs:label       "Attachment" ;
        skos:definition  "For referring to data content defined in other formats." ;
        skos:prefLabel   "Attachment" .

data-types:Ratio  a      owl:Class ;
        rdfs:label       "Ratio" ;
        skos:definition  "A relationship of two Quantity values - expressed as a numerator and a denominator." ;
        skos:prefLabel   "Ratio" .

data-types:id  a         owl:Class ;
        rdfs:label       "id" ;
        skos:definition  "Any combination of letters, numerals, \"-\" and \".\", with a length limit of 64 characters.  (This might be an integer, an unprefixed OID, UUID or any other identifier pattern that meets these constraints.)  Ids are case-insensitive." ;
        skos:prefLabel   "id" .

data-types:date  a       owl:Class ;
        rdfs:label       "date" ;
        skos:definition  "A date or partial date (e.g. just year or year + month). There is no time zone. The format is a union of the schema types gYear, gYearMonth and date.  Dates SHALL be valid dates." ;
        skos:prefLabel   "date" .

data-types:SampledData
        a                owl:Class ;
        rdfs:label       "SampledData" ;
        skos:definition  "A series of measurements taken by a device, with upper and lower limits. There may be more than one dimension in the data." ;
        skos:prefLabel   "SampledData" .

data-types:instant  a    owl:Class ;
        rdfs:label       "instant" ;
        skos:definition  "An instant in time - known at least to the second" ;
        skos:prefLabel   "instant" .

data-types:BackboneElement
        a                owl:Class ;
        rdfs:label       "BackboneElement" ;
        skos:definition  "Base definition for all elements that are defined inside a resource - but not those in a data type." ;
        skos:prefLabel   "BackboneElement" .

data-types:Quantity  a   owl:Class ;
        rdfs:label       "Quantity" ;
        skos:definition  "A measured amount (or an amount that can potentially be measured). Note that measured amounts include amounts that are not precisely quantified, including amounts involving arbitrary units and floating currencies." ;
        skos:prefLabel   "Quantity" .

data-types:base64Binary
        a                owl:Class ;
        rdfs:label       "base64Binary" ;
        skos:definition  "A stream of bytes" ;
        skos:prefLabel   "base64Binary" .

data-types:time  a       owl:Class ;
        rdfs:label       "time" ;
        skos:definition  "A time during the day, with no date specified" ;
        skos:prefLabel   "time" .

data-types:Coding  a     owl:Class ;
        rdfs:label       "Coding" ;
        skos:definition  "A reference to a code defined by a terminology system." ;
        skos:prefLabel   "Coding" .

data-types:DataRequirement
        a                owl:Class ;
        rdfs:label       "DataRequirement" ;
        skos:definition  "Describes a required data item for evaluation in terms of the type of data, and optional code or date-based filters of the data." ;
        skos:prefLabel   "DataRequirement" .

data-types:Age  a        owl:Class ;
        rdfs:label       "Age" ;
        skos:definition  "A duration of time during which an organism (or a process) has existed." ;
        skos:prefLabel   "Age" .

data-types:SimpleQuantity
        a               owl:Class ;
        rdfs:label      "SimpleQuantity" ;
        skos:prefLabel  "SimpleQuantity" .

data-types:RelatedArtifact
        a                owl:Class ;
        rdfs:label       "RelatedArtifact" ;
        skos:definition  "Related artifacts such as additional documentation, justification, or bibliographic references." ;
        skos:prefLabel   "RelatedArtifact" .

data-types:xhtml  a      owl:Class ;
        rdfs:label       "XHTML" ;
        skos:definition  "XHTML format, as defined by W3C, but restricted usage (mainly, no active content)" ;
        skos:prefLabel   "XHTML" .

data-types:TriggerDefinition
        a                owl:Class ;
        rdfs:label       "TriggerDefinition" ;
        skos:definition  "A description of a triggering event." ;
        skos:prefLabel   "TriggerDefinition" .

data-types:boolean  a    owl:Class ;
        rdfs:label       "boolean" ;
        skos:definition  "Value of \"true\" or \"false\"" ;
        skos:prefLabel   "boolean" .

data-types:Element  a    owl:Class ;
        rdfs:label       "Element" ;
        skos:definition  "Base definition for all elements in a resource." ;
        skos:prefLabel   "Element" .

data-types:Contributor
        a                owl:Class ;
        rdfs:label       "Contributor" ;
        skos:definition  "A contributor to the content of a knowledge asset, including authors, editors, reviewers, and endorsers." ;
        skos:prefLabel   "Contributor" .

data-types:ContactDetail
        a                owl:Class ;
        rdfs:label       "ContactDetail" ;
        skos:definition  "Specifies contact information for a person or organization." ;
        skos:prefLabel   "ContactDetail" .

data-types:  a           owl:Ontology ;
        rdfs:comment     "The type of an element - one of the FHIR data types." ;
        rdfs:label       "DataType" ;
        owl:versionIRI   "http://hl7.org/fhir/data-types/3.1.0" ;
        owl:versionInfo  "DataType(3.1.0)" .

data-types:UsageContext
        a                owl:Class ;
        rdfs:label       "UsageContext" ;
        skos:definition  "Specifies clinical/business/etc metadata that can be used to retrieve, index and/or categorize an artifact. This metadata can either be specific to the applicable population (e.g., age category, DRG) or the specific context of care (e.g., venue, care setting, provider of care)." ;
        skos:prefLabel   "UsageContext" .

data-types:decimal  a    owl:Class ;
        rdfs:label       "decimal" ;
        skos:definition  "A rational number with implicit precision" ;
        skos:prefLabel   "decimal" .

data-types:Count  a      owl:Class ;
        rdfs:label       "Count" ;
        skos:definition  "A measured amount (or an amount that can potentially be measured). Note that measured amounts include amounts that are not precisely quantified, including amounts involving arbitrary units and floating currencies." ;
        skos:prefLabel   "Count" .

data-types:Narrative  a  owl:Class ;
        rdfs:label       "Narrative" ;
        skos:definition  "A human-readable formatted text, including images." ;
        skos:prefLabel   "Narrative" .

data-types:HumanName  a  owl:Class ;
        rdfs:label       "HumanName" ;
        skos:definition  "A human's name with the ability to identify parts and usage." ;
        skos:prefLabel   "HumanName" .

data-types:unsignedInt
        a                owl:Class ;
        rdfs:label       "unsignedInt" ;
        skos:definition  "An integer with a value that is not negative (e.g. >= 0)" ;
        skos:prefLabel   "unsignedInt" .

data-types:Duration  a   owl:Class ;
        rdfs:label       "Duration" ;
        skos:definition  "A length of time." ;
        skos:prefLabel   "Duration" .

data-types:Dosage  a     owl:Class ;
        rdfs:label       "Dosage" ;
        skos:definition  "Indicates how the medication is/was taken or should be taken by the patient." ;
        skos:prefLabel   "Dosage" .

data-types:Reference  a  owl:Class ;
        rdfs:label       "Reference" ;
        skos:definition  "A reference from one resource to another." ;
        skos:prefLabel   "Reference" .

data-types:ParameterDefinition
        a                owl:Class ;
        rdfs:label       "ParameterDefinition" ;
        skos:definition  "The parameters to the module. This collection specifies both the input and output parameters. Input parameters are provided by the caller as part of the $evaluate operation. Output parameters are included in the GuidanceResponse." ;
        skos:prefLabel   "ParameterDefinition" .

data-types:CodeableConcept
        a                owl:Class ;
        rdfs:label       "CodeableConcept" ;
        skos:definition  "A concept that may be defined by a formal reference to a terminology or ontology or may be provided by text." ;
        skos:prefLabel   "CodeableConcept" .
