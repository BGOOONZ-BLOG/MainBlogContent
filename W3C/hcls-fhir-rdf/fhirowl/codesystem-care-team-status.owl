@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix care-team-status: <http://hl7.org/fhir/care-team-status/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

care-team-status:entered-in-error
        a                owl:Class ;
        rdfs:label       "Entered In Error" ;
        skos:definition  "The care team should have never existed." ;
        skos:prefLabel   "Entered In Error" .

care-team-status:inactive
        a                owl:Class ;
        rdfs:label       "Inactive" ;
        skos:definition  "The care team was, but is no longer, participating in the coordination and delivery of care." ;
        skos:prefLabel   "Inactive" .

care-team-status:  a     owl:Ontology ;
        rdfs:comment     "Indicates the status of the care team." ;
        rdfs:label       "CareTeamStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/care-team-status/3.1.0" ;
        owl:versionInfo  "CareTeamStatus(3.1.0)" .

care-team-status:proposed
        a                owl:Class ;
        rdfs:label       "Proposed" ;
        skos:definition  "The care team has been drafted and proposed, but not yet participating in the coordination and delivery of care." ;
        skos:prefLabel   "Proposed" .

care-team-status:active
        a                owl:Class ;
        rdfs:label       "Active" ;
        skos:definition  "The care team is currently participating in the coordination and delivery of care." ;
        skos:prefLabel   "Active" .

care-team-status:suspended
        a                owl:Class ;
        rdfs:label       "Suspended" ;
        skos:definition  "The care team is temporarily on hold or suspended and not participating in the coordination and delivery of care." ;
        skos:prefLabel   "Suspended" .
