@prefix encounter-discharge-disposition: <http://hl7.org/fhir/discharge-disposition/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

encounter-discharge-disposition:aadvice
        a                owl:Class ;
        rdfs:label       "Left against advice" ;
        skos:definition  "The patient self discharged against medical advice." ;
        skos:prefLabel   "Left against advice" .

encounter-discharge-disposition:exp
        a                owl:Class ;
        rdfs:label       "Expired" ;
        skos:definition  "The patient has deceased during this encounter." ;
        skos:prefLabel   "Expired" .

encounter-discharge-disposition:rehab
        a                owl:Class ;
        rdfs:label       "Rehabilitation" ;
        skos:definition  "The patient was discharged and is to receive post acute care rehabilitation services." ;
        skos:prefLabel   "Rehabilitation" .

encounter-discharge-disposition:hosp
        a                owl:Class ;
        rdfs:label       "Hospice" ;
        skos:definition  "The patient has been discharged into palliative care." ;
        skos:prefLabel   "Hospice" .

encounter-discharge-disposition:home
        a                owl:Class ;
        rdfs:label       "Home" ;
        skos:definition  "The patient was dicharged and has indicated that they are going to return home afterwards." ;
        skos:prefLabel   "Home" .

encounter-discharge-disposition:
        a                owl:Ontology ;
        rdfs:comment     "This value set defines a set of codes that can be used to where the patient left the hospital." ;
        rdfs:label       "DischargeDisposition" ;
        owl:versionIRI   "http://hl7.org/fhir/discharge-disposition/3.1.0" ;
        owl:versionInfo  "DischargeDisposition(3.1.0)" .

encounter-discharge-disposition:psy
        a                owl:Class ;
        rdfs:label       "Psychiatric hospital" ;
        skos:definition  "The patient has been transferred to a psychiatric facility." ;
        skos:prefLabel   "Psychiatric hospital" .

encounter-discharge-disposition:oth
        a                owl:Class ;
        rdfs:label       "Other" ;
        skos:definition  "The discharge disposition has not otherwise defined." ;
        skos:prefLabel   "Other" .

encounter-discharge-disposition:snf
        a                owl:Class ;
        rdfs:label       "Skilled nursing facility" ;
        skos:definition  "The patient has been discharged to a skilled nursing facility for the patient to receive additional care." ;
        skos:prefLabel   "Skilled nursing facility" .

encounter-discharge-disposition:long
        a                owl:Class ;
        rdfs:label       "Long-term care" ;
        skos:definition  "The patient has been discharged into long-term care where is likely to be monitored through an ongoing episode-of-care." ;
        skos:prefLabel   "Long-term care" .

encounter-discharge-disposition:other-hcf
        a                owl:Class ;
        rdfs:label       "Other healthcare facility" ;
        skos:definition  "The patient was transferred to another healthcare facility." ;
        skos:prefLabel   "Other healthcare facility" .
