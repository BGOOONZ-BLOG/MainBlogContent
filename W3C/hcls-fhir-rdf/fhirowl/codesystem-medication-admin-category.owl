@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix medication-admin-category: <http://hl7.org/fhir/medication-admin-category/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

medication-admin-category:inpatient
        a                owl:Class ;
        rdfs:label       "Inpatient" ;
        skos:definition  "Includes administrations in an inpatient or acute care setting" ;
        skos:prefLabel   "Inpatient" .

medication-admin-category:
        a                owl:Ontology ;
        rdfs:comment     "A coded concept describing where the medication administered is expected to occur" ;
        rdfs:label       "MedicationAdministrationCategory" ;
        owl:versionIRI   "http://hl7.org/fhir/medication-admin-category/3.1.0" ;
        owl:versionInfo  "MedicationAdministrationCategory(3.1.0)" .

medication-admin-category:outpatient
        a                owl:Class ;
        rdfs:label       "Outpatient" ;
        skos:definition  "Includes administrations in an outpatient setting (for example, Emergency Department, Outpatient Clinic, Outpatient Surgery, Doctor's office)" ;
        skos:prefLabel   "Outpatient" .

medication-admin-category:community
        a                owl:Class ;
        rdfs:label       "Community" ;
        skos:definition  "Includes administrations by the patient in their home (this would include long term care or nursing homes, hospices, etc)" ;
        skos:prefLabel   "Community" .
