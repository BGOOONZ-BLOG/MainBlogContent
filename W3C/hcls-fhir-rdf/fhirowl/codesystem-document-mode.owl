@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix document-mode: <http://hl7.org/fhir/document-mode/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

document-mode:  a        owl:Ontology ;
        rdfs:comment     "Whether the application produces or consumes documents." ;
        rdfs:label       "DocumentMode" ;
        owl:versionIRI   "http://hl7.org/fhir/document-mode/3.1.0" ;
        owl:versionInfo  "DocumentMode(3.1.0)" .

document-mode:producer
        a                owl:Class ;
        rdfs:label       "Producer" ;
        skos:definition  "The application produces documents of the specified type." ;
        skos:prefLabel   "Producer" .

document-mode:consumer
        a                owl:Class ;
        rdfs:label       "Consumer" ;
        skos:definition  "The application consumes documents of the specified type." ;
        skos:prefLabel   "Consumer" .
