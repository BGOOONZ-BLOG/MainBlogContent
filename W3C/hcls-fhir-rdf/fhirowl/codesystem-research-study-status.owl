@prefix research-study-status: <http://hl7.org/fhir/research-study-status/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

research-study-status:
        a                owl:Ontology ;
        rdfs:comment     "Codes that convey the current status of the research study" ;
        rdfs:label       "ResearchStudyStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/research-study-status/3.1.0" ;
        owl:versionInfo  "ResearchStudyStatus(3.1.0)" .

research-study-status:entered-in-error
        a                owl:Class ;
        rdfs:label       "Entered in error" ;
        skos:definition  "This study never actually existed.  The record is retained for tracking purposes in the event decisions may have been made based on this erroneous information." ;
        skos:prefLabel   "Entered in error" .

research-study-status:completed
        a                owl:Class ;
        rdfs:label       "Completed" ;
        skos:definition  "The information sought by the study has been gathered and compiled and no further work is being performed" ;
        skos:prefLabel   "Completed" .

research-study-status:in-progress
        a                owl:Class ;
        rdfs:label       "In-progress" ;
        skos:definition  "The study is currently being executed" ;
        skos:prefLabel   "In-progress" .

research-study-status:draft
        a                owl:Class ;
        rdfs:label       "Draft" ;
        skos:definition  "The study is undergoing design but the process of selecting study subjects and capturing data has not yet begun." ;
        skos:prefLabel   "Draft" .

research-study-status:stopped
        a                owl:Class ;
        rdfs:label       "Stopped" ;
        skos:definition  "The study was terminated prior to the final determination of results" ;
        skos:prefLabel   "Stopped" .

research-study-status:suspended
        a                owl:Class ;
        rdfs:label       "Suspended" ;
        skos:definition  "Execution of the study has been temporarily paused" ;
        skos:prefLabel   "Suspended" .
