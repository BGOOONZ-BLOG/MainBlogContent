@prefix reaction-event-severity: <http://hl7.org/fhir/reaction-event-severity/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

reaction-event-severity:mild
        a                owl:Class ;
        rdfs:label       "Mild" ;
        skos:definition  "Causes mild physiological effects." ;
        skos:prefLabel   "Mild" .

reaction-event-severity:severe
        a                owl:Class ;
        rdfs:label       "Severe" ;
        skos:definition  "Causes severe physiological effects." ;
        skos:prefLabel   "Severe" .

reaction-event-severity:
        a                owl:Ontology ;
        rdfs:comment     "Clinical assessment of the severity of a reaction event as a whole, potentially considering multiple different manifestations." ;
        rdfs:label       "AllergyIntoleranceSeverity" ;
        owl:versionIRI   "http://hl7.org/fhir/reaction-event-severity/3.1.0" ;
        owl:versionInfo  "AllergyIntoleranceSeverity(3.1.0)" .

reaction-event-severity:moderate
        a                owl:Class ;
        rdfs:label       "Moderate" ;
        skos:definition  "Causes moderate physiological effects." ;
        skos:prefLabel   "Moderate" .
