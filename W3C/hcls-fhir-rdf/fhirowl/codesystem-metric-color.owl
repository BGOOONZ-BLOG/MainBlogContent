@prefix metric-color: <http://hl7.org/fhir/metric-color/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

metric-color:white  a    owl:Class ;
        rdfs:label       "Color White" ;
        skos:definition  "Color for representation - white." ;
        skos:prefLabel   "Color White" .

metric-color:black  a    owl:Class ;
        rdfs:label       "Color Black" ;
        skos:definition  "Color for representation - black." ;
        skos:prefLabel   "Color Black" .

metric-color:cyan  a     owl:Class ;
        rdfs:label       "Color Cyan" ;
        skos:definition  "Color for representation - cyan." ;
        skos:prefLabel   "Color Cyan" .

metric-color:  a         owl:Ontology ;
        rdfs:comment     "Describes the typical color of representation." ;
        rdfs:label       "DeviceMetricColor" ;
        owl:versionIRI   "http://hl7.org/fhir/metric-color/3.1.0" ;
        owl:versionInfo  "DeviceMetricColor(3.1.0)" .

metric-color:magenta  a  owl:Class ;
        rdfs:label       "Color Magenta" ;
        skos:definition  "Color for representation - magenta." ;
        skos:prefLabel   "Color Magenta" .

metric-color:yellow  a   owl:Class ;
        rdfs:label       "Color Yellow" ;
        skos:definition  "Color for representation - yellow." ;
        skos:prefLabel   "Color Yellow" .

metric-color:red  a      owl:Class ;
        rdfs:label       "Color Red" ;
        skos:definition  "Color for representation - red." ;
        skos:prefLabel   "Color Red" .

metric-color:blue  a     owl:Class ;
        rdfs:label       "Color Blue" ;
        skos:definition  "Color for representation - blue." ;
        skos:prefLabel   "Color Blue" .

metric-color:green  a    owl:Class ;
        rdfs:label       "Color Green" ;
        skos:definition  "Color for representation - green." ;
        skos:prefLabel   "Color Green" .
