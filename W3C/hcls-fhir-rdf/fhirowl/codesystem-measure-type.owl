@prefix measure-type: <http://hl7.org/fhir/measure-type/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

measure-type:composite
        a                owl:Class ;
        rdfs:label       "Composite" ;
        skos:definition  "A measure that combines multiple component measures in to a single quality measure" ;
        skos:prefLabel   "Composite" .

measure-type:  a         owl:Ontology ;
        rdfs:comment     "The type of measure (includes codes from 2.16.840.1.113883.1.11.20368)" ;
        rdfs:label       "MeasureType" ;
        owl:versionIRI   "http://hl7.org/fhir/measure-type/3.1.0" ;
        owl:versionInfo  "MeasureType(3.1.0)" .

measure-type:patient-reported-outcome
        a                owl:Class ;
        rdfs:label       "Patient Reported Outcome" ;
        skos:definition  "A measure that focuses on patient-reported information such as patient engagement or patient experience measures" ;
        skos:prefLabel   "Patient Reported Outcome" .

measure-type:outcome  a  owl:Class ;
        rdfs:label       "Outcome" ;
        skos:definition  "A measure that indicates the result of the performance (or non-performance) of a function or process" ;
        skos:prefLabel   "Outcome" .

measure-type:structure
        a                owl:Class ;
        rdfs:label       "Structure" ;
        skos:definition  "A measure that focuses on a health care provider's capacity, systems, and processes to provide high-quality care" ;
        skos:prefLabel   "Structure" .

measure-type:process  a  owl:Class ;
        rdfs:label       "Process" ;
        skos:definition  "A measure which focuses on a process which leads to a certain outcome, meaning that a scientific basis exists for believing that the process, when executed well, will increase the probability of achieving a desired outcome" ;
        skos:prefLabel   "Process" .
