        skos:prefLabel  "Other" .

v2-0247:Y  a            owl:Class ;
        rdfs:label      "Evaluation completed" ;
        skos:prefLabel  "Evaluation completed" .

v2-0247:C  a            owl:Class ;
        rdfs:label      "Product received in condition which made analysis impossible" ;
        skos:prefLabel  "Product received in condition which made analysis impossible" .
