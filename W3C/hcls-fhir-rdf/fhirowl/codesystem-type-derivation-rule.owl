@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix type-derivation-rule: <http://hl7.org/fhir/type-derivation-rule/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

type-derivation-rule:
        a                owl:Ontology ;
        rdfs:comment     "How a type relates to its baseDefinition." ;
        rdfs:label       "TypeDerivationRule" ;
        owl:versionIRI   "http://hl7.org/fhir/type-derivation-rule/3.1.0" ;
        owl:versionInfo  "TypeDerivationRule(3.1.0)" .

type-derivation-rule:constraint
        a                owl:Class ;
        rdfs:label       "Constraint" ;
        skos:definition  "This definition adds additional rules to an existing concrete type" ;
        skos:prefLabel   "Constraint" .

type-derivation-rule:specialization
        a                owl:Class ;
        rdfs:label       "Specialization" ;
        skos:definition  "This definition defines a new type that adds additional elements to the base type" ;
        skos:prefLabel   "Specialization" .
