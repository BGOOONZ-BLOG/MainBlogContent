@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix measure-data-usage: <http://hl7.org/fhir/measure-data-usage/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

measure-data-usage:risk-adjustment-factor
        a                owl:Class ;
        rdfs:label       "Risk Adjustment Factor" ;
        skos:definition  "The data is intended to be used to calculate and apply a risk adjustment model for the measure" ;
        skos:prefLabel   "Risk Adjustment Factor" .

measure-data-usage:  a   owl:Ontology ;
        rdfs:comment     "The intended usage for supplemental data elements in the measure" ;
        rdfs:label       "MeasureDataUsage" ;
        owl:versionIRI   "http://hl7.org/fhir/measure-data-usage/3.1.0" ;
        owl:versionInfo  "MeasureDataUsage(3.1.0)" .

measure-data-usage:supplemental-data
        a                owl:Class ;
        rdfs:label       "Supplemental Data" ;
        skos:definition  "The data is intended to be provided as additional information alongside the measure results" ;
        skos:prefLabel   "Supplemental Data" .
