@prefix v3-AcknowledgementCondition: <http://hl7.org/fhir/v3/AcknowledgementCondition/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-AcknowledgementCondition:NE
        a                owl:Class ;
        rdfs:label       "Never" ;
        skos:definition  "Never send an acknowledgement." ;
        skos:prefLabel   "Never" .

v3-AcknowledgementCondition:ER
        a                owl:Class ;
        rdfs:label       "Error/reject only" ;
        skos:definition  "Send an acknowledgement for error/reject conditions only." ;
        skos:prefLabel   "Error/reject only" .

v3-AcknowledgementCondition:SU
        a                owl:Class ;
        rdfs:label       "Successful only" ;
        skos:definition  "Send an acknowledgement for successful completions only." ;
        skos:prefLabel   "Successful only" .

v3-AcknowledgementCondition:AL
        a                owl:Class ;
        rdfs:label       "Always" ;
        skos:definition  "Always send an acknowledgement." ;
        skos:prefLabel   "Always" .

v3-AcknowledgementCondition:
        a                owl:Ontology ;
        rdfs:comment     " The codes identify the conditions under which accept acknowledgements are required to be returned in response to this message. Note that accept acknowledgement address two different issues at the same time: reliable transport as well as syntactical correctness" ;
        rdfs:label       "v3 Code System AcknowledgementCondition" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/AcknowledgementCondition/2016-11-11" ;
        owl:versionInfo  "v3 Code System AcknowledgementCondition(2016-11-11)" .
