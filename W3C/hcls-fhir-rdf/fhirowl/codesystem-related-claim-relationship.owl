@prefix related-claim-relationship: <http://hl7.org/fhir/ex-relatedclaimrelationship/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

related-claim-relationship:associated
        a                owl:Class ;
        rdfs:label       "Associated Claim" ;
        skos:definition  "A claim for a different suite of services which is related the suite claimed here." ;
        skos:prefLabel   "Associated Claim" .

related-claim-relationship:
        a                owl:Ontology ;
        rdfs:comment     "This value set includes sample Related Claim Relationship codes." ;
        rdfs:label       "Example Related Claim Relationship Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/ex-relatedclaimrelationship/3.1.0" ;
        owl:versionInfo  "Example Related Claim Relationship Codes(3.1.0)" .

related-claim-relationship:prior
        a                owl:Class ;
        rdfs:label       "Prior Claim" ;
        skos:definition  "A prior claim instance for the same intended suite of services." ;
        skos:prefLabel   "Prior Claim" .
