@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix detectedissue-severity: <http://hl7.org/fhir/detectedissue-severity/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

detectedissue-severity:high
        a                owl:Class ;
        rdfs:label       "High" ;
        skos:definition  "Indicates the issue may be life-threatening or has the potential to cause permanent injury." ;
        skos:prefLabel   "High" .

detectedissue-severity:moderate
        a                owl:Class ;
        rdfs:label       "Moderate" ;
        skos:definition  "Indicates the issue may result in noticeable adverse consequences but is unlikely to be life-threatening or cause permanent injury." ;
        skos:prefLabel   "Moderate" .

detectedissue-severity:low
        a                owl:Class ;
        rdfs:label       "Low" ;
        skos:definition  "Indicates the issue may result in some adverse consequences but is unlikely to substantially affect the situation of the subject." ;
        skos:prefLabel   "Low" .

detectedissue-severity:
        a                owl:Ontology ;
        rdfs:comment     "Indicates the potential degree of impact of the identified issue on the patient." ;
        rdfs:label       "DetectedIssueSeverity" ;
        owl:versionIRI   "http://hl7.org/fhir/detectedissue-severity/3.1.0" ;
        owl:versionInfo  "DetectedIssueSeverity(3.1.0)" .
