namespace ttp = "http://www.w3.org/ns/ttml#parameter"

# Standard Metadata Attributes

TTAF.cellResolution.attrib
  = attribute ttp:cellResolution { TTAF.CellResolution.datatype }?
TTAF.clockMode.attrib
