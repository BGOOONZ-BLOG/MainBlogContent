@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v2-0536: <http://hl7.org/fhir/v2/0536/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v2-0536:R  a            owl:Class ;
        rdfs:label      "Revoked" ;
        skos:prefLabel  "Revoked" .

v2-0536:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 0536 ( Certificate Status)" ;
        rdfs:label       "v2 Certificate Status" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/0536/2.8.2" ;
        owl:versionInfo  "v2 Certificate Status(2.8.2)" .

v2-0536:P  a            owl:Class ;
        rdfs:label      "Provisional" ;
        skos:prefLabel  "Provisional" .

v2-0536:I  a            owl:Class ;
        rdfs:label      "Inactive" ;
        skos:prefLabel  "Inactive" .

v2-0536:V  a            owl:Class ;
        rdfs:label      "Active/Valid" ;
        skos:prefLabel  "Active/Valid" .

v2-0536:E  a            owl:Class ;
        rdfs:label      "Expired" ;
        skos:prefLabel  "Expired" .
