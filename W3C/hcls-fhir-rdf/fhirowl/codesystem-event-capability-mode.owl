@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix event-capability-mode: <http://hl7.org/fhir/event-capability-mode/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

event-capability-mode:
        a                owl:Ontology ;
        rdfs:comment     "The mode of a message capability statement." ;
        rdfs:label       "EventCapabilityMode" ;
        owl:versionIRI   "http://hl7.org/fhir/event-capability-mode/3.1.0" ;
        owl:versionInfo  "EventCapabilityMode(3.1.0)" .

event-capability-mode:receiver
        a                owl:Class ;
        rdfs:label       "Receiver" ;
        skos:definition  "The application receives requests and sends responses." ;
        skos:prefLabel   "Receiver" .

event-capability-mode:sender
        a                owl:Class ;
        rdfs:label       "Sender" ;
        skos:definition  "The application sends requests and receives responses." ;
        skos:prefLabel   "Sender" .
