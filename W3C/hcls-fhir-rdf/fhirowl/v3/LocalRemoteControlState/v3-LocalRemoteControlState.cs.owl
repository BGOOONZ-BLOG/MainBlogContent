@prefix v3-LocalRemoteControlState: <http://hl7.org/fhir/v3/LocalRemoteControlState/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-LocalRemoteControlState:
        a                owl:Ontology ;
        rdfs:comment     "**** MISSING DEFINITIONS ****" ;
        rdfs:label       "v3 Code System LocalRemoteControlState" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/LocalRemoteControlState/2016-11-11" ;
        owl:versionInfo  "v3 Code System LocalRemoteControlState(2016-11-11)" .

v3-LocalRemoteControlState:L
        a                owl:Class ;
        rdfs:label       "Local" ;
        skos:definition  "An equipment can either work autonomously ('Local' control state)." ;
        skos:prefLabel   "Local" .

v3-LocalRemoteControlState:R
        a                owl:Class ;
        rdfs:label       "Remote" ;
        skos:definition  "An equipment can be controlled by another system, e.g., LAS computer ('Remote' control state)." ;
        skos:prefLabel   "Remote" .
