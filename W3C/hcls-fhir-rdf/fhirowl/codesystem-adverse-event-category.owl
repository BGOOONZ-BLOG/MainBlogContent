@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix adverse-event-category: <http://hl7.org/fhir/adverse-event-category/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

adverse-event-category:
        a                owl:Ontology ;
        rdfs:comment     "Overall categorization of the event, e.g. product related or situational" ;
        rdfs:label       "AdverseEventCategory" ;
        owl:versionIRI   "http://hl7.org/fhir/adverse-event-category/3.1.0" ;
        owl:versionInfo  "AdverseEventCategory(3.1.0)" .

adverse-event-category:WrongTime
        a                owl:Class ;
        rdfs:label       "Wrong Time" ;
        rdfs:subClassOf  adverse-event-category:ProductUseError ;
        skos:prefLabel   "Wrong Time" .

adverse-event-category:ProductQuality
        a               owl:Class ;
        rdfs:label      "Product Quality" ;
        skos:prefLabel  "Product Quality" .

adverse-event-category:IncorrectPrescribingInformation
        a                owl:Class ;
        rdfs:label       "Incorrect Prescribing Information" ;
        rdfs:subClassOf  adverse-event-category:ProductUseError ;
        skos:prefLabel   "Incorrect Prescribing Information" .

adverse-event-category:ProductProblem
        a               owl:Class ;
        rdfs:label      "Product Problem" ;
        skos:prefLabel  "Product Problem" .

adverse-event-category:WrongRouteOfAdministration
        a                owl:Class ;
        rdfs:label       "Wrong Route of Administration" ;
        rdfs:subClassOf  adverse-event-category:ProductUseError ;
        skos:prefLabel   "Wrong Route of Administration" .

adverse-event-category:MedicalDeviceUseError
        a               owl:Class ;
        rdfs:label      "Medical Device Use Error" ;
        skos:prefLabel  "Medical Device Use Error" .

adverse-event-category:WrongTechnique
        a                owl:Class ;
        rdfs:label       "Wrong Technique" ;
        rdfs:subClassOf  adverse-event-category:ProductUseError ;
        skos:prefLabel   "Wrong Technique" .

adverse-event-category:WrongRate
        a                owl:Class ;
        rdfs:label       "Wrong Rate" ;
        rdfs:subClassOf  adverse-event-category:ProductUseError ;
        skos:prefLabel   "Wrong Rate" .

adverse-event-category:ProblemDifferentManufacturer
        a               owl:Class ;
        rdfs:label      "Problem with Different Manufacturer of Same Medicine" ;
        skos:prefLabel  "Problem with Different Manufacturer of Same Medicine" .

adverse-event-category:ExpiredDrug
        a                owl:Class ;
        rdfs:label       "Expired Drug" ;
        rdfs:subClassOf  adverse-event-category:ProductUseError ;
        skos:prefLabel   "Expired Drug" .

adverse-event-category:WrongDose
        a                owl:Class ;
        rdfs:label       "Wrong Dose" ;
        rdfs:subClassOf  adverse-event-category:ProductUseError ;
        skos:prefLabel   "Wrong Dose" .

adverse-event-category:WrongDuration
        a                owl:Class ;
        rdfs:label       "Wrong Duration" ;
        rdfs:subClassOf  adverse-event-category:ProductUseError ;
        skos:prefLabel   "Wrong Duration" .

adverse-event-category:ProductUseError
        a               owl:Class ;
        rdfs:label      "Product Use Error" ;
        skos:prefLabel  "Product Use Error" .

adverse-event-category:UnsafePhysicalEnvironment
        a               owl:Class ;
        rdfs:label      "Unsafe Physical Environment" ;
        skos:prefLabel  "Unsafe Physical Environment" .
