@prefix medication-request-priority: <http://hl7.org/fhir/medication-request-priority/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

medication-request-priority:asap
        a                owl:Class ;
        rdfs:label       "ASAP" ;
        skos:definition  "The order should be acted on as soon as possible." ;
        skos:prefLabel   "ASAP" .

medication-request-priority:urgent
        a                owl:Class ;
        rdfs:label       "Urgent" ;
        skos:definition  "The order should be urgently." ;
        skos:prefLabel   "Urgent" .

medication-request-priority:stat
        a                owl:Class ;
        rdfs:label       "Stat" ;
        skos:definition  "The order is time-critical." ;
        skos:prefLabel   "Stat" .

medication-request-priority:
        a                owl:Ontology ;
        rdfs:comment     "Identifies the level of importance to be assigned to actioning the request" ;
        rdfs:label       "MedicationRequestPriority" ;
        owl:versionIRI   "http://hl7.org/fhir/medication-request-priority/3.1.0" ;
        owl:versionInfo  "MedicationRequestPriority(3.1.0)" .

medication-request-priority:routine
        a                owl:Class ;
        rdfs:label       "Routine" ;
        skos:definition  "The order has a normal priority ." ;
        skos:prefLabel   "Routine" .
