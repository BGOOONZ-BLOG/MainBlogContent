:hasDiskSize rdf:type owl:DatatypeProperty ;
             
             rdfs:range xsd:string ;
             
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Disk
                                         :RequiredResources
                                         :ScaleParam
                                       )
                         ] .



