@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .
@prefix animal-genderstatus: <http://hl7.org/fhir/animal-genderstatus/> .

animal-genderstatus:unknown
        a                owl:Class ;
        rdfs:label       "Unknown" ;
        skos:definition  "Unable to determine whether the animal has been neutered." ;
        skos:prefLabel   "Unknown" .

animal-genderstatus:  a  owl:Ontology ;
        rdfs:comment     "This example value set defines a set of codes that can be used to indicate the current state of the animal's reproductive organs." ;
        rdfs:label       "GenderStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/animal-genderstatus/3.1.0" ;
        owl:versionInfo  "GenderStatus(3.1.0)" .

animal-genderstatus:intact
        a                owl:Class ;
        rdfs:label       "Intact" ;
        skos:definition  "The animal's reproductive organs are intact." ;
        skos:prefLabel   "Intact" .

animal-genderstatus:neutered
        a                owl:Class ;
        rdfs:label       "Neutered" ;
        skos:definition  "The animal has been sterilized, castrated or otherwise made infertile." ;
        skos:prefLabel   "Neutered" .
