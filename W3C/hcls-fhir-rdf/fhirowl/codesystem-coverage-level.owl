@prefix coverage-level: <http://hl7.org/fhir/coverage-level/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

coverage-level:  a       owl:Ontology ;
        rdfs:comment     "This value set includes sample Coverage Level codes." ;
        rdfs:label       "Coverage Level Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/coverage-level/3.1.0" ;
        owl:versionInfo  "Coverage Level Codes(3.1.0)" .

coverage-level:class  a  owl:Class ;
        rdfs:label       "Class" ;
        skos:definition  "A class of benefits." ;
        skos:prefLabel   "Class" .

coverage-level:subplan
        a                owl:Class ;
        rdfs:label       "SubPlan" ;
        skos:definition  "A subset of a specific suite of benefits." ;
        skos:prefLabel   "SubPlan" .

coverage-level:subclass
        a                owl:Class ;
        rdfs:label       "SubClass" ;
        skos:definition  "A subset of a class of benefits." ;
        skos:prefLabel   "SubClass" .

coverage-level:group  a  owl:Class ;
        rdfs:label       "Group" ;
        skos:definition  "An employee group" ;
        skos:prefLabel   "Group" .

coverage-level:plan  a   owl:Class ;
        rdfs:label       "Plan" ;
        skos:definition  "A specific suite of benefits." ;
        skos:prefLabel   "Plan" .

coverage-level:subgroup
        a                owl:Class ;
        rdfs:label       "SubGroup" ;
        skos:definition  "A sub-group of an employee group" ;
        skos:prefLabel   "SubGroup" .
