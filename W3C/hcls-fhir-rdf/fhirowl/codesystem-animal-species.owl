@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix animal-species: <http://hl7.org/fhir/animal-species/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

animal-species:  a       owl:Ontology ;
        rdfs:comment     "This example value set defines a set of codes that can be used to indicate species of animal patients." ;
        rdfs:label       "AnimalSpecies" ;
        owl:versionIRI   "http://hl7.org/fhir/animal-species/3.1.0" ;
        owl:versionInfo  "AnimalSpecies(3.1.0)" .

animal-species:ovisa  a  owl:Class ;
        rdfs:label       "Sheep" ;
        skos:definition  "Ovis aries" ;
        skos:prefLabel   "Sheep" .

animal-species:serinuscd
        a                owl:Class ;
        rdfs:label       "Domestic Canary" ;
        skos:definition  "Serinus canaria domestica" ;
        skos:prefLabel   "Domestic Canary" .

animal-species:canislf
        a                owl:Class ;
        rdfs:label       "Dog" ;
        skos:definition  "Canis lupus familiaris" ;
        skos:prefLabel   "Dog" .
