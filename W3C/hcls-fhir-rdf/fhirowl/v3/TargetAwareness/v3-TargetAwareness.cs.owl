@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v3-TargetAwareness: <http://hl7.org/fhir/v3/TargetAwareness/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-TargetAwareness:M  a  owl:Class ;
        rdfs:label       "marginal" ;
        skos:definition  "Target person is marginally aware of the issue." ;
        skos:prefLabel   "marginal" .

v3-TargetAwareness:F  a  owl:Class ;
        rdfs:label       "full awareness" ;
        skos:definition  "Target person is fully aware of the issue." ;
        skos:prefLabel   "full awareness" .

v3-TargetAwareness:P  a  owl:Class ;
        rdfs:label       "partial" ;
        skos:definition  "Target person is partially aware of the issue." ;
        skos:prefLabel   "partial" .

v3-TargetAwareness:  a   owl:Ontology ;
        rdfs:comment     "**** MISSING DEFINITIONS ****" ;
        rdfs:label       "v3 Code System TargetAwareness" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/TargetAwareness/2016-11-11" ;
        owl:versionInfo  "v3 Code System TargetAwareness(2016-11-11)" .

v3-TargetAwareness:U  a  owl:Class ;
        rdfs:label       "uninformed" ;
        skos:definition  "Target person has not yet been informed of the issue." ;
        skos:prefLabel   "uninformed" .

v3-TargetAwareness:D  a  owl:Class ;
        rdfs:label       "denying" ;
        skos:definition  "Target person has been informed about the issue but currently denies it." ;
        skos:prefLabel   "denying" .

v3-TargetAwareness:I  a  owl:Class ;
        rdfs:label       "incapable" ;
        skos:definition  "Target person is not capable of comprehending the issue." ;
        skos:prefLabel   "incapable" .
