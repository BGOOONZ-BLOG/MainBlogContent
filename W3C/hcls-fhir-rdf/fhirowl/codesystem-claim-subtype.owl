@prefix claim-subtype: <http://hl7.org/fhir/ex-claimsubtype/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

claim-subtype:ortho  a   owl:Class ;
        rdfs:label       "Orthodontic Claim" ;
        skos:definition  "A claim for Orthodontic Services." ;
        skos:prefLabel   "Orthodontic Claim" .

claim-subtype:emergency
        a                owl:Class ;
        rdfs:label       "Emergency Claim" ;
        skos:definition  "A claim for emergency services." ;
        skos:prefLabel   "Emergency Claim" .

claim-subtype:  a        owl:Ontology ;
        rdfs:comment     "This value set includes sample Claim SubType codes which are used to distinguish the claim types for example within type institutional there may be subtypes for emergency services, bedstay and transportation." ;
        rdfs:label       "Example Claim SubType Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/ex-claimsubtype/3.1.0" ;
        owl:versionInfo  "Example Claim SubType Codes(3.1.0)" .
