@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix related-artifact-type: <http://hl7.org/fhir/related-artifact-type/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

related-artifact-type:justification
        a                owl:Class ;
        rdfs:label       "Justification" ;
        skos:definition  "A summary of the justification for the knowledge resource including supporting evidence, relevant guidelines, or other clinically important information. This information is intended to provide a way to make the justification for the knowledge resource available to the consumer of interventions or results produced by the knowledge resource" ;
        skos:prefLabel   "Justification" .

related-artifact-type:predecessor
        a                owl:Class ;
        rdfs:label       "Predecessor" ;
        skos:definition  "The previous version of the knowledge resource" ;
        skos:prefLabel   "Predecessor" .

related-artifact-type:composed-of
        a                owl:Class ;
        rdfs:label       "Composed Of" ;
        skos:definition  "The knowledge resource is composed of the given related artifact" ;
        skos:prefLabel   "Composed Of" .

related-artifact-type:documentation
        a                owl:Class ;
        rdfs:label       "Documentation" ;
        skos:definition  "Additional documentation for the knowledge resource. This would include additional instructions on usage as well as additional information on clinical context or appropriateness" ;
        skos:prefLabel   "Documentation" .

related-artifact-type:depends-on
        a                owl:Class ;
        rdfs:label       "Depends On" ;
        skos:definition  "The knowledge resource depends on the given related artifact" ;
        skos:prefLabel   "Depends On" .

related-artifact-type:derived-from
        a                owl:Class ;
        rdfs:label       "Derived From" ;
        skos:definition  "The knowledge resource is derived from the related artifact. This is intended to capture the relationship in which a particular knowledge resource is based on the content of another artifact, but is modified to capture either a different set of overall requirements, or a more specific set of requirements such as those involved in a particular institution or clinical setting" ;
        skos:prefLabel   "Derived From" .

related-artifact-type:
        a                owl:Ontology ;
        rdfs:comment     "The type of relationship to the related artifact" ;
        rdfs:label       "RelatedArtifactType" ;
        owl:versionIRI   "http://hl7.org/fhir/related-artifact-type/3.1.0" ;
        owl:versionInfo  "RelatedArtifactType(3.1.0)" .

related-artifact-type:citation
        a                owl:Class ;
        rdfs:label       "Citation" ;
        skos:definition  "Bibliographic citation for papers, references, or other relevant material for the knowledge resource. This is intended to allow for citation of related material, but that was not necessarily specifically prepared in connection with this knowledge resource" ;
        skos:prefLabel   "Citation" .

related-artifact-type:successor
        a                owl:Class ;
        rdfs:label       "Successor" ;
        skos:definition  "The next version of the knowledge resource" ;
        skos:prefLabel   "Successor" .
