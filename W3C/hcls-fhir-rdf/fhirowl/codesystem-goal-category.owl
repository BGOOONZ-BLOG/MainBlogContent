@prefix goal-category: <http://hl7.org/fhir/goal-category/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

goal-category:physiotherapy
        a                owl:Class ;
        rdfs:label       "Physiotherapy" ;
        skos:definition  "Goals related to the mobility and motor capability of the subject." ;
        skos:prefLabel   "Physiotherapy" .

goal-category:safety  a  owl:Class ;
        rdfs:label       "Safety" ;
        skos:definition  "Goals related to the personal protection of the subject." ;
        skos:prefLabel   "Safety" .

goal-category:  a        owl:Ontology ;
        rdfs:comment     "Example codes for grouping goals for filtering or presentation." ;
        rdfs:label       "GoalCategory" ;
        owl:versionIRI   "http://hl7.org/fhir/goal-category/3.1.0" ;
        owl:versionInfo  "GoalCategory(3.1.0)" .

goal-category:nursing
        a                owl:Class ;
        rdfs:label       "Nursing" ;
        skos:definition  "Goals related to the practice of nursing or established by nurses." ;
        skos:prefLabel   "Nursing" .

goal-category:behavioral
        a                owl:Class ;
        rdfs:label       "Behavioral" ;
        skos:definition  "Goals related to the manner in which the subject acts." ;
        skos:prefLabel   "Behavioral" .

goal-category:dietary
        a                owl:Class ;
        rdfs:label       "Dietary" ;
        skos:definition  "Goals related to the consumption of food and/or beverages." ;
        skos:prefLabel   "Dietary" .
