###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#isRequested

:isRequested rdf:type owl:DatatypeProperty ;
                
             rdfs:comment "Indicate if a service is requested (True) or not (False)."^^xsd:string ;

             rdfs:domain :MonitoringService ;

             rdfs:range xsd:boolean .  



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#isOffered

:isOffered rdf:type owl:DatatypeProperty ;

           rdfs:comment "Indicate if a service is provided (offered to be requested by users)."^^xsd:string ;
             
           rdfs:domain :MonitoringService ;
             
           rdfs:range xsd:boolean .



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#endTime

:endTime rdf:type owl:DatatypeProperty ;
                            
         rdfs:domain :Lifetime ;
              
         rdfs:range xsd:dateTime .



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#startTime

:startTime rdf:type owl:DatatypeProperty ;
                            
           rdfs:domain :Lifetime ;
              
           rdfs:range xsd:dateTime .



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#elapsedTimeAtClientSinceExperimentStartedInSeconds

:elapsedTimeAtClientSinceExperimentStartedInSeconds rdf:type owl:DatatypeProperty ;
                                                
                                           rdfs:range xsd:decimal .



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#elapsedTimeAtServerSinceExperimentStartedInSeconds

:elapsedTimeAtServerSinceExperimentStartedInSeconds rdf:type owl:DatatypeProperty ;
                                                
                                           rdfs:range xsd:decimal .



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#requiresUsername

:requiresUsername rdf:type owl:DatatypeProperty ;
              
                  rdfs:comment "Determines the username."^^xsd:string ;
              
                  rdfs:range xsd:string ;

                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Service
                                              :Tool
                                            )
                              ] . 



###  https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-monitoring#requiresPassword

:requiresPassword rdf:type owl:DatatypeProperty ;

                  rdfs:comment "Determines the password."^^xsd:string ;
              
                  rdfs:range xsd:string ;

                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Service
                                              :Tool
                                            )
                             ] . 





#################################################################
