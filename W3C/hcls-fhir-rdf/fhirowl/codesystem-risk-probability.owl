@prefix risk-probability: <http://hl7.org/fhir/risk-probability/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

risk-probability:  a     owl:Ontology ;
        rdfs:comment     "Codes representing the likelihood of a particular outcome in a risk assessment." ;
        rdfs:label       "Risk Probability" ;
        owl:versionIRI   "http://hl7.org/fhir/risk-probability/3.1.0" ;
        owl:versionInfo  "Risk Probability(3.1.0)" .

risk-probability:negligible
        a                owl:Class ;
        rdfs:label       "Negligible likelihood" ;
        skos:definition  "The specified outcome is exceptionally unlikely." ;
        skos:prefLabel   "Negligible likelihood" .

risk-probability:certain
        a                owl:Class ;
        rdfs:label       "Certain" ;
        skos:definition  "The specified outcome is effectively guaranteed." ;
        skos:prefLabel   "Certain" .

risk-probability:low  a  owl:Class ;
        rdfs:label       "Low likelihood" ;
        skos:definition  "The specified outcome is possible but unlikely." ;
        skos:prefLabel   "Low likelihood" .

risk-probability:high
        a                owl:Class ;
        rdfs:label       "High likelihood" ;
        skos:definition  "The specified outcome is more likely to occur than not." ;
        skos:prefLabel   "High likelihood" .

risk-probability:moderate
        a                owl:Class ;
        rdfs:label       "Moderate likelihood" ;
        skos:definition  "The specified outcome has a reasonable likelihood of occurrence." ;
        skos:prefLabel   "Moderate likelihood" .
