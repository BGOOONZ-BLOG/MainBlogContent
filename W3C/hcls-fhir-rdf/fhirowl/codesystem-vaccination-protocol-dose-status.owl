@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix vaccination-protocol-dose-status: <http://hl7.org/fhir/vaccination-protocol-dose-status/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

vaccination-protocol-dose-status:nocount
        a                owl:Class ;
        rdfs:label       "Does not Count" ;
        skos:definition  "The dose does not count toward fulfilling a path to immunity for a patient." ;
        skos:prefLabel   "Does not Count" .

vaccination-protocol-dose-status:
        a                owl:Ontology ;
        rdfs:comment     "The value set to instantiate this attribute should be drawn from a terminologically robust code system that consists of or contains concepts to support describing the validity of a dose relative to a particular recommended schedule. This value set is provided as a suggestive example." ;
        rdfs:label       "Vaccination Protocol Dose Status codes" ;
        owl:versionIRI   "http://hl7.org/fhir/vaccination-protocol-dose-status/3.1.0" ;
        owl:versionInfo  "Vaccination Protocol Dose Status codes(3.1.0)" .

vaccination-protocol-dose-status:count
        a                owl:Class ;
        rdfs:label       "Counts" ;
        skos:definition  "The dose counts toward fulfilling a path to immunity for a patient, providing protection against the target disease." ;
        skos:prefLabel   "Counts" .
