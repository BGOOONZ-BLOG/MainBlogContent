{
    "@context":[  
       "https://www.w3.org/2019/wot/td/v1"
    ],  
    "title":"Festo-discrete-plant",
    "description":"Servient of Station Distribute U9",
    "@type":"Thing",
    "security":[  
       "nosec_sc"
    ],
    "properties":{  
       "moduleState":{  
          "type":"string",
          "enum":[  
             "Idle",
             "Running",
             "EmgStop",
             "Error",
             "EmptyFeeder"
          ],
          "description":"Current state of module 'Distribute'",
          "observable":true,
          "readOnly":true,
          "forms":[  
             {  
                "href":"http://192.168.0.168:8080/U9/properties/moduleState",
                "contentType":"application/json",
                "op":"readproperty"
             },
             {  
                "href":"http://192.168.0.168:8080/U9/properties/moduleState/observable",
                "contentType":"application/json",
                "op":[  
                   "observeproperty"
                ],
                "subprotocol":"longpoll"
             },
             {  
                "href":"coap://192.168.0.168:5683/U9/pr/moduleState",
                "contentType":"application/json",
                "op":"readproperty"
             }
          ]
       },
       "processedWorkPieces":{  
          "type":"integer",
          "description":"Number of processed workpieces by module 'Distribute'",
          "observable":true,
          "readOnly":true,
          "forms":[  
             {  
                "href":"http://192.168.0.168:8080/U9/properties/processedWorkPieces",
                "contentType":"application/json",
                "op":"readproperty"
             },
             {  
                "href":"http://192.168.0.168:8080/U9/properties/processedWorkPieces/observable",
                "contentType":"application/json",
                "op":[  
                   "observeproperty"
                ],
                "subprotocol":"longpoll"
             },
             {  
                "href":"coap://192.168.0.168:5683/U9/pr/processedWorkPieces",
                "contentType":"application/json",
                "op":"readproperty"
             }
          ]
       },
       "emergencyStop":{  
          "type":"boolean",
          "description":"Immediately stops the module",
          "observable":false,
          "readOnly":true,
          "forms":[  
             {  
                "href":"http://192.168.0.168:8080/U9/properties/emergencyStop",
                "contentType":"application/json",
                "op":"readproperty"
             },
             {  
                "href":"coap://192.168.0.168:5683/U9/pr/emergencyStop",
                "contentType":"application/json",
                "op":"readproperty"
             }
          ]
       },
       "plantSpeed":{  
          "type":"string",
          "enum":[  
             "Stop",
             "Slow",
             "Normal",
             "Fast"
          ],
          "description":"The speed the module should operate with",
          "observable":false,
          "readOnly":true,
          "forms":[  
             {  
                "href":"http://192.168.0.168:8080/U9/properties/plantSpeed",
                "contentType":"application/json",
                "op":"readproperty"
             },
             {  
                "href":"coap://192.168.0.168:5683/U9/pr/plantSpeed",
                "contentType":"application/json",
                "op":"readproperty"
             }
          ]
       }
    },
    "id":"urn:uuid:Festo-discrete-plant",
    "securityDefinitions":{  
       "nosec_sc":{  
          "scheme":"nosec"
       }
    }
 }
