@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix v2-0315: <http://hl7.org/fhir/v2/0315/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v2-0315:I  a            owl:Class ;
        rdfs:label      "No, patient does not have a living will but information was provided" ;
        skos:prefLabel  "No, patient does not have a living will but information was provided" .

v2-0315:N  a            owl:Class ;
        rdfs:label      "No, patient does not have a living will and no information was provided" ;
        skos:prefLabel  "No, patient does not have a living will and no information was provided" .

v2-0315:Y  a            owl:Class ;
        rdfs:label      "Yes, patient has a living will" ;
        skos:prefLabel  "Yes, patient has a living will" .

v2-0315:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 0315 ( Living Will Code)" ;
        rdfs:label       "v2 Living Will Code" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/0315/2.8.2" ;
        owl:versionInfo  "v2 Living Will Code(2.8.2)" .

v2-0315:F  a            owl:Class ;
        rdfs:label      "Yes, patient has a living will but it is not on file" ;
        skos:prefLabel  "Yes, patient has a living will but it is not on file" .

v2-0315:U  a            owl:Class ;
        rdfs:label      "Unknown" ;
        skos:prefLabel  "Unknown" .
