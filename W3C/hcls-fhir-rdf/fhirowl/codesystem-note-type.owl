@prefix note-type: <http://hl7.org/fhir/note-type/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

note-type:  a            owl:Ontology ;
        rdfs:comment     "The presentation types of notes." ;
        rdfs:label       "NoteType" ;
        owl:versionIRI   "http://hl7.org/fhir/note-type/3.1.0" ;
        owl:versionInfo  "NoteType(3.1.0)" .

note-type:print  a       owl:Class ;
        rdfs:label       "Print (Form)" ;
        skos:definition  "Print the note on the form." ;
        skos:prefLabel   "Print (Form)" .

note-type:printoper  a   owl:Class ;
        rdfs:label       "Print (Operator)" ;
        skos:definition  "Print the note for the operator." ;
        skos:prefLabel   "Print (Operator)" .

note-type:display  a     owl:Class ;
        rdfs:label       "Display" ;
        skos:definition  "Display the note." ;
        skos:prefLabel   "Display" .
