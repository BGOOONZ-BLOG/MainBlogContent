@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix goal-acceptance-status: <http://hl7.org/fhir/goal-acceptance-status/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

goal-acceptance-status:agree
        a                owl:Class ;
        rdfs:label       "Agree" ;
        skos:definition  "Stakeholder supports pursuit of the goal" ;
        skos:prefLabel   "Agree" .

goal-acceptance-status:disagree
        a                owl:Class ;
        rdfs:label       "Disagree" ;
        skos:definition  "Stakeholder is not in support of the pursuit of the goal" ;
        skos:prefLabel   "Disagree" .

goal-acceptance-status:
        a                owl:Ontology ;
        rdfs:comment     "Codes indicating whether the goal has been accepted by a stakeholder" ;
        rdfs:label       "GoalAcceptanceStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/goal-acceptance-status/3.1.0" ;
        owl:versionInfo  "GoalAcceptanceStatus(3.1.0)" .

goal-acceptance-status:pending
        a                owl:Class ;
        rdfs:label       "Pending" ;
        skos:definition  "Stakeholder has not yet made a decision on whether they support the goal" ;
        skos:prefLabel   "Pending" .
