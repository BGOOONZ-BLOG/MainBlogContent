@prefix w5:    <http://hl7.org/fhir/w5#> .

v2-0252:TC  a           owl:Class ;
        rdfs:label      "Toxic levels of product documented in blood or body fluids" ;
        skos:prefLabel  "Toxic levels of product documented in blood or body fluids" .

v2-0252:OE  a           owl:Class ;
        rdfs:label      "Occurrence of event was confirmed by objective evidence" ;
        skos:prefLabel  "Occurrence of event was confirmed by objective evidence" .

v2-0252:DR  a           owl:Class ;
        rdfs:label      "Dose response observed" ;
        skos:prefLabel  "Dose response observed" .

v2-0252:EX  a           owl:Class ;
        rdfs:label      "Alternative explanations for the event available" ;
        skos:prefLabel  "Alternative explanations for the event available" .

v2-0252:OT  a           owl:Class ;
        rdfs:label      "Other" ;
        skos:prefLabel  "Other" .

v2-0252:IN  a           owl:Class ;
        rdfs:label      "Event occurred after product introduced" ;
        skos:prefLabel  "Event occurred after product introduced" .

v2-0252:SE  a           owl:Class ;
        rdfs:label      "Similar events in past for this patient" ;
        skos:prefLabel  "Similar events in past for this patient" .

v2-0252:AW  a           owl:Class ;
        rdfs:label      "Abatement of event after product withdrawn" ;
        skos:prefLabel  "Abatement of event after product withdrawn" .

v2-0252:PL  a           owl:Class ;
