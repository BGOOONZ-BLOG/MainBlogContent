@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix composition-attestation-mode: <http://hl7.org/fhir/composition-attestation-mode/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

composition-attestation-mode:professional
        a                owl:Class ;
        rdfs:label       "Professional" ;
        skos:definition  "The person authenticated the content in their professional capacity." ;
        skos:prefLabel   "Professional" .

composition-attestation-mode:
        a                owl:Ontology ;
        rdfs:comment     "The way in which a person authenticated a composition." ;
        rdfs:label       "CompositionAttestationMode" ;
        owl:versionIRI   "http://hl7.org/fhir/composition-attestation-mode/3.1.0" ;
        owl:versionInfo  "CompositionAttestationMode(3.1.0)" .

composition-attestation-mode:official
        a                owl:Class ;
        rdfs:label       "Official" ;
        skos:definition  "The organization authenticated the content as consistent with their policies and procedures." ;
        skos:prefLabel   "Official" .

composition-attestation-mode:personal
        a                owl:Class ;
        rdfs:label       "Personal" ;
        skos:definition  "The person authenticated the content in their personal capacity." ;
        skos:prefLabel   "Personal" .

composition-attestation-mode:legal
        a                owl:Class ;
        rdfs:label       "Legal" ;
        skos:definition  "The person authenticated the content and accepted legal responsibility for its content." ;
        skos:prefLabel   "Legal" .
