@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex-onsettype: <http://hl7.org/fhir/ex-onsettype/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

ex-onsettype:  a         owl:Ontology ;
        rdfs:comment     "This value set includes example Onset Type codes which are used to identify the event for which the onset, starting date, is required." ;
        rdfs:label       "Example Onset Type (Reason) Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/ex-onsettype/3.1.0" ;
        owl:versionInfo  "Example Onset Type (Reason) Codes(3.1.0)" .

ex-onsettype:sym  a      owl:Class ;
        rdfs:label       "Start of Symptoms" ;
        skos:definition  "Date when symptoms were first noticed." ;
        skos:prefLabel   "Start of Symptoms" .

ex-onsettype:lmn  a      owl:Class ;
        rdfs:label       "Last Menstruation" ;
        skos:definition  "Start date of last menstruation." ;
        skos:prefLabel   "Last Menstruation" .

ex-onsettype:lxm  a      owl:Class ;
        rdfs:label       "Last Exam" ;
        skos:definition  "Date of last examination." ;
        skos:prefLabel   "Last Exam" .
