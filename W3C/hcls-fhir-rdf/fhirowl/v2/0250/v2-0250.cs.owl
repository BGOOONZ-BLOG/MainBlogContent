@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v2-0250: <http://hl7.org/fhir/v2/0250/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v2-0250:H  a            owl:Class ;
        rdfs:label      "Highly probable" ;
        skos:prefLabel  "Highly probable" .

v2-0250:M  a            owl:Class ;
        rdfs:label      "Moderately probable" ;
        skos:prefLabel  "Moderately probable" .

v2-0250:I  a            owl:Class ;
        rdfs:label      "Improbable" ;
        skos:prefLabel  "Improbable" .

v2-0250:N  a            owl:Class ;
        rdfs:label      "Not related" ;
        skos:prefLabel  "Not related" .

v2-0250:S  a            owl:Class ;
        rdfs:label      "Somewhat probable" ;
        skos:prefLabel  "Somewhat probable" .

v2-0250:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 0250 ( Relatedness Assessment)" ;
        rdfs:label       "v2 Relatedness Assessment" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/0250/2.8.2" ;
        owl:versionInfo  "v2 Relatedness Assessment(2.8.2)" .
