@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix iso-21089-lifecycle: <http://hl7.org/fhir/iso-21089-lifecycle/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

iso-21089-lifecycle:11
        a                owl:Class ;
        rdfs:label       "Pseudonymize - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to remove record entry content to reduce the association between a set of identifying data and the data subject in a way that may be reversible" ;
        skos:prefLabel   "Pseudonymize - Lifecycle Event" .

iso-21089-lifecycle:17
        a                owl:Class ;
        rdfs:label       "Deprecate - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to tag record entry(ies) as obsolete, erroneous or untrustworthy, to warn against its future use" ;
        skos:prefLabel   "Deprecate - Lifecycle Event" .

iso-21089-lifecycle:20
        a                owl:Class ;
        rdfs:label       "Unmerge - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to reverse a previous record entry merge operation, rendering them separate again" ;
        skos:prefLabel   "Unmerge - Lifecycle Event" .

iso-21089-lifecycle:2
        a                owl:Class ;
        rdfs:label       "Amend (Update) - Lifeycle Event" ;
        skos:definition  "occurs when an agent makes any change to record entry content currently residing in storage considered permanent (persistent)" ;
        skos:prefLabel   "Amend (Update) - Lifeycle Event" .

iso-21089-lifecycle:26
        a                owl:Class ;
        rdfs:label       "Encrypt - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to encode record entry content in a cipher" ;
        skos:prefLabel   "Encrypt - Lifecycle Event" .

iso-21089-lifecycle:8
        a                owl:Class ;
        rdfs:label       "Transmit - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to send record entry content from one (EHR/PHR/other) system to another" ;
        skos:prefLabel   "Transmit - Lifecycle Event" .

iso-21089-lifecycle:10
        a                owl:Class ;
        rdfs:label       "De-Identify (Anononymize) - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to scrub record entry content to reduce the association between a set of identifying data and the data subject in a way that may or may not be reversible" ;
        skos:prefLabel   "De-Identify (Anononymize) - Lifecycle Event" .

iso-21089-lifecycle:16
        a                owl:Class ;
        rdfs:label       "Destroy/Delete - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to permanently erase record entry content from the system" ;
        skos:prefLabel   "Destroy/Delete - Lifecycle Event" .

iso-21089-lifecycle:25
        a                owl:Class ;
        rdfs:label       "Verify - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to confirm compliance of data or data objects with regulations, requirements, specifications, or other imposed conditions based on organizational policy" ;
        skos:prefLabel   "Verify - Lifecycle Event" .

iso-21089-lifecycle:1
        a                owl:Class ;
        rdfs:label       "Originate/Retain - Record Lifecyle Event" ;
        skos:definition  "occurs when an agent causes the system to:  a) initiate capture of potential record content, and b) incorporate that content into the storage considered a permanent part of the health record" ;
        skos:prefLabel   "Originate/Retain - Record Lifecyle Event" .

iso-21089-lifecycle:7
        a                owl:Class ;
        rdfs:label       "Disclose - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to release, transfer, provision access to, or otherwise divulge record entry content" ;
        skos:prefLabel   "Disclose - Lifecycle Event" .

iso-21089-lifecycle:15
        a                owl:Class ;
        rdfs:label       "Restore - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to recreate record entries and their content from a previous created archive artifact" ;
        skos:prefLabel   "Restore - Lifecycle Event" .

iso-21089-lifecycle:24
        a                owl:Class ;
        rdfs:label       "Remove Legal Hold - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to remove a tag or other cues for special access management had required to fulfill organizational policy under the legal doctrine of â€œduty to preserveâ€" ;
        skos:prefLabel   "Remove Legal Hold - Lifecycle Event" .

iso-21089-lifecycle:6
        a                owl:Class ;
        rdfs:label       "Report (Output) - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to produce and deliver record entry content in a particular form and manner" ;
        skos:prefLabel   "Report (Output) - Lifecycle Event" .

iso-21089-lifecycle:  a  owl:Ontology ;
        rdfs:comment     "Attached is vocabulary for the 27 record lifecycle events, as per ISO TS 21089-2017, Health Informatics - Trusted End-to-End Information Flows, Section 3, Terms and Definitions (2017, at ISO Central Secretariat, passed ballot and ready for publication).  This will also be included in the FHIR EHR Record Lifecycle Event Implementation Guide, balloted and (to be) published with FHIR STU-3." ;
        rdfs:label       "ISO 21089-2017 Health Record Lifecycle Events" ;
        owl:versionIRI   "http://hl7.org/fhir/iso-21089-lifecycle/null" ;
        owl:versionInfo  "ISO 21089-2017 Health Record Lifecycle Events(null)" .

iso-21089-lifecycle:14
        a                owl:Class ;
        rdfs:label       "Archive - Lifeycle Event" ;
        skos:definition  "occurs when an agent causes the system to create and move archive artifacts containing record entry content, typically to long-term offline storage" ;
        skos:prefLabel   "Archive - Lifeycle Event" .

iso-21089-lifecycle:13
        a                owl:Class ;
        rdfs:label       "Extract - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to selectively pull out a subset of record entry content, based on explicit criteria" ;
        skos:prefLabel   "Extract - Lifecycle Event" .

iso-21089-lifecycle:19
        a                owl:Class ;
        rdfs:label       "Merge - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to combine or join content from two or more record entries, resulting in a single logical record entry" ;
        skos:prefLabel   "Merge - Lifecycle Event" .

iso-21089-lifecycle:22
        a                owl:Class ;
        rdfs:label       "Unlink - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to disconnect two or more record entries previously connected, rendering them separate (disconnected) again" ;
        skos:prefLabel   "Unlink - Lifecycle Event" .

iso-21089-lifecycle:4
        a                owl:Class ;
        rdfs:label       "Attest - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to capture the agentâ€™s digital signature (or equivalent indication) during formal validation of record entry content" ;
        skos:prefLabel   "Attest - Lifecycle Event" .

iso-21089-lifecycle:12
        a                owl:Class ;
        rdfs:label       "Re-identify - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to restore information to data that allows identification of information source and/or information subject" ;
        skos:prefLabel   "Re-identify - Lifecycle Event" .

iso-21089-lifecycle:18
        a                owl:Class ;
        rdfs:label       "Re-activate - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to recreate or restore full status to record entries previously deleted or deprecated" ;
        skos:prefLabel   "Re-activate - Lifecycle Event" .

iso-21089-lifecycle:3
        a                owl:Class ;
        rdfs:label       "Transform/Translate - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to change the form, language or code system used to represent record entry content" ;
        skos:prefLabel   "Transform/Translate - Lifecycle Event" .

iso-21089-lifecycle:21
        a                owl:Class ;
        rdfs:label       "Link - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to connect related record entries" ;
        skos:prefLabel   "Link - Lifecycle Event" .

iso-21089-lifecycle:27
        a                owl:Class ;
        rdfs:label       "Decrypt - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to decode record entry content from a cipher" ;
        skos:prefLabel   "Decrypt - Lifecycle Event" .

iso-21089-lifecycle:9
        a                owl:Class ;
        rdfs:label       "Receive/Retain - Lifecycle Event" ;
        skos:definition  "occurs when an agent causes the system to:  a) initiate capture of data content from elseware, and b) incorporate that content into the storage considered a permanent part of the health record" ;
        skos:prefLabel   "Receive/Retain - Lifecycle Event" .
