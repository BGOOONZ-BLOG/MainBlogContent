@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix map-context-type: <http://hl7.org/fhir/map-context-type/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

map-context-type:variable
        a                owl:Class ;
        rdfs:label       "Variable" ;
        skos:definition  "The context specifies a variable" ;
        skos:prefLabel   "Variable" .

map-context-type:type
        a                owl:Class ;
        rdfs:label       "Type" ;
        skos:definition  "The context specifies a type" ;
        skos:prefLabel   "Type" .

map-context-type:  a     owl:Ontology ;
        rdfs:comment     "How to interpret the context" ;
        rdfs:label       "StructureMapContextType" ;
        owl:versionIRI   "http://hl7.org/fhir/map-context-type/3.1.0" ;
        owl:versionInfo  "StructureMapContextType(3.1.0)" .
