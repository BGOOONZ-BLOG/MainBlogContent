@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v3-EmployeeJobClass: <http://hl7.org/fhir/v3/EmployeeJobClass/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-EmployeeJobClass:  a  owl:Ontology ;
        rdfs:comment     "**** MISSING DEFINITIONS ****" ;
        rdfs:label       "v3 Code System EmployeeJobClass" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/EmployeeJobClass/2016-11-11" ;
        owl:versionInfo  "v3 Code System EmployeeJobClass(2016-11-11)" .

v3-EmployeeJobClass:PT
        a                owl:Class ;
        rdfs:label       "part-time" ;
        skos:definition  "Employment in which the employee is expected to work less than a standard work week (defined by the US Bureau of Labor Statistics as 35-44 hours per week)" ;
        skos:prefLabel   "part-time" .

v3-EmployeeJobClass:FT
        a                owl:Class ;
        rdfs:label       "full-time" ;
        skos:definition  "Employment in which the employee is expected to work at least a standard work week (defined by the US Bureau of Labor Statistics as 35-44 hours per week)" ;
        skos:prefLabel   "full-time" .
