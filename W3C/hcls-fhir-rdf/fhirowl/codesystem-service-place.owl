@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix service-place: <http://hl7.org/fhir/ex-serviceplace/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

service-place:04  a      owl:Class ;
        rdfs:label       "Homeless Shelter" ;
        skos:definition  "A facility or location whose primary purpose is to provide temporary housing to homeless individuals (e.g., emergency shelters, individual or family shelters)." ;
        skos:prefLabel   "Homeless Shelter" .

service-place:13  a      owl:Class ;
        rdfs:label       "Assisted Living Fa" ;
        skos:definition  "Congregate residential facility with self-contained living units providing assessment of each resident's needs and on-site support 24 hours a day, 7 days a week, with the capacity to deliver or arrange for services including some health care and other services." ;
        skos:prefLabel   "Assisted Living Fa" .

service-place:19  a      owl:Class ;
        rdfs:label       "Off Campus-Outpatient Hospital" ;
        skos:definition  "portion of an off-campus hospital provider based department which provides diagnostic, therapeutic (both surgical and nonsurgical), and rehabilitation services to sick or injured persons who do not require hospitalization or institutionalization." ;
        skos:prefLabel   "Off Campus-Outpatient Hospital" .

service-place:41  a      owl:Class ;
        rdfs:label       "Ambulance—Land" ;
        skos:definition  "A land vehicle specifically designed, equipped and staffed for lifesaving and transporting the sick or injured." ;
        skos:prefLabel   "Ambulance—Land" .

service-place:  a        owl:Ontology ;
        rdfs:comment     "This value set includes a smattering of Service Place codes." ;
        rdfs:label       "Example Service Place Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/ex-serviceplace/3.1.0" ;
        owl:versionInfo  "Example Service Place Codes(3.1.0)" .

service-place:03  a      owl:Class ;
        rdfs:label       "School" ;
        skos:definition  "A facility whose primary purpose is education." ;
        skos:prefLabel   "School" .

service-place:09  a      owl:Class ;
        rdfs:label       "Prison/Correctional Facility" ;
        skos:definition  "A prison, jail, reformatory, work farm, detention center, or any other similar facility maintained by either Federal, State or local authorities for the purpose of confinement or rehabilitation of adult or juvenile criminal offenders." ;
        skos:prefLabel   "Prison/Correctional Facility" .

service-place:12  a      owl:Class ;
        rdfs:label       "Home" ;
        skos:definition  "Location, other than a hospital or other facility, where the patient receives care in a private residence." ;
        skos:prefLabel   "Home" .

service-place:21  a      owl:Class ;
        rdfs:label       "Inpatient Hospital" ;
        skos:definition  "A facility, other than psychiatric, which primarily provides diagnostic, therapeutic (both surgical and nonsurgical), and rehabilitation services by, or under, the supervision of physicians to patients admitted for a variety of medical conditions." ;
        skos:prefLabel   "Inpatient Hospital" .

service-place:08  a      owl:Class ;
        rdfs:label       "Tribal 638 Provider-Based Facility" ;
        skos:definition  "A facility or location owned and operated by a federally recognized American Indian or Alaska Native tribe or tribal organization under a 638 agreement, which provides diagnostic, therapeutic (surgical and nonsurgical), and rehabilitation services to tribal members admitted as inpatients or outpatients." ;
        skos:prefLabel   "Tribal 638 Provider-Based Facility" .

service-place:11  a      owl:Class ;
        rdfs:label       "Office" ;
        skos:definition  "Location, other than a hospital, skilled nursing facility (SNF), military treatment facility, community health center, State or local public health clinic, or intermediate care facility (ICF), where the health professional routinely provides health examinations, diagnosis, and treatment of illness or injury on an ambulatory basis." ;
        skos:prefLabel   "Office" .

service-place:20  a      owl:Class ;
        rdfs:label       "Urgent Care Facility" ;
        skos:definition  "Location, distinct from a hospital emergency room, an office, or a clinic, whose purpose is to diagnose and treat illness or injury for unscheduled, ambulatory patients seeking immediate medical attention." ;
        skos:prefLabel   "Urgent Care Facility" .

service-place:01  a      owl:Class ;
        rdfs:label       "Pharmacy" ;
        skos:definition  "A facility or location where drugs and other medically related items and services are sold, dispensed, or otherwise provided directly to patients." ;
        skos:prefLabel   "Pharmacy" .

service-place:07  a      owl:Class ;
        rdfs:label       "Tribal 638 Free-Standing Facility" ;
        skos:definition  "A facility or location owned and operated by a federally recognized American Indian or Alaska Native tribe or tribal organization under a 638 agreement, which provides diagnostic, therapeutic (surgical and nonsurgical), and rehabilitation services to tribal members who do not require hospitalization." ;
        skos:prefLabel   "Tribal 638 Free-Standing Facility" .

service-place:06  a      owl:Class ;
        rdfs:label       "Indian Health Service Provider-based Facility" ;
        skos:definition  "A facility or location, owned and operated by the Indian Health Service, which provides diagnostic, therapeutic (surgical and nonsurgical), and rehabilitation services rendered by, or under the supervision of, physicians to American Indians and Alaska Natives admitted as inpatients or outpatients." ;
        skos:prefLabel   "Indian Health Service Provider-based Facility" .

service-place:15  a      owl:Class ;
        rdfs:label       "Mobile Unit" ;
        skos:definition  "A facility/unit that moves from place-to-place equipped to provide preventive, screening, diagnostic, and/or treatment services." ;
        skos:prefLabel   "Mobile Unit" .

service-place:05  a      owl:Class ;
        rdfs:label       "Indian Health Service Free-standing Facility" ;
        skos:definition  "A facility or location, owned and operated by the Indian Health Service, which provides diagnostic, therapeutic (surgical and nonsurgical), and rehabilitation services to American Indians and Alaska Natives who do not require hospitalization." ;
        skos:prefLabel   "Indian Health Service Free-standing Facility" .

service-place:14  a      owl:Class ;
        rdfs:label       "Group Home" ;
        skos:definition  "A residence, with shared living areas, where clients receive supervision and other services such as social and/or behavioral services, custodial service, and minimal services (e.g., medication administration)." ;
        skos:prefLabel   "Group Home" .
