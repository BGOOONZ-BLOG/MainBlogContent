@prefix goal-status-reason: <http://hl7.org/fhir/goal-status-reason/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

goal-status-reason:patient-request
        a                owl:Class ;
        rdfs:label       "Patient Request" ;
        skos:definition  "Patient wishes the goal to be set aside, at least temporarily." ;
        skos:prefLabel   "Patient Request" .

goal-status-reason:permanent-not-attainable
        a                owl:Class ;
        rdfs:label       "Goal Not Attainable Permanently" ;
        skos:definition  "Goal can not be reached permanently." ;
        skos:prefLabel   "Goal Not Attainable Permanently" .

goal-status-reason:financial-barrier
        a                owl:Class ;
        rdfs:label       "Financial Reason" ;
        skos:definition  "Goal can not be reached due to financial barrier or reason." ;
        skos:prefLabel   "Financial Reason" .

goal-status-reason:lack-of-transportation
        a                owl:Class ;
        rdfs:label       "Lack Of Transportation" ;
        skos:definition  "Goal can not be reached due to a lack of transportation." ;
        skos:prefLabel   "Lack Of Transportation" .

goal-status-reason:lack-of-social-support
        a                owl:Class ;
        rdfs:label       "Lack Of Social Support" ;
        skos:definition  "Goal can not be reached due to a lack of social support." ;
        skos:prefLabel   "Lack Of Social Support" .

goal-status-reason:  a   owl:Ontology ;
        rdfs:comment     "Example codes indicating the reason for a current status.  Note that these are in no way complete and may not even be appropriate for some uses." ;
        rdfs:label       "GoalStatusReason" ;
        owl:versionIRI   "http://hl7.org/fhir/goal-status-reason/3.1.0" ;
        owl:versionInfo  "GoalStatusReason(3.1.0)" .

goal-status-reason:replaced
        a                owl:Class ;
        rdfs:label       "Replaced" ;
        skos:definition  "Goal has been superseded by a new goal." ;
        skos:prefLabel   "Replaced" .

goal-status-reason:temp-not-attainable
        a                owl:Class ;
        rdfs:label       "Goal Not Attainable Temporarily" ;
        skos:definition  "Goal can not be reached temporarily." ;
        skos:prefLabel   "Goal Not Attainable Temporarily" .

goal-status-reason:surgery
        a                owl:Class ;
        rdfs:label       "Surgery" ;
        skos:definition  "Goal suspended or ended because of a surgical procedure." ;
        skos:prefLabel   "Surgery" .

goal-status-reason:life-event
        a                owl:Class ;
        rdfs:label       "Life Event" ;
        skos:definition  "Goal suspended or ended because of a significant life event (marital change, bereavement, etc.)." ;
        skos:prefLabel   "Life Event" .
