@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .
@prefix endpoint-connection-type: <http://hl7.org/fhir/endpoint-connection-type/> .

endpoint-connection-type:hl7v2-mllp
        a                owl:Class ;
        rdfs:label       "HL7 v2 MLLP" ;
        skos:definition  "HL7v2 messages over an LLP TCP connection" ;
        skos:prefLabel   "HL7 v2 MLLP" .

endpoint-connection-type:secure-email
        a                owl:Class ;
        rdfs:label       "Secure email" ;
        skos:definition  "Email delivery using a digital certificate to encrypt the content using the public key, receiver must have the private key to decrypt the content" ;
        skos:prefLabel   "Secure email" .

endpoint-connection-type:ihe-iid
        a                owl:Class ;
        rdfs:label       "IHE IID" ;
        skos:definition  "IHE Invoke Image Display (IID) - http://wiki.ihe.net/index.php/Invoke_Image_Display" ;
        skos:prefLabel   "IHE IID" .

endpoint-connection-type:dicom-stow-rs
        a                owl:Class ;
        rdfs:label       "DICOM STOW-RS" ;
        skos:definition  "DICOMweb RESTful image sending and storage - http://dicom.nema.org/medical/dicom/current/output/chtml/part18/sect_6.6.html" ;
        skos:prefLabel   "DICOM STOW-RS" .

endpoint-connection-type:ihe-xdr
        a                owl:Class ;
        rdfs:label       "IHE XDR" ;
        skos:definition  "IHE Cross-Enterprise Document Reliable Exchange (XDR) - http://wiki.ihe.net/index.php/Cross-enterprise_Document_Reliable_Interchange" ;
        skos:prefLabel   "IHE XDR" .

endpoint-connection-type:dicom-wado-rs
        a                owl:Class ;
        rdfs:label       "DICOM WADO-RS" ;
        skos:definition  "DICOMweb RESTful Image Retrieve - http://dicom.nema.org/medical/dicom/current/output/chtml/part18/sect_6.5.html" ;
        skos:prefLabel   "DICOM WADO-RS" .

endpoint-connection-type:
        a                owl:Ontology ;
        rdfs:comment     "This is an example value set defined by the FHIR project, that could be used to represent possible connection type profile values." ;
        rdfs:label       "Endpoint Connection Type" ;
        owl:versionIRI   "http://hl7.org/fhir/endpoint-connection-type/3.1.0" ;
        owl:versionInfo  "Endpoint Connection Type(3.1.0)" .

endpoint-connection-type:dicom-wado-uri
        a                owl:Class ;
        rdfs:label       "DICOM WADO-URI" ;
        skos:definition  "DICOMweb Image Retrieve - http://dicom.nema.org/dicom/2013/output/chtml/part18/sect_6.3.html" ;
        skos:prefLabel   "DICOM WADO-URI" .

endpoint-connection-type:hl7-fhir-rest
        a                owl:Class ;
        rdfs:label       "HL7 FHIR" ;
        skos:definition  "Interact with the server interface using FHIR's RESTful interface. For details on its version/capabilities you should connect the the value in Endpoint.address and retrieve the FHIR CapabilityStatement." ;
        skos:prefLabel   "HL7 FHIR" .

endpoint-connection-type:hl7-fhir-msg
        a                owl:Class ;
        rdfs:label       "HL7 FHIR Messaging" ;
        skos:definition  "Use the servers FHIR Messaging interface. Details can be found on the messaging.html page in the FHIR Specification. The FHIR server's base address is specified in the Endpoint.address property." ;
        skos:prefLabel   "HL7 FHIR Messaging" .

endpoint-connection-type:dicom-qido-rs
        a                owl:Class ;
        rdfs:label       "DICOM QIDO-RS" ;
