@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix allerg-intol-substance-exp-risk: <http://hl7.org/fhir/allerg-intol-substance-exp-risk/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

allerg-intol-substance-exp-risk:no-known-reaction-risk
        a                owl:Class ;
        rdfs:label       "No Known Reaction Risk" ;
        skos:definition  "No known risk of allergy or intolerance reaction upon exposure to the specified substance." ;
        skos:prefLabel   "No Known Reaction Risk" .

allerg-intol-substance-exp-risk:
        a                owl:Ontology ;
        rdfs:comment     "The risk of an adverse reaction (allergy or intolerance) for this patient upon exposure to the substance (including pharmaceutical products)." ;
        rdfs:label       "AllergyIntoleranceSubstanceExposureRisk" ;
        owl:versionIRI   "http://hl7.org/fhir/allerg-intol-substance-exp-risk/3.1.0" ;
        owl:versionInfo  "AllergyIntoleranceSubstanceExposureRisk(3.1.0)" .

allerg-intol-substance-exp-risk:known-reaction-risk
        a                owl:Class ;
        rdfs:label       "Known Reaction Risk" ;
        skos:definition  "Known risk of allergy or intolerance reaction upon exposure to the specified substance." ;
        skos:prefLabel   "Known Reaction Risk" .
