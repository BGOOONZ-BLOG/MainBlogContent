@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix v2-0179: <http://hl7.org/fhir/v2/0179/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v2-0179:ER  a           owl:Class ;
        rdfs:label      "Error/Reject conditions only.  Only MFA segments denoting errors must be returned via the application-level acknowledgment for this message" ;
        skos:prefLabel  "Error/Reject conditions only.  Only MFA segments denoting errors must be returned via the application-level acknowledgment for this message" .

v2-0179:SU  a           owl:Class ;
        rdfs:label      "Success.  Only MFA segments denoting success must be returned via the application-level acknowledgment for this message" ;
        skos:prefLabel  "Success.  Only MFA segments denoting success must be returned via the application-level acknowledgment for this message" .

v2-0179:AL  a           owl:Class ;
        rdfs:label      "Always.  All MFA segments (whether denoting errors or not) must be returned via the application-level acknowledgment message" ;
        skos:prefLabel  "Always.  All MFA segments (whether denoting errors or not) must be returned via the application-level acknowledgment message" .

v2-0179:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 0179 ( Response Level)" ;
        rdfs:label       "v2 Response Level" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/0179/2.8.2" ;
        owl:versionInfo  "v2 Response Level(2.8.2)" .

v2-0179:NE  a           owl:Class ;
        rdfs:label      "Never.  No application-level response needed" ;
        skos:prefLabel  "Never.  No application-level response needed" .
