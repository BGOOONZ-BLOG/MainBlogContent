@prefix ex-diagnosistype: <http://hl7.org/fhir/ex-diagnosistype/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

ex-diagnosistype:retrospective
        a                owl:Class ;
        rdfs:label       "Retrospective Diagnosis" ;
        skos:definition  "The labeling of an illness in a specific historical event using modern knowledge, methods and disease classifications." ;
        skos:prefLabel   "Retrospective Diagnosis" .

ex-diagnosistype:admitting
        a                owl:Class ;
        rdfs:label       "Admitting Diagnosis" ;
        skos:definition  "The diagnosis given as the reason why the patient was admitted to the hospital." ;
        skos:prefLabel   "Admitting Diagnosis" .

ex-diagnosistype:self
        a                owl:Class ;
        rdfs:label       "Self Diagnosis" ;
        skos:definition  "A diagnosis determined by the patient." ;
        skos:prefLabel   "Self Diagnosis" .

ex-diagnosistype:nursing
        a                owl:Class ;
        rdfs:label       "Nursing Diagnosis" ;
        skos:definition  "A diagnosis which identifies people's responses to situations in their lives, such as a readiness to change or a willingness to accept assistance." ;
        skos:prefLabel   "Nursing Diagnosis" .

ex-diagnosistype:prenatal
        a                owl:Class ;
        rdfs:label       "Prenatal Diagnosis" ;
        skos:definition  "A diagnosis determined prior to birth." ;
        skos:prefLabel   "Prenatal Diagnosis" .

ex-diagnosistype:laboratory
        a                owl:Class ;
        rdfs:label       "Laboratory Diagnosis" ;
        skos:definition  "A diagnosis based significantly on laboratory reports or test results, rather than the physical examination of the patient." ;
        skos:prefLabel   "Laboratory Diagnosis" .

ex-diagnosistype:remote
        a                owl:Class ;
        rdfs:label       "Remote Diagnosis" ;
        skos:definition  "A diagnosis determined using telemedicine techniques." ;
        skos:prefLabel   "Remote Diagnosis" .

ex-diagnosistype:principal
        a                owl:Class ;
        rdfs:label       "Principal Diagnosis" ;
        skos:definition  "The single medical diagnosis that is most relevant to the patient's chief complaint or need for treatment." ;
        skos:prefLabel   "Principal Diagnosis" .

ex-diagnosistype:clinical
        a                owl:Class ;
        rdfs:label       "Clinical Diagnosis" ;
        skos:definition  "A diagnosis made on the basis of medical signs and patient-reported symptoms, rather than diagnostic tests." ;
        skos:prefLabel   "Clinical Diagnosis" .

ex-diagnosistype:  a     owl:Ontology ;
        rdfs:comment     "This value set includes example Diagnosis Type codes." ;
        rdfs:label       "Example Diagnosis Type Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/ex-diagnosistype/3.1.0" ;
        owl:versionInfo  "Example Diagnosis Type Codes(3.1.0)" .

ex-diagnosistype:discharge
        a                owl:Class ;
        rdfs:label       "Discharge Diagnosis" ;
        skos:definition  "The diagnosis given when the patient is discharged from the hospital." ;
        skos:prefLabel   "Discharge Diagnosis" .

ex-diagnosistype:radiology
        a                owl:Class ;
        rdfs:label       "Radiology Diagnosis" ;
        skos:definition  "A diagnosis based primarily on the results from medical imaging studies." ;
        skos:prefLabel   "Radiology Diagnosis" .

ex-diagnosistype:differential
        a                owl:Class ;
        rdfs:label       "Differential Diagnosis" ;
        skos:definition  "One of a set of the possible diagnoses that could be connected to the signs, symptoms, and lab findings." ;
        skos:prefLabel   "Differential Diagnosis" .
