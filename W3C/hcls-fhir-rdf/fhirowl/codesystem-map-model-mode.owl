@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix map-model-mode: <http://hl7.org/fhir/map-model-mode/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

map-model-mode:source
        a                owl:Class ;
        rdfs:label       "Source Structure Definition" ;
        skos:definition  "This structure describes an instance passed to the mapping engine that is used a source of data" ;
        skos:prefLabel   "Source Structure Definition" .

map-model-mode:target
        a                owl:Class ;
        rdfs:label       "Target Structure Definition" ;
        skos:definition  "This structure describes an instance passed to the mapping engine that is used a target of data" ;
        skos:prefLabel   "Target Structure Definition" .

map-model-mode:queried
        a                owl:Class ;
        rdfs:label       "Queried Structure Definition" ;
        skos:definition  "This structure describes an instance that the mapping engine may ask for that is used a source of data" ;
        skos:prefLabel   "Queried Structure Definition" .

map-model-mode:produced
        a                owl:Class ;
        rdfs:label       "Produced Structure Definition" ;
        skos:definition  "This structure describes an instance that the mapping engine may ask to create that is used a target of data" ;
        skos:prefLabel   "Produced Structure Definition" .

map-model-mode:  a       owl:Ontology ;
        rdfs:comment     "How the referenced structure is used in this mapping" ;
        rdfs:label       "StructureMapModelMode" ;
        owl:versionIRI   "http://hl7.org/fhir/map-model-mode/3.1.0" ;
        owl:versionInfo  "StructureMapModelMode(3.1.0)" .
