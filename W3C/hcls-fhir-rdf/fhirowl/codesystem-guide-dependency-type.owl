@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix guide-dependency-type: <http://hl7.org/fhir/guide-dependency-type/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

guide-dependency-type:inclusion
        a                owl:Class ;
        rdfs:label       "Inclusion" ;
        skos:definition  "The guide is embedded in this guide when published." ;
        skos:prefLabel   "Inclusion" .

guide-dependency-type:reference
        a                owl:Class ;
        rdfs:label       "Reference" ;
        skos:definition  "The guide is referred to by URL." ;
        skos:prefLabel   "Reference" .

guide-dependency-type:
        a                owl:Ontology ;
        rdfs:comment     "How a dependency is represented when the guide is published." ;
        rdfs:label       "GuideDependencyType" ;
        owl:versionIRI   "http://hl7.org/fhir/guide-dependency-type/3.1.0" ;
        owl:versionInfo  "GuideDependencyType(3.1.0)" .
