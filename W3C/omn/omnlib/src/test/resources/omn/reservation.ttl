@prefix : <http://open-multinet.info/ontology/examples#> .
@prefix omn: <https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix motor: <http://fiteagle.org/ontology/resource/motor#> .
@prefix omn-lifecycle: <https://github.com/w3c/omn/blob/master/omnlib/ontologies/omn-lifecycle#> .
@base <http://open-multinet.info/ontology/example#> .

<http://open-multinet.info/ontology/example#reservation> rdf:type owl:Ontology ;
                                                         
                                                         rdfs:comment "Example on how to make a reservation"@en .


#################################################################
#
#    Individuals
#
#################################################################


###  http://open-multinet.info/ontology/examples#res1

:reservation1 rdf:type omn:Reservation ,
               owl:NamedIndividual ,
               time:Interval ;
      
      omn:isReservationOf :unboundmotor1 ;
      
      omn-lifecycle:hasState omn-lifecycle:Allocated ;
       
      time:hasEnd :reservation1endtime .

:reservation1endtime a time:Instant ;
    time:xsdDateTime   "2015-01-20T17:53:24+01:00"^^xsd:dateTime.


###  http://open-multinet.info/ontology/examples#res2

:reservation2 rdf:type omn:Reservation ,
               owl:NamedIndividual ,
               time:Interval ;
      
      omn:isReservationOf :unboundmotor2 ;
      
      omn-lifecycle:hasState omn-lifecycle:Allocated ;
      
      time:hasEnd :reservation2endtime .

:reservation2endtime a time:Instant ;
    time:xsdDateTime   "2015-01-20T19:20:01+01:00"^^xsd:dateTime.



###  http://open-multinet.info/ontology/examples#reservationRequest

:reservationRequest rdf:type omn:Topology ,
                             omn-lifecycle:Request ,
                             owl:NamedIndividual ;
                    
                    omn:hasResource :unboundmotor1 ,
                                    :unboundmotor2 .



###  http://open-multinet.info/ontology/examples#unboundmotor1

:unboundmotor1 rdf:type motor:Motor ,
                        omn:Resource ,
                        owl:NamedIndividual ;
               
               motor:rpm "0"^^xsd:long ;
               
               motor:throttle "0"^^xsd:long ;
               
               motor:maxRPM "3000"^^xsd:long ;
               
               motor:vendor "Fraunhofer FOKUS"^^xsd:string ;
               
               motor:isDynamic "false"^^xsd:boolean ;
               
               omn:hasReservation :reservation1 ;
               
               omn:isResourceOf :reservationRequest .



###  http://open-multinet.info/ontology/examples#unboundmotor2

:unboundmotor2 rdf:type motor:Motor ,
                        omn:Resource ,
                        owl:NamedIndividual ;
               
               motor:throttle "0"^^xsd:long ;
               
               motor:rpm "0"^^xsd:long ;
              
               motor:maxRPM "2000"^^xsd:long ;
               
               motor:vendor "Fraunhofer FOKUS"^^xsd:string ;
               
               motor:isDynamic "true"^^xsd:boolean ;
               
               omn:hasReservation :reservation2 ;
               
               omn:isResourceOf :reservationRequest .




