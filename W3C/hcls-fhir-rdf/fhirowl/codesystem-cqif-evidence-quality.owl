@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix cqif-evidence-quality: <http://hl7.org/fhir/evidence-quality/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

cqif-evidence-quality:moderate
        a                owl:Class ;
        rdfs:label       "Moderate quality" ;
        skos:definition  "Moderate quality evidence" ;
        skos:prefLabel   "Moderate quality" .

cqif-evidence-quality:low
        a                owl:Class ;
        rdfs:label       "Low quality" ;
        skos:definition  "Low quality evidence" ;
        skos:prefLabel   "Low quality" .

cqif-evidence-quality:
        a                owl:Ontology ;
        rdfs:comment     "A rating system that describes the quality of evidence such as the GRADE, DynaMed, or Oxford CEBM systems" ;
        rdfs:label       "QualityOfEvidenceRating" ;
        owl:versionIRI   "http://hl7.org/fhir/evidence-quality/3.1.0" ;
        owl:versionInfo  "QualityOfEvidenceRating(3.1.0)" .

cqif-evidence-quality:high
        a                owl:Class ;
        rdfs:label       "High quality" ;
        skos:definition  "High quality evidence" ;
        skos:prefLabel   "High quality" .

cqif-evidence-quality:very-low
        a                owl:Class ;
        rdfs:label       "Very low quality" ;
        skos:definition  "Very low quality evidence" ;
        skos:prefLabel   "Very low quality" .
