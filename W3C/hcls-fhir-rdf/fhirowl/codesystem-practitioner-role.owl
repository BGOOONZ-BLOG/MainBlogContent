@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix practitioner-role: <http://hl7.org/fhir/practitioner-role/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

practitioner-role:pharmacist
        a                owl:Class ;
        rdfs:label       "Pharmacist" ;
        skos:definition  "A qualified/registered/licensed pharmacist" ;
        skos:prefLabel   "Pharmacist" .

practitioner-role:doctor
        a                owl:Class ;
        rdfs:label       "Doctor" ;
        skos:definition  "A qualified/registered medical practitioner" ;
        skos:prefLabel   "Doctor" .

practitioner-role:teacher
        a                owl:Class ;
        rdfs:label       "Teacher/educator" ;
        skos:definition  "Someone who is able to provide educational services" ;
        skos:prefLabel   "Teacher/educator" .

practitioner-role:ict
        a                owl:Class ;
        rdfs:label       "ICT professional" ;
        skos:definition  "Someone who is qualified in Information and Communication Technologies" ;
        skos:prefLabel   "ICT professional" .

practitioner-role:researcher
        a                owl:Class ;
        rdfs:label       "Researcher" ;
        skos:definition  "A practitioner that may perform research" ;
        skos:prefLabel   "Researcher" .

practitioner-role:nurse
        a                owl:Class ;
        rdfs:label       "Nurse" ;
        skos:definition  "A practitoner with nursing experience that may be qualified/registered" ;
        skos:prefLabel   "Nurse" .

practitioner-role:  a    owl:Ontology ;
        rdfs:comment     "This example value set defines a set of codes that can be used to indicate the role of a Practitioner." ;
        rdfs:label       "PractitionerRole" ;
        owl:versionIRI   "http://hl7.org/fhir/practitioner-role/3.1.0" ;
        owl:versionInfo  "PractitionerRole(3.1.0)" .
