@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix variant-state: <http://hl7.org/fhir/variant-state/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

variant-state:positive
        a                owl:Class ;
        rdfs:label       "positive" ;
        skos:definition  "the variant is detected" ;
        skos:prefLabel   "positive" .

variant-state:  a        owl:Ontology ;
        rdfs:comment     "Codes providing the status of the variant test result" ;
        rdfs:label       "sequenceStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/variant-state/3.1.0" ;
        owl:versionInfo  "sequenceStatus(3.1.0)" .

variant-state:negative
        a                owl:Class ;
        rdfs:label       "negative" ;
        skos:definition  "no variant is detected" ;
        skos:prefLabel   "negative" .

variant-state:absent  a  owl:Class ;
        rdfs:label       "absent" ;
        skos:definition  "result of the variant is missing" ;
        skos:prefLabel   "absent" .
