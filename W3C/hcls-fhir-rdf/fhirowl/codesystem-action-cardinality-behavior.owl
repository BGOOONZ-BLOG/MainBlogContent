@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix action-cardinality-behavior: <http://hl7.org/fhir/action-cardinality-behavior/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

action-cardinality-behavior:single
        a                owl:Class ;
        rdfs:label       "Single" ;
        skos:definition  "The action may only be selected one time" ;
        skos:prefLabel   "Single" .

action-cardinality-behavior:
        a                owl:Ontology ;
        rdfs:comment     "Defines behavior for an action or a group for how many times that item may be repeated" ;
        rdfs:label       "ActionCardinalityBehavior" ;
        owl:versionIRI   "http://hl7.org/fhir/action-cardinality-behavior/3.1.0" ;
        owl:versionInfo  "ActionCardinalityBehavior(3.1.0)" .

action-cardinality-behavior:multiple
        a                owl:Class ;
        rdfs:label       "Multiple" ;
        skos:definition  "The action may be selected multiple times" ;
        skos:prefLabel   "Multiple" .
