@prefix transaction-mode: <http://hl7.org/fhir/transaction-mode/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

transaction-mode:both
        a                owl:Class ;
        rdfs:label       "Batches & Transactions" ;
        skos:definition  "Both batches and transactions are supported." ;
        skos:prefLabel   "Batches & Transactions" .

transaction-mode:not-supported
        a                owl:Class ;
        rdfs:label       "None" ;
        skos:definition  "Neither batch or transaction is supported." ;
        skos:prefLabel   "None" .

transaction-mode:batch
        a                owl:Class ;
        rdfs:label       "Batches supported" ;
        skos:definition  "Batches are  supported." ;
        skos:prefLabel   "Batches supported" .

transaction-mode:  a     owl:Ontology ;
        rdfs:comment     "A code that indicates how transactions are supported." ;
        rdfs:label       "TransactionMode" ;
        owl:versionIRI   "http://hl7.org/fhir/transaction-mode/3.1.0" ;
        owl:versionInfo  "TransactionMode(3.1.0)" .

transaction-mode:transaction
        a                owl:Class ;
        rdfs:label       "Transactions Supported" ;
        skos:definition  "Transactions are supported." ;
        skos:prefLabel   "Transactions Supported" .
