@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix v3-CommunicationFunctionType: <http://hl7.org/fhir/v3/CommunicationFunctionType/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-CommunicationFunctionType:SND
        a                owl:Class ;
        rdfs:label       "sender" ;
        skos:definition  "The entity is the sender of the transmission." ;
        skos:prefLabel   "sender" .

v3-CommunicationFunctionType:RSP
        a                owl:Class ;
        rdfs:label       "respond to" ;
        skos:definition  "The entity is the one to which the response or reply to the transmission should be sent." ;
        skos:prefLabel   "respond to" .

v3-CommunicationFunctionType:
        a                owl:Ontology ;
        rdfs:comment     " Describes the type of communication function that the associated entity plays in the associated transmission." ;
        rdfs:label       "v3 Code System CommunicationFunctionType" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/CommunicationFunctionType/2016-11-11" ;
        owl:versionInfo  "v3 Code System CommunicationFunctionType(2016-11-11)" .

v3-CommunicationFunctionType:RCV
        a                owl:Class ;
        rdfs:label       "receiver" ;
        skos:definition  "The entity is the receiver of the transmission." ;
        skos:prefLabel   "receiver" .
