@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix name-assembly-order: <http://hl7.org/fhir/name-assembly-order/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

name-assembly-order:NL1
        a               owl:Class ;
        rdfs:label      "Own Name" ;
        skos:prefLabel  "Own Name" .

name-assembly-order:NL4
        a               owl:Class ;
        rdfs:label      "Own Name followed by Partner Name" ;
        skos:prefLabel  "Own Name followed by Partner Name" .

name-assembly-order:NL2
        a               owl:Class ;
        rdfs:label      "Partner Name" ;
        skos:prefLabel  "Partner Name" .

name-assembly-order:NL3
        a               owl:Class ;
        rdfs:label      "Partner Name followed by Maiden Name" ;
        skos:prefLabel  "Partner Name followed by Maiden Name" .

name-assembly-order:  a  owl:Ontology ;
        rdfs:comment     "A code that represents the preferred display order of the components of a human name" ;
        rdfs:label       "HumanNameAssemblyOrder" ;
        owl:versionIRI   "http://hl7.org/fhir/name-assembly-order/3.1.0" ;
        owl:versionInfo  "HumanNameAssemblyOrder(3.1.0)" .
