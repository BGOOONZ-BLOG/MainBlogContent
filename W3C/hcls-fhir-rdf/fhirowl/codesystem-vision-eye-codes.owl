@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix vision-eye-codes: <http://hl7.org/fhir/vision-eye-codes/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

vision-eye-codes:  a     owl:Ontology ;
        rdfs:comment     "A coded concept listing the eye codes." ;
        rdfs:label       "VisionEyes" ;
        owl:versionIRI   "http://hl7.org/fhir/vision-eye-codes/3.1.0" ;
        owl:versionInfo  "VisionEyes(3.1.0)" .

vision-eye-codes:left
        a                owl:Class ;
        rdfs:label       "Left Eye" ;
        skos:definition  "Left Eye" ;
        skos:prefLabel   "Left Eye" .

vision-eye-codes:right
        a                owl:Class ;
        rdfs:label       "Right Eye" ;
        skos:definition  "Right Eye" ;
        skos:prefLabel   "Right Eye" .
