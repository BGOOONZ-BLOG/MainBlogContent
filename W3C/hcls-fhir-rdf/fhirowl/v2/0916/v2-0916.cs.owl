        owl:versionInfo  "v2 Relevant Clincial Information(2.8.2)" .

v2-0916:NG  a           owl:Class ;
        rdfs:label      "Not Given - Patient was not asked at the time of the procedure." ;
        skos:prefLabel  "Not Given - Patient was not asked at the time of the procedure." .

v2-0916:FNA  a          owl:Class ;
        rdfs:label      "Fasting not asked of the patient at time of procedure." ;
        skos:prefLabel  "Fasting not asked of the patient at time of procedure." .

v2-0916:NF  a           owl:Class ;
