@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix namingsystem-type: <http://hl7.org/fhir/namingsystem-type/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

namingsystem-type:codesystem
        a                owl:Class ;
        rdfs:label       "Code System" ;
        skos:definition  "The naming system is used to define concepts and symbols to represent those concepts; e.g. UCUM, LOINC, NDC code, local lab codes, etc." ;
        skos:prefLabel   "Code System" .

namingsystem-type:identifier
        a                owl:Class ;
        rdfs:label       "Identifier" ;
        skos:definition  "The naming system is used to manage identifiers (e.g. license numbers, order numbers, etc.)." ;
        skos:prefLabel   "Identifier" .

namingsystem-type:  a    owl:Ontology ;
        rdfs:comment     "Identifies the purpose of the naming system." ;
        rdfs:label       "NamingSystemType" ;
        owl:versionIRI   "http://hl7.org/fhir/namingsystem-type/3.1.0" ;
        owl:versionInfo  "NamingSystemType(3.1.0)" .

namingsystem-type:root
        a                owl:Class ;
        rdfs:label       "Root" ;
        skos:definition  "The naming system is used as the root for other identifiers and naming systems." ;
        skos:prefLabel   "Root" .
