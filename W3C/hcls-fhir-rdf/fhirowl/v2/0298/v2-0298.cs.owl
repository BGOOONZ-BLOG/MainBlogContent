v2-0298:P  a            owl:Class ;
        rdfs:label      "Pro-rate. Apply this price to this interval, pro-rated by whatever portion of the interval has occurred/been consumed" ;
        skos:prefLabel  "Pro-rate. Apply this price to this interval, pro-rated by whatever portion of the interval has occurred/been consumed" .

v2-0298:F  a            owl:Class ;
        rdfs:label      "Flat-rate. Apply the entire price to this interval, do not pro-rate the price if the full interval has not occurred/been consumed" ;
        skos:prefLabel  "Flat-rate. Apply the entire price to this interval, do not pro-rate the price if the full interval has not occurred/been consumed" .

v2-0298:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 0298 ( CP Range Type)" ;
        rdfs:label       "v2 CP Range Type" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/0298/2.8.2" ;
        owl:versionInfo  "v2 CP Range Type(2.8.2)" .
