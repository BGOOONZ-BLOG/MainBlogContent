@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix document-reference-status: <http://hl7.org/fhir/document-reference-status/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

document-reference-status:current
        a                owl:Class ;
        rdfs:label       "Current" ;
        skos:definition  "This is the current reference for this document." ;
        skos:prefLabel   "Current" .

document-reference-status:
        a                owl:Ontology ;
        rdfs:comment     "The status of the document reference." ;
        rdfs:label       "DocumentReferenceStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/document-reference-status/3.1.0" ;
        owl:versionInfo  "DocumentReferenceStatus(3.1.0)" .

document-reference-status:entered-in-error
        a                owl:Class ;
        rdfs:label       "Entered in Error" ;
        skos:definition  "This reference was created in error." ;
        skos:prefLabel   "Entered in Error" .

document-reference-status:superseded
        a                owl:Class ;
        rdfs:label       "Superseded" ;
        skos:definition  "This reference has been superseded by another reference." ;
        skos:prefLabel   "Superseded" .
