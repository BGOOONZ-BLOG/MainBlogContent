	$$PRS $(ALL_AMAYA_OPTIONS) -DLINK_NUMBER -DPAGE HTMLP HTMLPLP ;\
	$$PRS $(ALL_AMAYA_OPTIONS) -DLINK_NUMBER -DPAGE -DLANDSCAPE HTMLP  HTMLPLL ;\
	$$PRS $(ALL_AMAYA_OPTIONS) -DLINK_NUMBER -DUS_PAPER HTMLP HTMLPLPUS ;\
