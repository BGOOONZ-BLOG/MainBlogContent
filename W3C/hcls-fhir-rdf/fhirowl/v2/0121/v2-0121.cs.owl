        skos:prefLabel  "Same as E, also Replacement and Parent-Child" .

v2-0121:F  a            owl:Class ;
        rdfs:label      "Same as D, plus confirmations explicitly" ;
        skos:prefLabel  "Same as D, plus confirmations explicitly" .

v2-0121:D  a            owl:Class ;
        rdfs:label      "Same as R, also other associated segments" ;
