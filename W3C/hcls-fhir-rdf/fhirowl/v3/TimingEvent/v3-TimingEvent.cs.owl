@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v3-TimingEvent: <http://hl7.org/fhir/v3/TimingEvent/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-TimingEvent:PCD  a    owl:Class ;
        rdfs:label       "PCD" ;
        skos:definition  "after lunch (from lat. post cibus diurnus)" ;
        skos:prefLabel   "PCD" .

v3-TimingEvent:ICV  a    owl:Class ;
        rdfs:label       "ICV" ;
        skos:definition  "between dinner and the hour of sleep" ;
        skos:prefLabel   "ICV" .

v3-TimingEvent:PCV  a    owl:Class ;
        rdfs:label       "PCV" ;
        skos:definition  "after dinner (from lat. post cibus vespertinus)" ;
        skos:prefLabel   "PCV" .

v3-TimingEvent:ACD  a    owl:Class ;
        rdfs:label       "ACD" ;
        skos:definition  "before lunch (from lat. ante cibus diurnus)" ;
        skos:prefLabel   "ACD" .

v3-TimingEvent:PC  a     owl:Class ;
        rdfs:label       "PC" ;
        skos:definition  "after meal (from lat. post cibus)" ;
        skos:prefLabel   "PC" .

v3-TimingEvent:ACV  a    owl:Class ;
        rdfs:label       "ACV" ;
        skos:definition  "before dinner (from lat. ante cibus vespertinus)" ;
        skos:prefLabel   "ACV" .

v3-TimingEvent:CD  a     owl:Class ;
        rdfs:label       "CD" ;
        rdfs:subClassOf  v3-TimingEvent:C ;
        skos:definition  "Description: lunch (from lat. cibus diurnus)" ;
        skos:prefLabel   "CD" .

v3-TimingEvent:HS  a     owl:Class ;
        rdfs:label       "HS" ;
        skos:definition  "Description: Prior to beginning a regular period of extended sleep (this would exclude naps).  Note that this might occur at different times of day depending on a person's regular sleep schedule." ;
        skos:prefLabel   "HS" .

v3-TimingEvent:CV  a     owl:Class ;
        rdfs:label       "CV" ;
        rdfs:subClassOf  v3-TimingEvent:C ;
        skos:definition  "Description: dinner (from lat. cibus vespertinus)" ;
        skos:prefLabel   "CV" .

v3-TimingEvent:  a       owl:Ontology ;
        rdfs:comment     "**** MISSING DEFINITIONS ****" ;
        rdfs:label       "v3 Code System TimingEvent" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/TimingEvent/2016-11-11" ;
        owl:versionInfo  "v3 Code System TimingEvent(2016-11-11)" .

v3-TimingEvent:ICM  a    owl:Class ;
        rdfs:label       "ICM" ;
        skos:definition  "between breakfast and lunch" ;
        skos:prefLabel   "ICM" .

v3-TimingEvent:WAKE  a   owl:Class ;
        rdfs:label       "WAKE" ;
        skos:definition  "Description: Upon waking up from a regular period of sleep, in order to start regular activities (this would exclude waking up from a nap or temporarily waking up during a period of sleep)\r\n\n                        \n                           Usage Notes: e.g.\r\n\n                        Take pulse rate on waking in management of thyrotoxicosis.\r\n\n                        Take BP on waking in management of hypertension\r\n\n                        Take basal body temperature on waking in establishing date of ovulation" ;
        skos:prefLabel   "WAKE" .

v3-TimingEvent:PCM  a    owl:Class ;
        rdfs:label       "PCM" ;
        skos:definition  "after breakfast (from lat. post cibus matutinus)" ;
        skos:prefLabel   "PCM" .

v3-TimingEvent:C  a      owl:Class ;
        rdfs:label       "C" ;
        skos:definition  "Description: meal (from lat. ante cibus)" ;
        skos:prefLabel   "C" .

v3-TimingEvent:ACM  a    owl:Class ;
        rdfs:label       "ACM" ;
        skos:definition  "before breakfast (from lat. ante cibus matutinus)" ;
        skos:prefLabel   "ACM" .

v3-TimingEvent:AC  a     owl:Class ;
        rdfs:label       "AC" ;
        skos:definition  "before meal (from lat. ante cibus)" ;
        skos:prefLabel   "AC" .

v3-TimingEvent:CM  a     owl:Class ;
        rdfs:label       "CM" ;
        rdfs:subClassOf  v3-TimingEvent:C ;
        skos:definition  "Description: breakfast (from lat. cibus matutinus)" ;
        skos:prefLabel   "CM" .

v3-TimingEvent:IC  a     owl:Class ;
        rdfs:label       "IC" ;
        skos:definition  "between meals (from lat. inter cibus)" ;
        skos:prefLabel   "IC" .

v3-TimingEvent:ICD  a    owl:Class ;
        rdfs:label       "ICD" ;
        skos:definition  "between lunch and dinner" ;
        skos:prefLabel   "ICD" .
