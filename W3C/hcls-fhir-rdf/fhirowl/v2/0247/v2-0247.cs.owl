@prefix w5:    <http://hl7.org/fhir/w5#> .

v2-0247:R  a            owl:Class ;
        rdfs:label      "Product under recall/corrective action" ;
        skos:prefLabel  "Product under recall/corrective action" .

v2-0247:A  a            owl:Class ;
        rdfs:label      "Evaluation anticipated, but not yet begun" ;
        skos:prefLabel  "Evaluation anticipated, but not yet begun" .

v2-0247:K  a            owl:Class ;
        rdfs:label      "Problem already known, no evaluation necessary" ;
        skos:prefLabel  "Problem already known, no evaluation necessary" .

v2-0247:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 0247 ( Status of Evaluation)" ;
        rdfs:label       "v2 Status of Evaluation" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/0247/2.8.2" ;
        owl:versionInfo  "v2 Status of Evaluation(2.8.2)" .

v2-0247:P  a            owl:Class ;
        rdfs:label      "Evaluation in progress" ;
        skos:prefLabel  "Evaluation in progress" .

v2-0247:U  a            owl:Class ;
        rdfs:label      "Product unavailable for follow up investigation" ;
        skos:prefLabel  "Product unavailable for follow up investigation" .

v2-0247:D  a            owl:Class ;
