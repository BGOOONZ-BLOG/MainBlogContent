@prefix fhir:  <http://hl7.org/fhir/> .
@prefix procedure-progress-status-codes: <http://hl7.org/fhir/procedure-progress-status-code/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

procedure-progress-status-codes:open-incision
        a                owl:Class ;
        rdfs:label       "Open Incision" ;
        skos:definition  "The patient has open incision(s)." ;
        skos:prefLabel   "Open Incision" .

procedure-progress-status-codes:in-recovery-room
        a                owl:Class ;
        rdfs:label       "In Recovery Room" ;
        skos:definition  "The patient is in the recovery room." ;
        skos:prefLabel   "In Recovery Room" .

procedure-progress-status-codes:
        a                owl:Ontology ;
        rdfs:comment     "This value set is provided as an example. The value set to instantiate this attribute should be drawn from a robust terminology code system that consists of or contains concepts to support the procedure performance process." ;
        rdfs:label       "Procedure Progress Status Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/procedure-progress-status-code/3.1.0" ;
        owl:versionInfo  "Procedure Progress Status Codes(3.1.0)" .

procedure-progress-status-codes:prepared
        a                owl:Class ;
        rdfs:label       "Prepared" ;
        skos:definition  "The patient is prepared for a procedure." ;
        skos:prefLabel   "Prepared" .

procedure-progress-status-codes:closed-incision
        a                owl:Class ;
        rdfs:label       "Closed Incision" ;
        skos:definition  "The patient has incision(s) closed." ;
        skos:prefLabel   "Closed Incision" .

procedure-progress-status-codes:in-operating-room
        a                owl:Class ;
        rdfs:label       "In Operating Room" ;
        skos:definition  "A patient is in the Operating Room." ;
        skos:prefLabel   "In Operating Room" .

procedure-progress-status-codes:anesthesia-induced
        a                owl:Class ;
        rdfs:label       "Anesthesia Induced" ;
        skos:definition  "The patient is under anesthesia." ;
        skos:prefLabel   "Anesthesia Induced" .
