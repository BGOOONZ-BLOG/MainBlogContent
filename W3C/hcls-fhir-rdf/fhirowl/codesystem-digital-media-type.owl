@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix digital-media-type: <http://hl7.org/fhir/digital-media-type/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

digital-media-type:video
        a                owl:Class ;
        rdfs:label       "Video" ;
        skos:definition  "The media consists of a series of frames that capture a moving image" ;
        skos:prefLabel   "Video" .

digital-media-type:photo
        a                owl:Class ;
        rdfs:label       "Photo" ;
        skos:definition  "The media consists of one or more unmoving images, including photographs, computer-generated graphs and charts, and scanned documents" ;
        skos:prefLabel   "Photo" .

digital-media-type:  a   owl:Ontology ;
        rdfs:comment     "Whether the media is a photo, video, or audio" ;
        rdfs:label       "DigitalMediaType" ;
        owl:versionIRI   "http://hl7.org/fhir/digital-media-type/3.1.0" ;
        owl:versionInfo  "DigitalMediaType(3.1.0)" .

digital-media-type:audio
        a                owl:Class ;
        rdfs:label       "Audio" ;
        skos:definition  "The media consists of a sound recording" ;
        skos:prefLabel   "Audio" .
