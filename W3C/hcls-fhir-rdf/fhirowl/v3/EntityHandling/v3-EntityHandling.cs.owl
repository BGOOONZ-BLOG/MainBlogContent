@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .
@prefix v3-EntityHandling: <http://hl7.org/fhir/v3/EntityHandling/> .

v3-EntityHandling:REF
        a                owl:Class ;
        rdfs:label       "Refrigerated temperature" ;
        skos:definition  "Keep at refrigerated temperature:4-8C Accidental warming or freezing is of little consequence." ;
        skos:prefLabel   "Refrigerated temperature" .

v3-EntityHandling:NTR
        a                owl:Class ;
        rdfs:label       "nitrogen" ;
        skos:definition  "Keep in liquid nitrogen" ;
        skos:prefLabel   "nitrogen" .

v3-EntityHandling:CATM
        a                owl:Class ;
        rdfs:label       "Protect from Air" ;
        skos:definition  "Critical. Do not expose to atmosphere.  Do not uncap." ;
        skos:prefLabel   "Protect from Air" .

v3-EntityHandling:FRZ
        a                owl:Class ;
        rdfs:label       "frozen" ;
        skos:definition  "Keep frozen below 0 ?C" ;
        skos:prefLabel   "frozen" .

v3-EntityHandling:DFRZ
        a                owl:Class ;
        rdfs:label       "Deep Frozen" ;
        skos:definition  "Deep Frozen -16 to -20C." ;
        skos:prefLabel   "Deep Frozen" .

v3-EntityHandling:MTLF
        a                owl:Class ;
        rdfs:label       "Metal Free" ;
        skos:definition  "Container is free of heavy metals, including lead." ;
        skos:prefLabel   "Metal Free" .

v3-EntityHandling:CREF
        a                owl:Class ;
        rdfs:label       "Critical refrigerated temperature" ;
        skos:definition  "Critical refrigerated - must not be allowed to freeze or warm until imediately prior to testing." ;
        skos:prefLabel   "Critical refrigerated temperature" .

v3-EntityHandling:C37
        a                owl:Class ;
        rdfs:label       "Body Temperature" ;
        skos:definition  "Critical to keep at body temperature 36-38C" ;
        skos:prefLabel   "Body Temperature" .

v3-EntityHandling:PSO
        a                owl:Class ;
        rdfs:label       "no shock" ;
        skos:definition  "Protect against shock" ;
        skos:prefLabel   "no shock" .

v3-EntityHandling:UFRZ
        a                owl:Class ;
        rdfs:label       "Ultra frozen" ;
        skos:definition  "Ultra cold frozen -75 to -85C.  Ultra cold freezer is typically at temperature of dry ice." ;
        skos:prefLabel   "Ultra frozen" .

v3-EntityHandling:CFRZ
        a                owl:Class ;
        rdfs:label       "Critical frozen" ;
        skos:definition  "Critical frozen. Specimen must not be allowed to thaw until immediately prior to testing." ;
        skos:prefLabel   "Critical frozen" .

v3-EntityHandling:  a    owl:Ontology ;
        rdfs:comment     "**** MISSING DEFINITIONS ****" ;
        rdfs:label       "v3 Code System EntityHandling" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/EntityHandling/2016-11-11" ;
        owl:versionInfo  "v3 Code System EntityHandling(2016-11-11)" .

v3-EntityHandling:UPR
        a                owl:Class ;
        rdfs:label       "upright" ;
        skos:definition  "Keep upright, do not turn upside down" ;
        skos:prefLabel   "upright" .

v3-EntityHandling:DRY
        a                owl:Class ;
        rdfs:label       "dry" ;
        skos:definition  "Keep in a dry environment" ;
        skos:prefLabel   "dry" .

v3-EntityHandling:AMB
        a                owl:Class ;
        rdfs:label       "Ambient Temperature" ;
        skos:definition  "Keep at ambient temperature, 22 +/- 2C" ;
        skos:prefLabel   "Ambient Temperature" .

v3-EntityHandling:CAMB
        a                owl:Class ;
        rdfs:label       "Critical Ambient temperature" ;
        skos:definition  "Critical ambient - must not be refrigerated or frozen." ;
        skos:prefLabel   "Critical Ambient temperature" .

v3-EntityHandling:PSA
        a                owl:Class ;
        rdfs:label       "do not shake" ;
        skos:definition  "Do not shake" ;
        skos:prefLabel   "do not shake" .

v3-EntityHandling:SBU
        a                owl:Class ;
        rdfs:label       "Shake before use" ;
        skos:definition  "Shake thoroughly before using" ;
        skos:prefLabel   "Shake before use" .

v3-EntityHandling:PRTL
        a                owl:Class ;
        rdfs:label       "Protect from Light" ;
        skos:definition  "Protect from light (eg. Wrap in aluminum foil)." ;
        skos:prefLabel   "Protect from Light" .
