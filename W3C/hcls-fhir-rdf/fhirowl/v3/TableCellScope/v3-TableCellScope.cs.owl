        rdfs:label       "row" ;
        skos:definition  "row" ;
        skos:prefLabel   "row" .

v3-TableCellScope:  a    owl:Ontology ;
        rdfs:comment     " These values are defined within the XHTML 4.0 Table Model" ;
        rdfs:label       "v3 Code System TableCellScope" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/TableCellScope/2016-11-11" ;
        owl:versionInfo  "v3 Code System TableCellScope(2016-11-11)" .

v3-TableCellScope:colgroup
        a                owl:Class ;
        rdfs:label       "colgroup" ;
        skos:definition  "colgroup" ;
        skos:prefLabel   "colgroup" .

v3-TableCellScope:rowgroup
        a                owl:Class ;
        rdfs:label       "rowgroup" ;
        skos:definition  "rowgroup" ;
        skos:prefLabel   "rowgroup" .

v3-TableCellScope:col
        a                owl:Class ;
        rdfs:label       "col" ;
        skos:definition  "col" ;
        skos:prefLabel   "col" .
