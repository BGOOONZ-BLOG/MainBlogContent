@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix v2-4000: <http://hl7.org/fhir/v2/4000/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v2-4000:A  a            owl:Class ;
        rdfs:label      "Alphabetic (i.e., Default or some single-byte)" ;
        skos:prefLabel  "Alphabetic (i.e., Default or some single-byte)" .

v2-4000:P  a            owl:Class ;
        rdfs:label      "Phonetic (i.e., ASCII, Katakana, Hiragana, etc.)" ;
        skos:prefLabel  "Phonetic (i.e., ASCII, Katakana, Hiragana, etc.)" .

v2-4000:I  a            owl:Class ;
        rdfs:label      "Ideographic (i.e., Kanji)" ;
        skos:prefLabel  "Ideographic (i.e., Kanji)" .

v2-4000:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 4000 ( Name/address representation)" ;
        rdfs:label       "v2 Name/address representation" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/4000/2.8.2" ;
        owl:versionInfo  "v2 Name/address representation(2.8.2)" .
