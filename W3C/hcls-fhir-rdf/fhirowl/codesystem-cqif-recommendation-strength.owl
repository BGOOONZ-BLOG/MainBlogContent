@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .
@prefix cqif-recommendation-strength: <http://hl7.org/fhir/recommendation-strength/> .

cqif-recommendation-strength:strong
        a                owl:Class ;
        rdfs:label       "Strong" ;
        skos:definition  "Strong recommendation" ;
        skos:prefLabel   "Strong" .

cqif-recommendation-strength:
        a                owl:Ontology ;
        rdfs:comment     "A rating system that describes the strength of the recommendation, such as the GRADE, DynaMed, or HGPS systems" ;
        rdfs:label       "StrengthOfRecommendationRating" ;
        owl:versionIRI   "http://hl7.org/fhir/recommendation-strength/3.1.0" ;
        owl:versionInfo  "StrengthOfRecommendationRating(3.1.0)" .

cqif-recommendation-strength:weak
        a                owl:Class ;
        rdfs:label       "Weak" ;
        skos:definition  "Weak recommendation" ;
        skos:prefLabel   "Weak" .
