        skos:prefLabel  "Product discarded -- unable to follow up" .

v2-0247:I  a            owl:Class ;
        rdfs:label      "Product remains implanted -- unable to follow up" ;
        skos:prefLabel  "Product remains implanted -- unable to follow up" .

v2-0247:X  a            owl:Class ;
