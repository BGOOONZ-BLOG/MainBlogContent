@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix marital-status: <http://hl7.org/fhir/marital-status/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

marital-status:U  a      owl:Class ;
        rdfs:label       "Unmarried" ;
        skos:definition  "The person is not presently married. The marital history is not known or stated." ;
        skos:prefLabel   "Unmarried" .

marital-status:  a       owl:Ontology ;
        rdfs:comment     "This value set defines the set of codes that can be used to indicate the marital status of a person." ;
        rdfs:label       "Marital Status Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/marital-status/3.1.0" ;
        owl:versionInfo  "Marital Status Codes(3.1.0)" .
