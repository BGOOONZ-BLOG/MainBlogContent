@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix assert-operator-codes: <http://hl7.org/fhir/assert-operator-codes/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

assert-operator-codes:lessThan
        a                owl:Class ;
        rdfs:label       "lessThan" ;
        skos:definition  "Compare value to be less than a known value." ;
        skos:prefLabel   "lessThan" .

assert-operator-codes:equals
        a                owl:Class ;
        rdfs:label       "equals" ;
        skos:definition  "Default value. Equals comparison." ;
        skos:prefLabel   "equals" .

assert-operator-codes:eval
        a                owl:Class ;
        rdfs:label       "evaluate" ;
        skos:definition  "Evaluate the fluentpath expression as a boolean condition." ;
        skos:prefLabel   "evaluate" .

assert-operator-codes:in
        a                owl:Class ;
        rdfs:label       "in" ;
        skos:definition  "Compare value within a known set of values." ;
        skos:prefLabel   "in" .

assert-operator-codes:notIn
        a                owl:Class ;
        rdfs:label       "notIn" ;
        skos:definition  "Compare value not within a known set of values." ;
        skos:prefLabel   "notIn" .

assert-operator-codes:notEquals
        a                owl:Class ;
        rdfs:label       "notEquals" ;
        skos:definition  "Not equals comparison." ;
        skos:prefLabel   "notEquals" .

assert-operator-codes:greaterThan
        a                owl:Class ;
        rdfs:label       "greaterThan" ;
        skos:definition  "Compare value to be greater than a known value." ;
        skos:prefLabel   "greaterThan" .

assert-operator-codes:contains
        a                owl:Class ;
        rdfs:label       "contains" ;
        skos:definition  "Compare value string contains a known value." ;
        skos:prefLabel   "contains" .

assert-operator-codes:
        a                owl:Ontology ;
        rdfs:comment     "The type of operator to use for assertion." ;
        rdfs:label       "AssertionOperatorType" ;
        owl:versionIRI   "http://hl7.org/fhir/assert-operator-codes/3.1.0" ;
        owl:versionInfo  "AssertionOperatorType(3.1.0)" .

assert-operator-codes:notEmpty
        a                owl:Class ;
        rdfs:label       "notEmpty" ;
        skos:definition  "Compare value is not empty." ;
        skos:prefLabel   "notEmpty" .

assert-operator-codes:empty
        a                owl:Class ;
        rdfs:label       "empty" ;
        skos:definition  "Compare value is empty." ;
        skos:prefLabel   "empty" .

assert-operator-codes:notContains
        a                owl:Class ;
        rdfs:label       "notContains" ;
        skos:definition  "Compare value string does not contain a known value." ;
        skos:prefLabel   "notContains" .
