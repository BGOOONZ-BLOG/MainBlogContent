@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix medication-status: <http://hl7.org/fhir/medication-status/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

medication-status:entered-in-error
        a                owl:Class ;
        rdfs:label       "Entered in Error" ;
        skos:definition  "The medication was entered in error" ;
        skos:prefLabel   "Entered in Error" .

medication-status:  a    owl:Ontology ;
        rdfs:comment     "A coded concept defining if the medication is in active use" ;
        rdfs:label       "MedicationStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/medication-status/3.1.0" ;
        owl:versionInfo  "MedicationStatus(3.1.0)" .

medication-status:inactive
        a                owl:Class ;
        rdfs:label       "Inactive" ;
        skos:definition  "The medication is not available for use" ;
        skos:prefLabel   "Inactive" .

medication-status:active
        a                owl:Class ;
        rdfs:label       "Active" ;
        skos:definition  "The medication is available for use" ;
        skos:prefLabel   "Active" .
