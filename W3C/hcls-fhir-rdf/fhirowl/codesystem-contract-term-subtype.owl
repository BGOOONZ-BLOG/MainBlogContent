@prefix contract-term-subtype: <http://hl7.org/fhir/contracttermsubtypecodes/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

contract-term-subtype:innominate
        a                owl:Class ;
        rdfs:label       "Innominate" ;
        skos:definition  "Breach of which may or may not go to the root of the contract depending upon the nature of the breach" ;
        skos:prefLabel   "Innominate" .

contract-term-subtype:condition
        a                owl:Class ;
        rdfs:label       "Condition" ;
        skos:definition  "Terms that go to the very root of a contract." ;
        skos:prefLabel   "Condition" .

contract-term-subtype:warranty
        a                owl:Class ;
        rdfs:label       "Warranty" ;
        skos:definition  "Less imperative than a condition, so the contract will survive a breach" ;
        skos:prefLabel   "Warranty" .

contract-term-subtype:
        a                owl:Ontology ;
        rdfs:comment     "This value set includes sample Contract Term SubType codes." ;
        rdfs:label       "Contract Term Subtype Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/contracttermsubtypecodes/3.1.0" ;
        owl:versionInfo  "Contract Term Subtype Codes(3.1.0)" .
