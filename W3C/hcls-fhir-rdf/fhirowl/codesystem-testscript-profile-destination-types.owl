@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix testscript-profile-destination-types: <http://hl7.org/fhir/testscript-profile-destination-types/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

testscript-profile-destination-types:FHIR-SDC-FormManager
        a                owl:Class ;
        rdfs:label       "FHIR SDC FormManager" ;
        skos:definition  "A FHIR server acting as a Structured Data Capture Form Manager." ;
        skos:prefLabel   "FHIR SDC FormManager" .

testscript-profile-destination-types:FHIR-Server
        a                owl:Class ;
        rdfs:label       "FHIR Server" ;
        skos:definition  "General FHIR server used to respond to operations sent from a FHIR client." ;
        skos:prefLabel   "FHIR Server" .

testscript-profile-destination-types:FHIR-SDC-FormReceiver
        a                owl:Class ;
        rdfs:label       "FHIR SDC FormReceiver" ;
        skos:definition  "A FHIR server acting as a Structured Data Capture Form Receiver." ;
        skos:prefLabel   "FHIR SDC FormReceiver" .

testscript-profile-destination-types:
        a                owl:Ontology ;
        rdfs:comment     "This value set defines a set of codes that are used to indicate the profile type of a test system when acting as the destination within a TestScript." ;
        rdfs:label       "TestScriptProfileDestinationType" ;
        owl:versionIRI   "http://hl7.org/fhir/testscript-profile-destination-types/3.1.0" ;
        owl:versionInfo  "TestScriptProfileDestinationType(3.1.0)" .

testscript-profile-destination-types:FHIR-SDC-FormProcessor
        a                owl:Class ;
        rdfs:label       "FHIR SDC FormProcessor" ;
        skos:definition  "A FHIR server acting as a Structured Data Capture Form Processor." ;
        skos:prefLabel   "FHIR SDC FormProcessor" .
