@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix search-comparator: <http://hl7.org/fhir/search-comparator/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

search-comparator:eq  a  owl:Class ;
        rdfs:label       "Equals" ;
        skos:definition  "the value for the parameter in the resource is equal to the provided value" ;
        skos:prefLabel   "Equals" .

search-comparator:le  a  owl:Class ;
        rdfs:label       "Less of Equal" ;
        skos:definition  "the value for the parameter in the resource is less or equal to the provided value" ;
        skos:prefLabel   "Less of Equal" .

search-comparator:lt  a  owl:Class ;
        rdfs:label       "Less Then" ;
        skos:definition  "the value for the parameter in the resource is less than the provided value" ;
        skos:prefLabel   "Less Then" .

search-comparator:ne  a  owl:Class ;
        rdfs:label       "Not Equals" ;
        skos:definition  "the value for the parameter in the resource is not equal to the provided value" ;
        skos:prefLabel   "Not Equals" .

search-comparator:ge  a  owl:Class ;
        rdfs:label       "Greater or Equals" ;
        skos:definition  "the value for the parameter in the resource is greater or equal to the provided value" ;
        skos:prefLabel   "Greater or Equals" .

search-comparator:ap  a  owl:Class ;
        rdfs:label       "Approximately" ;
        skos:definition  "the value for the parameter in the resource is approximately the same to the provided value." ;
        skos:prefLabel   "Approximately" .

search-comparator:gt  a  owl:Class ;
        rdfs:label       "Greater Than" ;
        skos:definition  "the value for the parameter in the resource is greater than the provided value" ;
        skos:prefLabel   "Greater Than" .

search-comparator:sa  a  owl:Class ;
        rdfs:label       "Starts After" ;
        skos:definition  "the value for the parameter in the resource starts after the provided value" ;
        skos:prefLabel   "Starts After" .

search-comparator:  a    owl:Ontology ;
        rdfs:comment     "What Search Comparator Codes are supported in search" ;
        rdfs:label       "SearchComparator" ;
        owl:versionIRI   "http://hl7.org/fhir/search-comparator/3.1.0" ;
        owl:versionInfo  "SearchComparator(3.1.0)" .

search-comparator:eb  a  owl:Class ;
        rdfs:label       "Ends Before" ;
        skos:definition  "the value for the parameter in the resource ends before the provided value" ;
        skos:prefLabel   "Ends Before" .
