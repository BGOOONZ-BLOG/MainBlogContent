@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix adverse-event-actuality: <http://hl7.org/fhir/adverse-event-actuality/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

adverse-event-actuality:actual
        a               owl:Class ;
        rdfs:label      "Adverse Event" ;
        skos:prefLabel  "Adverse Event" .

adverse-event-actuality:
        a                owl:Ontology ;
        rdfs:comment     "Overall nature of the event, e.g. real or potential" ;
        rdfs:label       "AdverseEventActuality" ;
        owl:versionIRI   "http://hl7.org/fhir/adverse-event-actuality/3.1.0" ;
        owl:versionInfo  "AdverseEventActuality(3.1.0)" .

adverse-event-actuality:potential
        a               owl:Class ;
        rdfs:label      "Potential Adverse Event" ;
        skos:prefLabel  "Potential Adverse Event" .
