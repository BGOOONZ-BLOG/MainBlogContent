missing-tooth-reason:e
        a                owl:Class ;
        rdfs:label       "E" ;
        skos:definition  "Extraction" ;
        skos:prefLabel   "E" .

missing-tooth-reason:o
        a                owl:Class ;
        rdfs:label       "O" ;
        skos:definition  "Other" ;
        skos:prefLabel   "O" .

missing-tooth-reason:c
        a                owl:Class ;
        rdfs:label       "C" ;
        skos:definition  "Congenital" ;
        skos:prefLabel   "C" .

missing-tooth-reason:u
        a                owl:Class ;
        rdfs:label       "U" ;
        skos:definition  "Unknown" ;
        skos:prefLabel   "U" .

missing-tooth-reason:
        a                owl:Ontology ;
        rdfs:comment     "This value set includes sample Missing Tooth Reason codes." ;
        rdfs:label       "Missing Tooth Reason Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/missingtoothreason/3.1.0" ;
        owl:versionInfo  "Missing Tooth Reason Codes(3.1.0)" .
