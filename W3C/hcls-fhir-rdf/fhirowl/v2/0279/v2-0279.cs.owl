@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix v2-0279: <http://hl7.org/fhir/v2/0279/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v2-0279:Notify  a       owl:Class ;
        rdfs:label      "Notify the Placer Contact Person, through normal institutional procedures, that a substitution of this resource has been made" ;
        skos:prefLabel  "Notify the Placer Contact Person, through normal institutional procedures, that a substitution of this resource has been made" .

v2-0279:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 0279 ( Allow Substitution Codes)" ;
        rdfs:label       "v2 Allow Substitution Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/0279/2.8.2" ;
        owl:versionInfo  "v2 Allow Substitution Codes(2.8.2)" .

v2-0279:Yes  a          owl:Class ;
        rdfs:label      "Substitution of this resource is allowed" ;
        skos:prefLabel  "Substitution of this resource is allowed" .

v2-0279:No  a           owl:Class ;
        rdfs:label      "Substitution of this resource is not allowed" ;
        skos:prefLabel  "Substitution of this resource is not allowed" .

v2-0279:Confirm  a      owl:Class ;
        rdfs:label      "Contact the Placer Contact Person prior to making any substitutions of this resource" ;
        skos:prefLabel  "Contact the Placer Contact Person prior to making any substitutions of this resource" .
