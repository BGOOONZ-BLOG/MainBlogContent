@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix property-representation: <http://hl7.org/fhir/property-representation/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

property-representation:cdaText
        a                owl:Class ;
        rdfs:label       "CDA Text Format" ;
        skos:definition  "Use CDA narrative instead of XHTML" ;
        skos:prefLabel   "CDA Text Format" .

property-representation:xmlText
        a                owl:Class ;
        rdfs:label       "XML Text" ;
        skos:definition  "This element is represented using the XML text attribute (primitives only)" ;
        skos:prefLabel   "XML Text" .

property-representation:xmlAttr
        a                owl:Class ;
        rdfs:label       "XML Attribute" ;
        skos:definition  "In XML, this property is represented as an attribute not an element." ;
        skos:prefLabel   "XML Attribute" .

property-representation:
        a                owl:Ontology ;
        rdfs:comment     "How a property is represented when serialized." ;
        rdfs:label       "PropertyRepresentation" ;
        owl:versionIRI   "http://hl7.org/fhir/property-representation/3.1.0" ;
        owl:versionInfo  "PropertyRepresentation(3.1.0)" .

property-representation:typeAttr
        a                owl:Class ;
        rdfs:label       "Type Attribute" ;
        skos:definition  "The type of this element is indicated using xsi:type" ;
        skos:prefLabel   "Type Attribute" .

property-representation:xhtml
        a                owl:Class ;
        rdfs:label       "XHTML" ;
        skos:definition  "The property is represented using XHTML" ;
        skos:prefLabel   "XHTML" .
