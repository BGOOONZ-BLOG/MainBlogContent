@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix measure-scoring: <http://hl7.org/fhir/measure-scoring/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

measure-scoring:proportion
        a                owl:Class ;
        rdfs:label       "Proportion" ;
        skos:definition  "The measure score is defined using a proportion" ;
        skos:prefLabel   "Proportion" .

measure-scoring:cohort
        a                owl:Class ;
        rdfs:label       "Cohort" ;
        skos:definition  "The measure is a cohort definition" ;
        skos:prefLabel   "Cohort" .

measure-scoring:continuous-variable
        a                owl:Class ;
        rdfs:label       "Continuous Variable" ;
        skos:definition  "The score is defined by a calculation of some quantity" ;
        skos:prefLabel   "Continuous Variable" .

measure-scoring:  a      owl:Ontology ;
        rdfs:comment     "The scoring type of the measure" ;
        rdfs:label       "MeasureScoring" ;
        owl:versionIRI   "http://hl7.org/fhir/measure-scoring/3.1.0" ;
        owl:versionInfo  "MeasureScoring(3.1.0)" .

measure-scoring:ratio
        a                owl:Class ;
        rdfs:label       "Ratio" ;
        skos:definition  "The measure score is defined using a ratio" ;
        skos:prefLabel   "Ratio" .
