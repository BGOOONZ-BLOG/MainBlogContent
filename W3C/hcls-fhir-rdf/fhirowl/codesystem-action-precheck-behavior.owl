@prefix action-precheck-behavior: <http://hl7.org/fhir/action-precheck-behavior/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

action-precheck-behavior:no
        a                owl:Class ;
        rdfs:label       "No" ;
        skos:definition  "An action with this behavior is one of the less frequent actions included by the end user, for the particular context in which the action occurs. The system displaying the actions to the end user would typically not \"pre-check\" such an action" ;
        skos:prefLabel   "No" .

action-precheck-behavior:
        a                owl:Ontology ;
        rdfs:comment     "Defines selection frequency behavior for an action or group" ;
        rdfs:label       "ActionPrecheckBehavior" ;
        owl:versionIRI   "http://hl7.org/fhir/action-precheck-behavior/3.1.0" ;
        owl:versionInfo  "ActionPrecheckBehavior(3.1.0)" .

action-precheck-behavior:yes
        a                owl:Class ;
        rdfs:label       "Yes" ;
        skos:definition  "An action with this behavior is one of the most frequent action that is, or should be, included by an end user, for the particular context in which the action occurs. The system displaying the action to the end user should consider \"pre-checking\" such an action as a convenience for the user" ;
        skos:prefLabel   "Yes" .
