@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .
@prefix policyholder-relationship: <http://hl7.org/fhir/policyholder-relationship/> .

policyholder-relationship:other
        a                owl:Class ;
        rdfs:label       "Other" ;
        skos:definition  "The Beneficiary has some other relationship the Policyholder" ;
        skos:prefLabel   "Other" .

policyholder-relationship:child
        a                owl:Class ;
        rdfs:label       "Child" ;
        skos:definition  "The Beneficiary is a child of the Policyholder" ;
        skos:prefLabel   "Child" .

policyholder-relationship:spouse
        a                owl:Class ;
        rdfs:label       "Spouse" ;
        skos:definition  "The Beneficiary is a spouse or equivalent of the Policyholder" ;
        skos:prefLabel   "Spouse" .

policyholder-relationship:
        a                owl:Ontology ;
        rdfs:comment     "This value set includes codes for the relationship between the Policyholder and the Beneficiary (insured/covered party/patient).." ;
        rdfs:label       "Policyholder Relationship Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/policyholder-relationship/3.1.0" ;
        owl:versionInfo  "Policyholder Relationship Codes(3.1.0)" .

policyholder-relationship:parent
        a                owl:Class ;
        rdfs:label       "Parent" ;
        skos:definition  "The Beneficiary is a parent of the Policyholder" ;
        skos:prefLabel   "Parent" .

policyholder-relationship:common
        a                owl:Class ;
        rdfs:label       "Common Law Spouse" ;
        skos:definition  "The Beneficiary is a common law spouse or equivalent of the Policyholder" ;
        skos:prefLabel   "Common Law Spouse" .

policyholder-relationship:self
        a                owl:Class ;
        rdfs:label       "Self" ;
        skos:definition  "The Beneficiary is the Policyholder" ;
        skos:prefLabel   "Self" .
