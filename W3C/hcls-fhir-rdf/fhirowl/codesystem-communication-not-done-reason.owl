@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix communication-not-done-reason: <http://hl7.org/fhir/communication-not-done-reason/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

communication-not-done-reason:system-error
        a                owl:Class ;
        rdfs:label       "System Error" ;
        skos:definition  "The communication was not done due to a system error." ;
        skos:prefLabel   "System Error" .

communication-not-done-reason:patient-objection
        a                owl:Class ;
        rdfs:label       "Patient Objection" ;
        skos:definition  "The communication was not done due to a patient objection." ;
        skos:prefLabel   "Patient Objection" .

communication-not-done-reason:recipient-unavailable
        a                owl:Class ;
        rdfs:label       "Recipient Unavailable" ;
        skos:definition  "The communication was not done due to the recipient being unavailable." ;
        skos:prefLabel   "Recipient Unavailable" .

communication-not-done-reason:invalid-phone-number
        a                owl:Class ;
        rdfs:label       "Invalid Phone Number" ;
        skos:definition  "The communication was not done due to an invalid phone number." ;
        skos:prefLabel   "Invalid Phone Number" .

communication-not-done-reason:unknown
        a                owl:Class ;
        rdfs:label       "Unknown" ;
        skos:definition  "The communication was not done due to an unknown reason." ;
        skos:prefLabel   "Unknown" .

communication-not-done-reason:
        a                owl:Ontology ;
        rdfs:comment     "Codes for the reason why a communication was not done." ;
        rdfs:label       "CommunicationNotDoneReason" ;
        owl:versionIRI   "http://hl7.org/fhir/communication-not-done-reason/3.1.0" ;
        owl:versionInfo  "CommunicationNotDoneReason(3.1.0)" .

communication-not-done-reason:family-objection
        a                owl:Class ;
        rdfs:label       "Family Objection" ;
        skos:definition  "The communication was not done due to a family objection." ;
        skos:prefLabel   "Family Objection" .
