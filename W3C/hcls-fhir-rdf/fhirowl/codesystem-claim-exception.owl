@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix claim-exception: <http://hl7.org/fhir/claim-exception/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

claim-exception:  a      owl:Ontology ;
        rdfs:comment     "This value set includes sample Exception codes." ;
        rdfs:label       "Exception Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/claim-exception/3.1.0" ;
        owl:versionInfo  "Exception Codes(3.1.0)" .

claim-exception:student
        a                owl:Class ;
        rdfs:label       "Student (Fulltime)" ;
        skos:definition  "Fulltime Student" ;
        skos:prefLabel   "Student (Fulltime)" .

claim-exception:disabled
        a                owl:Class ;
        rdfs:label       "Disabled" ;
        skos:definition  "Disabled" ;
        skos:prefLabel   "Disabled" .
