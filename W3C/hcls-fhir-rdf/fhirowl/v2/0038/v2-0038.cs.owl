@prefix v2-0038: <http://hl7.org/fhir/v2/0038/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v2-0038:RP  a           owl:Class ;
        rdfs:label      "Order has been replaced" ;
        skos:prefLabel  "Order has been replaced" .

v2-0038:A  a            owl:Class ;
        rdfs:label      "Some, but not all, results available" ;
        skos:prefLabel  "Some, but not all, results available" .

v2-0038:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 0038 ( Order status)" ;
        rdfs:label       "v2 Order status" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/0038/2.8.2" ;
        owl:versionInfo  "v2 Order status(2.8.2)" .

v2-0038:CM  a           owl:Class ;
        rdfs:label      "Order is completed" ;
        skos:prefLabel  "Order is completed" .

v2-0038:SC  a           owl:Class ;
        rdfs:label      "In process, scheduled" ;
        skos:prefLabel  "In process, scheduled" .

v2-0038:CA  a           owl:Class ;
        rdfs:label      "Order was canceled" ;
        skos:prefLabel  "Order was canceled" .

v2-0038:ER  a           owl:Class ;
        rdfs:label      "Error, order not found" ;
        skos:prefLabel  "Error, order not found" .

v2-0038:HD  a           owl:Class ;
        rdfs:label      "Order is on hold" ;
        skos:prefLabel  "Order is on hold" .

v2-0038:DC  a           owl:Class ;
        rdfs:label      "Order was discontinued" ;
        skos:prefLabel  "Order was discontinued" .

v2-0038:IP  a           owl:Class ;
        rdfs:label      "In process, unspecified" ;
        skos:prefLabel  "In process, unspecified" .
