        a               owl:Class ;
        rdfs:label      "Drug and alcohol rehabilitation" ;
        skos:prefLabel  "Drug and alcohol rehabilitation" .

episodeofcare-type:hacc
        a               owl:Class ;
        rdfs:label      "Home and Community Care" ;
        skos:prefLabel  "Home and Community Care" .

episodeofcare-type:cacp
        a               owl:Class ;
        rdfs:label      "Community-based aged care" ;
        skos:prefLabel  "Community-based aged care" .

episodeofcare-type:  a   owl:Ontology ;
        rdfs:comment     "This example value set defines a set of codes that can be used to express the usage type of an EpisodeOfCare record." ;
        rdfs:label       "EpisodeOfCareType" ;
