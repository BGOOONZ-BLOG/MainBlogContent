@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v2-0085: <http://hl7.org/fhir/v2/0085/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v2-0085:B  a            owl:Class ;
        rdfs:label      "Appended Report - Final results reviewed and further information provided for clarity without change to the original result values." ;
        skos:prefLabel  "Appended Report - Final results reviewed and further information provided for clarity without change to the original result values." .

v2-0085:V  a            owl:Class ;
        rdfs:label      "Verified - Final results reviewed and confirmed to be correct, no change to result value, normal range or abnormal flag" ;
        skos:prefLabel  "Verified - Final results reviewed and confirmed to be correct, no change to result value, normal range or abnormal flag" .

v2-0085:O  a            owl:Class ;
        rdfs:label      "Order detail description only (no result)" ;
        skos:prefLabel  "Order detail description only (no result)" .

v2-0085:C  a            owl:Class ;
        rdfs:label      "Record coming over is a correction and thus replaces a final result" ;
        skos:prefLabel  "Record coming over is a correction and thus replaces a final result" .

v2-0085:W  a            owl:Class ;
        rdfs:label      "Post original as wrong, e.g. transmitted for wrong patient" ;
        skos:prefLabel  "Post original as wrong, e.g. transmitted for wrong patient" .

v2-0085:R  a            owl:Class ;
        rdfs:label      "Results entered -- not verified" ;
        skos:prefLabel  "Results entered -- not verified" .

v2-0085:  a              owl:Ontology ;
        rdfs:comment     "FHIR Value set/code system definition for HL7 v2 table 0085 ( Observation Result Status Codes Interpretation)" ;
        rdfs:label       "v2 Observation Result Status Codes Interpretation" ;
        owl:versionIRI   "http://hl7.org/fhir/v2/0085/2.8.2" ;
        owl:versionInfo  "v2 Observation Result Status Codes Interpretation(2.8.2)" .

v2-0085:A  a            owl:Class ;
        rdfs:label      "Amended based on adjustments provided by the Placer (Physician) regarding patient demographics (such as age and/or gender or other patient specific information" ;
        skos:prefLabel  "Amended based on adjustments provided by the Placer (Physician) regarding patient demographics (such as age and/or gender or other patient specific information" .

v2-0085:F  a            owl:Class ;
        rdfs:label      "Final results" ;
        skos:prefLabel  "Final results" .

v2-0085:P  a            owl:Class ;
        rdfs:label      "Preliminary results" ;
        skos:prefLabel  "Preliminary results" .

v2-0085:U  a            owl:Class ;
        rdfs:label      "Results status change to final without retransmitting results already sent as 'preliminary.'  E.g. radiology changes status from preliminary to final" ;
        skos:prefLabel  "Results status change to final without retransmitting results already sent as 'preliminary.'  E.g. radiology changes status from preliminary to final" .

v2-0085:D  a            owl:Class ;
        rdfs:label      "Deletes the OBX record" ;
        skos:prefLabel  "Deletes the OBX record" .

v2-0085:I  a            owl:Class ;
        rdfs:label      "Specimen in lab; results pending" ;
        skos:prefLabel  "Specimen in lab; results pending" .

v2-0085:N  a            owl:Class ;
        rdfs:label      "Not asked; used to affirmatively document that the observation identified in the OBX was not sought when the universal service ID in OBR-4 implies that it would be sought." ;
        skos:prefLabel  "Not asked; used to affirmatively document that the observation identified in the OBX was not sought when the universal service ID in OBR-4 implies that it would be sought." .

v2-0085:S  a            owl:Class ;
        rdfs:label      "Partial results.   Deprecated. Retained only for backward compatibility as of V2.6." ;
        skos:prefLabel  "Partial results.   Deprecated. Retained only for backward compatibility as of V2.6." .

v2-0085:X  a            owl:Class ;
        rdfs:label      "Results cannot be obtained for this observation" ;
        skos:prefLabel  "Results cannot be obtained for this observation" .
