      rdf:type owl:Class ;
      owl:unionOf (
          time:DurationDescription
          time:Interval
        ) ;
    ] ;
  rdfs:label "hasXsdDurationDescription (DataTypeProperty)"@en ;
  rdfs:range xsd:duration ;
