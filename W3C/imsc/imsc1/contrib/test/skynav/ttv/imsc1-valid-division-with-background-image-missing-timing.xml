<?xml version="1.0" encoding="utf-8"?>
<!-- valid - div with background image not timed on self or ancestor -->
<tt xml:lang="" xmlns="http://www.w3.org/ns/ttml" xmlns:tts="http://www.w3.org/ns/ttml#styling"
    xmlns:ttp="http://www.w3.org/ns/ttml#parameter" ttp:profile="http://www.w3.org/ns/ttml/profile/imsc1/image"
    xmlns:smpte="http://www.smpte-ra.org/schemas/2052-1/2010/smpte-tt"
    xmlns:ttva="http://skynav.com/ns/ttv/annotations" ttva:warnOn="all" ttva:expectedErrors="0" ttva:expectedWarnings="3" ttva:model="imsc1">
  <head/>
  <body>
    <div smpte:backgroundImage="foo.png"/>
    <div smpte:backgroundImage="foo.png" begin="0s"/>
    <div smpte:backgroundImage="foo.png" end="1s"/>
  </body>
</tt>
