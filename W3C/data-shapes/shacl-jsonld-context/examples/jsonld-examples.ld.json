    },
    "lessThan" : {
      "@type" : "@id"
    },
    "lessThanOrEquals" : {
      "@type" : "@id"
    },
    "node" : {
      "@type" : "@id"
    },
    "nodeKind" : {
      "@type" : "@id"
    },
    "not" : {
      "@type" : "@id"
    },
    "or" : {
      "@type" : "@id",
      "@container" : "@list"
