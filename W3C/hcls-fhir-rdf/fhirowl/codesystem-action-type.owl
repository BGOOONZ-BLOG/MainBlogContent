@prefix action-type: <http://hl7.org/fhir/action-type/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

action-type:update  a    owl:Class ;
        rdfs:label       "Update" ;
        skos:definition  "The action is to update an existing resource" ;
        skos:prefLabel   "Update" .

action-type:fire-event
        a                owl:Class ;
        rdfs:label       "Fire Event" ;
        skos:definition  "The action is to fire a specific event" ;
        skos:prefLabel   "Fire Event" .

action-type:  a          owl:Ontology ;
        rdfs:comment     "The type of action to be performed" ;
        rdfs:label       "ActionType" ;
        owl:versionIRI   "http://hl7.org/fhir/action-type/3.1.0" ;
        owl:versionInfo  "ActionType(3.1.0)" .

action-type:create  a    owl:Class ;
        rdfs:label       "Create" ;
        skos:definition  "The action is to create a new resource" ;
        skos:prefLabel   "Create" .

action-type:remove  a    owl:Class ;
        rdfs:label       "Remove" ;
        skos:definition  "The action is to remove an existing resource" ;
        skos:prefLabel   "Remove" .
