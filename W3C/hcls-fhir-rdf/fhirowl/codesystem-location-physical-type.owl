@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix location-physical-type: <http://hl7.org/fhir/location-physical-type/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

location-physical-type:ro
        a                owl:Class ;
        rdfs:label       "Room" ;
        skos:definition  "A space that is allocated as a room, it may have walls/roof etc., but does not require these." ;
        skos:prefLabel   "Room" .

location-physical-type:si
        a                owl:Class ;
        rdfs:label       "Site" ;
        skos:definition  "A collection of buildings or other locations such as a site or a campus." ;
        skos:prefLabel   "Site" .

location-physical-type:wa
        a                owl:Class ;
        rdfs:label       "Ward" ;
        skos:definition  "A Ward is a section of a medical facility that may contain rooms and other types of location." ;
        skos:prefLabel   "Ward" .

location-physical-type:ve
        a                owl:Class ;
        rdfs:label       "Vehicle" ;
        skos:definition  "A means of transportation." ;
        skos:prefLabel   "Vehicle" .

location-physical-type:ho
        a                owl:Class ;
        rdfs:label       "House" ;
        skos:definition  "A residential dwelling. Usually used to reference a location that a person/patient may reside." ;
        skos:prefLabel   "House" .

location-physical-type:lvl
        a                owl:Class ;
        rdfs:label       "Level" ;
        skos:definition  "A Level in a multi-level Building/Structure." ;
        skos:prefLabel   "Level" .

location-physical-type:bu
        a                owl:Class ;
        rdfs:label       "Building" ;
        skos:definition  "Any Building or structure. This may contain rooms, corridors, wings, etc. It may not have walls, or a roof, but is considered a defined/allocated space." ;
        skos:prefLabel   "Building" .

location-physical-type:
        a                owl:Ontology ;
        rdfs:comment     "This example value set defines a set of codes that can be used to indicate the physical form of the Location." ;
        rdfs:label       "LocationType" ;
        owl:versionIRI   "http://hl7.org/fhir/location-physical-type/3.1.0" ;
        owl:versionInfo  "LocationType(3.1.0)" .

location-physical-type:wi
        a                owl:Class ;
        rdfs:label       "Wing" ;
        skos:definition  "A Wing within a Building, this often contains levels, rooms and corridors." ;
        skos:prefLabel   "Wing" .

location-physical-type:bd
        a                owl:Class ;
        rdfs:label       "Bed" ;
        skos:definition  "A space that is allocated for sleeping/laying on. This is not the physical bed/trolley that may be moved about, but the space it may occupy." ;
        skos:prefLabel   "Bed" .

location-physical-type:co
        a                owl:Class ;
        rdfs:label       "Corridor" ;
        skos:definition  "Any corridor within a Building, that may connect rooms." ;
        skos:prefLabel   "Corridor" .

location-physical-type:rd
        a                owl:Class ;
        rdfs:label       "Road" ;
        skos:definition  "A defined path to travel between 2 points that has a known name." ;
        skos:prefLabel   "Road" .

location-physical-type:jdn
        a                owl:Class ;
        rdfs:label       "Jurisdiction" ;
        skos:definition  "A wide scope that covers a conceptual domain, such as a Nation (Country wide community or Federal Government - e.g. Ministry of Health),  Province or State (community or Government), Business (throughout the enterprise), Nation with a business scope of an agency (e.g. CDC, FDA etc.) or a Business segment (UK Pharmacy), not just an physical boundry" ;
        skos:prefLabel   "Jurisdiction" .

location-physical-type:area
        a                owl:Class ;
        rdfs:label       "Area" ;
        skos:definition  "A defined physical boundary of something, such as a flood risk zone, region, postcode" ;
        skos:prefLabel   "Area" .

location-physical-type:ca
        a                owl:Class ;
        rdfs:label       "Cabinet" ;
        skos:definition  "A container that can store goods, equipment, medications or other items." ;
        skos:prefLabel   "Cabinet" .
