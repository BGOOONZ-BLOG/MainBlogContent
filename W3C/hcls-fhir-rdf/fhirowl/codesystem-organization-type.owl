@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix organization-type: <http://hl7.org/fhir/organization-type/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

organization-type:team
        a                owl:Class ;
        rdfs:label       "Organizational team" ;
        skos:definition  "An organizational team is usually a grouping of practitioners that perform a specific function within an organization (which could be a top level organization, or a department)." ;
        skos:prefLabel   "Organizational team" .

organization-type:dept
        a                owl:Class ;
        rdfs:label       "Hospital Department" ;
        skos:definition  "A department or ward within a hospital (Generally is not applicable to top level organizations)" ;
        skos:prefLabel   "Hospital Department" .

organization-type:  a    owl:Ontology ;
        rdfs:comment     "This example value set defines a set of codes that can be used to indicate a type of organization." ;
        rdfs:label       "OrganizationType" ;
        owl:versionIRI   "http://hl7.org/fhir/organization-type/3.1.0" ;
        owl:versionInfo  "OrganizationType(3.1.0)" .

organization-type:other
        a                owl:Class ;
        rdfs:label       "Other" ;
        skos:definition  "Other type of organization not already specified." ;
        skos:prefLabel   "Other" .

organization-type:crs
        a                owl:Class ;
        rdfs:label       "Clinical Research Sponsor" ;
        skos:definition  "An organization that is identified as a Pharmaceutical/Clinical Research Sponsor." ;
        skos:prefLabel   "Clinical Research Sponsor" .

organization-type:prov
        a                owl:Class ;
        rdfs:label       "Healthcare Provider" ;
        skos:definition  "An organization that provides healthcare services." ;
        skos:prefLabel   "Healthcare Provider" .

organization-type:bus
        a                owl:Class ;
        rdfs:label       "Non-Healthcare Business or Corporation" ;
        skos:definition  "An organization that is a registered business or corporation but not identified by other types." ;
        skos:prefLabel   "Non-Healthcare Business or Corporation" .

organization-type:edu
        a                owl:Class ;
        rdfs:label       "Educational Institute" ;
        skos:definition  "An educational institution that provides education or research facilities." ;
        skos:prefLabel   "Educational Institute" .

organization-type:govt
        a                owl:Class ;
        rdfs:label       "Government" ;
        skos:definition  "A political body, often used when including organization records for government bodies such as a Federal Government, State or Local Government." ;
        skos:prefLabel   "Government" .

organization-type:cg  a  owl:Class ;
        rdfs:label       "Community Group" ;
        skos:definition  "An un-incorporated community group." ;
        skos:prefLabel   "Community Group" .

organization-type:reli
        a                owl:Class ;
        rdfs:label       "Religious Institution" ;
        skos:definition  "An organization that is identified as a part of a religious institution." ;
        skos:prefLabel   "Religious Institution" .

organization-type:ins
        a                owl:Class ;
        rdfs:label       "Insurance Company" ;
        skos:definition  "A company that provides insurance to its subscribers that may include healthcare related policies." ;
        skos:prefLabel   "Insurance Company" .
