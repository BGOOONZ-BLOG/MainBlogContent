@prefix contract-term-type: <http://hl7.org/fhir/contracttermtypecodes/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

contract-term-type:statutory
        a                owl:Class ;
        rdfs:label       "Statutory" ;
        skos:definition  "Based on specialized statutes that deal with particular subjects." ;
        skos:prefLabel   "Statutory" .

contract-term-type:subject-to
        a                owl:Class ;
        rdfs:label       "Subject To" ;
        skos:definition  "Execution of the term in the contract is conditional on the execution of other actions." ;
        skos:prefLabel   "Subject To" .

contract-term-type:  a   owl:Ontology ;
        rdfs:comment     "This value set includes sample Contract Term Type codes." ;
        rdfs:label       "Contract Term Type Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/contracttermtypecodes/3.1.0" ;
        owl:versionInfo  "Contract Term Type Codes(3.1.0)" .
