@prefix v3-DeviceAlertLevel: <http://hl7.org/fhir/v3/DeviceAlertLevel/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-DeviceAlertLevel:C
        a                owl:Class ;
        rdfs:label       "Critical" ;
        skos:definition  "Shut Down, Fix Problem and Re-initialize" ;
        skos:prefLabel   "Critical" .

v3-DeviceAlertLevel:W
        a                owl:Class ;
        rdfs:label       "Warning" ;
        skos:definition  "Corrective Action Anticipated" ;
        skos:prefLabel   "Warning" .

v3-DeviceAlertLevel:  a  owl:Ontology ;
        rdfs:comment     " Domain values for the Device.Alert_levelCode" ;
        rdfs:label       "v3 Code System DeviceAlertLevel" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/DeviceAlertLevel/2016-11-11" ;
        owl:versionInfo  "v3 Code System DeviceAlertLevel(2016-11-11)" .

v3-DeviceAlertLevel:N
        a                owl:Class ;
        rdfs:label       "Normal" ;
        skos:definition  "No Corrective Action Needed" ;
        skos:prefLabel   "Normal" .

v3-DeviceAlertLevel:S
        a                owl:Class ;
        rdfs:label       "Serious" ;
        skos:definition  "Corrective Action Required" ;
        skos:prefLabel   "Serious" .
