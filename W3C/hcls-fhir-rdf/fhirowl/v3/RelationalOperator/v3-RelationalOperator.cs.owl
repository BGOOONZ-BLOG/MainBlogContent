@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix v3-RelationalOperator: <http://hl7.org/fhir/v3/RelationalOperator/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-RelationalOperator:GN
        a                owl:Class ;
        rdfs:label       "Generic" ;
        skos:definition  "A generic comparison selects a record for inclusion in the response if the beginning of the designated element value matches the select string." ;
        skos:prefLabel   "Generic" .

v3-RelationalOperator:EQ
        a                owl:Class ;
        rdfs:label       "Equal" ;
        skos:definition  "Equal condition applied to comparisons." ;
        skos:prefLabel   "Equal" .

v3-RelationalOperator:CT
        a                owl:Class ;
        rdfs:label       "Contains" ;
        skos:definition  "Specified set of things includes value being evaluated." ;
        skos:prefLabel   "Contains" .

v3-RelationalOperator:LE
        a                owl:Class ;
        rdfs:label       "Less than or equal" ;
        skos:definition  "Less than or equal condition applied to comparisons." ;
        skos:prefLabel   "Less than or equal" .

v3-RelationalOperator:LT
        a                owl:Class ;
        rdfs:label       "Less than" ;
        skos:definition  "Less than condition applied to comparisons." ;
        skos:prefLabel   "Less than" .

v3-RelationalOperator:NE
        a                owl:Class ;
        rdfs:label       "Not Equal" ;
        skos:definition  "Not equal condition applied to comparisons." ;
        skos:prefLabel   "Not Equal" .

v3-RelationalOperator:GE
        a                owl:Class ;
        rdfs:label       "Greater than or equal" ;
        skos:definition  "Greater than or equal condition applied to comparisons." ;
        skos:prefLabel   "Greater than or equal" .

v3-RelationalOperator:
        a                owl:Ontology ;
        rdfs:comment     "**** MISSING DEFINITIONS ****" ;
        rdfs:label       "v3 Code System RelationalOperator" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/RelationalOperator/2016-11-11" ;
        owl:versionInfo  "v3 Code System RelationalOperator(2016-11-11)" .

v3-RelationalOperator:GT
        a                owl:Class ;
        rdfs:label       "Greater than" ;
        skos:definition  "Greater than condition applied to comparisons." ;
        skos:prefLabel   "Greater than" .
