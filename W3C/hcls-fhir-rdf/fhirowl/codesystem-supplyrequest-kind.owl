@prefix supplyrequest-kind: <http://hl7.org/fhir/supply-kind/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

supplyrequest-kind:nonstock
        a                owl:Class ;
        rdfs:label       "Non-Stock" ;
        skos:definition  "Supply is not onsite and must be requested from an outside vendor using a non-stock requisition." ;
        skos:prefLabel   "Non-Stock" .

supplyrequest-kind:central
        a                owl:Class ;
        rdfs:label       "Central Supply" ;
        skos:definition  "Supply is stored and requested from central supply." ;
        skos:prefLabel   "Central Supply" .

supplyrequest-kind:  a   owl:Ontology ;
        rdfs:comment     "This value sets refers to a Category of supply." ;
        rdfs:label       "Supply Type" ;
        owl:versionIRI   "http://hl7.org/fhir/supply-kind/3.1.0" ;
        owl:versionInfo  "Supply Type(3.1.0)" .
