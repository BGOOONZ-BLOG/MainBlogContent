@prefix benefit-term: <http://hl7.org/fhir/benefit-term/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

benefit-term:  a         owl:Ontology ;
        rdfs:comment     "This value set includes a smattering of Benefit Term codes." ;
        rdfs:label       "Benefit Term Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/benefit-term/3.1.0" ;
        owl:versionInfo  "Benefit Term Codes(3.1.0)" .

benefit-term:lifetime
        a                owl:Class ;
        rdfs:label       "Lifetime" ;
        skos:definition  "For the total term, lifetime, of the policy or coverage" ;
        skos:prefLabel   "Lifetime" .

benefit-term:day  a      owl:Class ;
        rdfs:label       "Day" ;
        skos:definition  "Per day" ;
        skos:prefLabel   "Day" .

benefit-term:annual  a   owl:Class ;
        rdfs:label       "Annual" ;
        skos:definition  "Annual, renewing on the anniversary" ;
        skos:prefLabel   "Annual" .
