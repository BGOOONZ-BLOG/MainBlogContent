@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix question-max-occurs: <http://hl7.org/fhir/question-max-occurs/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

question-max-occurs:  a  owl:Ontology ;
        rdfs:comment     "Flags an element as having unlimited repetitions" ;
        rdfs:label       "MaxOccurs" ;
        owl:versionIRI   "http://hl7.org/fhir/question-max-occurs/3.1.0" ;
        owl:versionInfo  "MaxOccurs(3.1.0)" .

<http://hl7.org/fhir/question-max-occurs/*>
        a                owl:Class ;
        rdfs:label       "Repeating" ;
        skos:definition  "Element can repeat an unlimited number of times" ;
        skos:prefLabel   "Repeating" .
