@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix v3-ActUncertainty: <http://hl7.org/fhir/v3/ActUncertainty/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-ActUncertainty:U  a   owl:Class ;
        rdfs:label       "stated with uncertainty" ;
        skos:definition  "Specifies that the originator of the Act statement does not have full confidence in the applicability (i.e., in event mood: factual truth) of the statement." ;
        skos:prefLabel   "stated with uncertainty" .

v3-ActUncertainty:N  a   owl:Class ;
        rdfs:label       "stated with no assertion of uncertainty" ;
        skos:definition  "Specifies that the act statement is made without explicit tagging of uncertainty. This is the normal statement, meaning that it is not free of errors and uncertainty may still exist." ;
        skos:prefLabel   "stated with no assertion of uncertainty" .

v3-ActUncertainty:  a    owl:Ontology ;
        rdfs:comment     "  OpenIssue:\r\nMissing Description" ;
        rdfs:label       "v3 Code System ActUncertainty" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/ActUncertainty/2016-11-11" ;
        owl:versionInfo  "v3 Code System ActUncertainty(2016-11-11)" .
