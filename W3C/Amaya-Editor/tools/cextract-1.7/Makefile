DEPS =

SRCS = 	parse.c 	main.c 	io.c

OBJS =       	parse.o 	main.o 	io.o

all:: cextract

cextract: $(OBJS) $(DEPS)
	$(RM) $@
	$(CCLINK) -o $@ $(LDOPTIONS) $(OBJS) $(LOCAL_LIBS1) $(LDLIBS) $(SYS_LIBS1) $(EXTRA_LOAD_FLAGS)

clean::
	$(RM) cextract

install:: cextract
	@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS)  cextract $(DESTDIR)$(BINDIR)/cextract

install.man:: cextract.man
	@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) cextract.man $(DESTDIR)$(MANDIR)/cextract.$(MANSUFFIX)
#

install.man:: cextrc.man
	@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) cextrc.man $(DESTDIR)$(MANDIR)/cextrc.$(MANSUFFIX)

