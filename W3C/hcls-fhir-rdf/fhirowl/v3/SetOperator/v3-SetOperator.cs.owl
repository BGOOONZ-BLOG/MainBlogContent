@prefix v3-SetOperator: <http://hl7.org/fhir/v3/SetOperator/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-SetOperator:_ValueSetOperator
        a                owl:Class ;
        rdfs:label       "ValueSetOperator" ;
        skos:definition  "Operations that can be used to associate concepts in a terminology." ;
        skos:prefLabel   "ValueSetOperator" .

v3-SetOperator:E  a      owl:Class ;
        rdfs:label       "exclude" ;
        rdfs:subClassOf  v3-SetOperator:_ValueSetOperator ;
        skos:definition  "Form the set-difference with this value, i.e., exclude this element or set from the resulting set." ;
        skos:prefLabel   "exclude" .

v3-SetOperator:H  a      owl:Class ;
        rdfs:label       "convex hull" ;
        skos:definition  "Form the convex hull with the value. The convex hull is defined over ordered domains and is the smallest contiguous superset (interval) that of all the operand sets." ;
        skos:prefLabel   "convex hull" .

v3-SetOperator:  a       owl:Ontology ;
        rdfs:comment     "**** MISSING DEFINITIONS ****" ;
        rdfs:label       "v3 Code System SetOperator" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/SetOperator/2016-11-11" ;
        owl:versionInfo  "v3 Code System SetOperator(2016-11-11)" .

v3-SetOperator:A  a      owl:Class ;
        rdfs:label       "intersect" ;
        skos:definition  "Form the intersection with the value." ;
        skos:prefLabel   "intersect" .

v3-SetOperator:P  a      owl:Class ;
        rdfs:label       "periodic hull" ;
        skos:definition  "Form the periodic hull with the value. The periodic hull is defined over ordered domains and is the periodic set that contains all contiguous supersets of pairs of intervals generated by the operand periodic intervals." ;
        skos:prefLabel   "periodic hull" .

v3-SetOperator:I  a      owl:Class ;
        rdfs:label       "include" ;
        rdfs:subClassOf  v3-SetOperator:_ValueSetOperator ;
        skos:definition  "Form the union with this value, i.e., include this element or set in the resulting set." ;
        skos:prefLabel   "include" .
